  <bibliography>
    <layout>
      <choose>
        <if type="book">
          <names variable="author" suffix=". ">
            <name et-al-min="7" et-al-use-first="1" delimiter=", " name-as-sort-order="all" sort-separator=", " form="long" initialize-with=""/>
          </names>
          <text variable="title" suffix=". "/>
