<?xml version="1.0" encoding="utf-8"?>
<style class="note" version="1.0" and="text" demote-non-dropping-particle="never" default-locale="nl-NL" xmlns="http://purl.org/net/xbiblio/csl">
  <info>
    <title>Uantwerpen - historische referentiestijl</title>
    <title-short>UAntwerpen</title-short>
    <id>http://www.zotero.org/styles/csledit/UA</id>
    <author>
      <name>R. Bal</name>
    </author>
    <category citation-format="note"/>
    <category field="history"/>
    <summary>Annotatie en bibliografie volgens de richtlijnen van de Universiteit Antwerpen, Departement Geschiedenis, zoals beschreven in Richtlijnen en suggesties bij het schrijven van een historisch werkstuk</summary>
    <updated>2013-11-04T22:05:02+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-NonCommercial-ShareAlike 3.0 Unported</rights>
  </info>
  <locale xml:lang="nl">
    <terms>
      <term name="et-al">e.a.</term>
      <term name="editor" form="short">
        <single> (red.)</single>
        <multiple> (red.)</multiple>
      </term>
      <term name="edition" form="short">druk</term>
      <term name="volume" form="short">
        <single>dl.</single>
        <multiple>dln.</multiple>
      </term>
      <term name="at">aldaar</term>
      <term name="note" form="short">noot</term>
      <term name="ibid">ibidem</term>
    </terms>
  </locale>
  <macro name="Naam biblio">
    <choose>
      <if match="any" variable="author editor translator">
        <names variable="author">
          <name and="text" delimiter-precedes-et-al="never" delimiter-precedes-last="never" et-al-min="4" et-al-use-first="3" et-al-subsequent-min="4" et-al-subsequent-use-first="1" name-as-sort-order="first">
            <name-part name="family" font-variant="small-caps"/>
          </name>
          <substitute>
            <names variable="editor"/>
            <names variable="translator"/>
          </substitute>
        </names>
      </if>
      <else>
        <text value="N.N."/>
      </else>
    </choose>
  </macro>
  <macro name="contributors-note">
    <names variable="author">
      <name and="text" delimiter-precedes-last="never"/>
      <label form="short"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
      </substitute>
    </names>
    <text macro="recipient-note"/>
  </macro>
  <macro name="title-note">
    <choose>
      <if type="bill book graphic legal_case motion_picture report song" match="any">
        <text variable="title" font-style="italic"/>
      </if>
      <else-if type="manuscript"/>
      <else>
        <text variable="title" prefix="‘" suffix="’"/>
      </else>
    </choose>
  </macro>
  <macro name="editor-translator">
    <group delimiter=", ">
      <names variable="editor" delimiter=", ">
        <name and="text" delimiter=", " delimiter-precedes-last="never"/>
        <label form="short" prefix=" "/>
      </names>
      <choose>
        <if variable="author editor" match="any">
          <names variable="translator" delimiter=", ">
            <label form="short"/>
            <name and="text" delimiter=", " delimiter-precedes-last="never"/>
          </names>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="collection-title">
    <text variable="collection-title"/>
    <text variable="collection-number" prefix=" "/>
  </macro>
  <macro name="locators-note">
    <choose>
      <if type="article-journal">
        <text variable="volume"/>
      </if>
      <else-if type="bill book graphic legal_case motion_picture report song chapter paper-conference" match="any">
        <text variable="volume" form="short"/>
      </else-if>
    </choose>
  </macro>
  <macro name="volume">
    <choose>
      <if type="article-journal">
        <text variable="volume"/>
      </if>
      <else-if type="bill book graphic legal_case motion_picture report song chapter paper-conference" match="any">
        <text variable="volume" form="short"/>
      </else-if>
    </choose>
  </macro>
  <macro name="issue-note">
    <choose>
      <if type="article-journal">
        <text macro="issued" prefix=" (" suffix=")"/>
      </if>
      <else-if variable="publisher-place" match="any">
        <group prefix=" (" suffix=")" delimiter=" ">
          <text macro="edition-note" suffix=";"/>
          <group delimiter=" ">
            <choose>
              <if variable="title" match="none"/>
              <else-if type="thesis speech" match="any">
                <text variable="genre" prefix=" "/>
              </else-if>
            </choose>
            <text macro="event"/>
          </group>
          <text macro="publisher"/>
          <text macro="issued"/>
        </group>
      </else-if>
      <else>
        <group prefix=" (" suffix=")" delimiter=" ">
          <text value="zp"/>
          <text macro="issued"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="container-title-note">
    <choose>
      <if type="chapter paper-conference" match="any">
        <text term="in" text-case="lowercase" suffix=": "/>
        <text macro="editor-translator" suffix=", "/>
      </if>
    </choose>
    <text variable="container-title" font-style="italic"/>
  </macro>
  <macro name="edition-note">
    <choose>
      <if type="bill book graphic legal_case motion_picture report song chapter paper-conference" match="any">
        <choose>
          <if is-numeric="edition">
            <group delimiter=" ">
              <number variable="edition" form="ordinal"/>
              <text term="edition" form="short" strip-periods="true"/>
            </group>
          </if>
          <else>
            <text variable="edition"/>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="editor-note">
    <names variable="editor">
      <name and="text" sort-separator=", " delimiter=", "/>
      <label form="short" prefix=", "/>
    </names>
  </macro>
  <macro name="translator-note">
    <names variable="translator">
      <name and="text" sort-separator=", " delimiter=", "/>
      <label form="short" prefix=", "/>
    </names>
  </macro>
  <macro name="recipient-note">
    <names variable="recipient" delimiter=", ">
      <label form="verb" prefix=" " text-case="lowercase" suffix=" "/>
      <name and="text" delimiter=", "/>
    </names>
  </macro>
  <macro name="recipient-short">
    <names variable="recipient">
      <label form="verb" prefix=" " text-case="lowercase" suffix=" "/>
      <name form="short" and="text" delimiter=", "/>
    </names>
  </macro>
  <macro name="contributors-short">
    <names variable="author">
      <name form="short" and="text" sort-separator=", " delimiter=", " delimiter-precedes-last="never"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
      </substitute>
    </names>
    <text macro="recipient-short"/>
  </macro>
  <macro name="interviewer-note">
    <names variable="interviewer" delimiter=", ">
      <label form="verb" prefix=" " text-case="lowercase" suffix=" "/>
      <name and="text" delimiter=", "/>
    </names>
  </macro>
  <macro name="locators-newspaper">
    <choose>
      <if type="article-newspaper">
        <group delimiter=", ">
          <group>
            <text variable="edition" suffix=" "/>
            <text term="edition" prefix=" "/>
          </group>
          <group>
            <text term="section" suffix=" "/>
            <text variable="section"/>
          </group>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="event">
    <group>
      <text term="presented at" suffix=" "/>
      <text variable="event"/>
    </group>
  </macro>
  <macro name="publisher">
    <group delimiter=" ">
      <choose>
        <if variable="publisher-place">
          <text variable="publisher-place" strip-periods="true"/>
        </if>
        <else>
          <text value="zp"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="issued">
    <choose>
      <if type="graphic report article-newspaper" match="any">
        <date variable="issued">
          <date-part name="day" suffix=" "/>
          <date-part name="month" suffix=" "/>
          <date-part name="year"/>
        </date>
      </if>
      <else-if type="bill book graphic legal_case motion_picture report song thesis chapter paper-conference" match="any">
        <choose>
          <if variable="issued">
            <date variable="issued">
              <date-part name="year"/>
            </date>
          </if>
          <else>
            <text value="zj"/>
          </else>
        </choose>
      </else-if>
      <else>
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </else>
    </choose>
  </macro>
  <macro name="pages">
    <choose>
      <if type="article-journal">
        <text variable="page" prefix=" "/>
      </if>
      <else>
        <choose>
          <if variable="volume">
            <text variable="page" prefix=" "/>
          </if>
          <else>
            <text variable="page" prefix=" "/>
          </else>
        </choose>
      </else>
    </choose>
  </macro>
  <macro name="point-locators">
    <text macro="pages"/>
    <choose>
      <if type="chapter paper-conference article-journal article-magazine article-newspaper" match="any">
        <choose>
          <if variable="page">
            <group prefix=", ">
              <text term="at" suffix=" "/>
              <text variable="locator"/>
            </group>
          </if>
          <else-if variable="note">
            <group prefix=", ">
              <text term="at" suffix=" "/>
              <text variable="locator"/>
            </group>
          </else-if>
          <else>
            <text term="at" suffix=" "/>
            <text variable="locator"/>
          </else>
        </choose>
      </if>
      <else-if type="bill book graphic legal_case motion_picture report song thesis webpage" match="any">
        <text variable="locator" prefix=" "/>
      </else-if>
      <else-if type=" manuscript" match="any">
        <label variable="locator" form="short" suffix=" "/>
        <text variable="locator"/>
      </else-if>
    </choose>
  </macro>
  <macro name="archive-note">
    <group delimiter=", ">
      <text variable="publisher-place"/>
      <text variable="archive"/>
      <text variable="archive_location"/>
    </group>
  </macro>
  <macro name="access-note">
    <group delimiter=", ">
      <choose>
        <if type="graphic report" match="any">
          <text macro="archive-note" prefix=", "/>
        </if>
        <else-if type="bill book graphic legal_case motion_picture report song article-journal article-magazine article-newspaper thesis chapter paper-conference entry-encyclopedia entry-dictionary" match="none">
          <text macro="archive-note" prefix=", "/>
        </else-if>
      </choose>
    </group>
    <choose>
      <if variable="DOI">
        <text variable="DOI" prefix=" &lt;doi:" suffix="&gt;"/>
      </if>
      <else-if type="webpage post-weblog" match="any">
        <group prefix=" [" suffix="]">
          <text variable="URL" prefix=", "/>
          <date form="text" variable="event-date" prefix=" (" suffix=")"/>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="short-title-note">
    <choose>
      <if type="bill book graphic legal_case motion_picture report song" match="any">
        <text variable="title" font-style="italic" form="short"/>
        <text macro="locators-note" prefix=" "/>
      </if>
      <else-if type="manuscript">
        <group delimiter=", ">
          <text variable="publisher-place"/>
          <text variable="title" form="short"/>
          <text variable="archive_location"/>
        </group>
      </else-if>
      <else>
        <text variable="title" prefix="‘" suffix="’" form="short"/>
      </else>
    </choose>
  </macro>
  <macro name="point-locators-subsequent">
    <choose>
      <if variable="page">
        <text variable="locator"/>
      </if>
      <else-if type=" manuscript" match="any">
        <label variable="locator" form="short" suffix=" "/>
        <text variable="locator"/>
      </else-if>
      <else>
        <text variable="locator"/>
      </else>
    </choose>
  </macro>
  <macro name="Naam citaat">
    <choose>
      <if match="any" variable="author editor translator">
        <names variable="author">
          <name and="text" delimiter-precedes-et-al="never" delimiter-precedes-last="never" et-al-min="4" et-al-use-first="3" et-al-subsequent-min="4" et-al-subsequent-use-first="1">
            <name-part name="family" font-variant="small-caps"/>
          </name>
          <substitute>
            <names variable="editor"/>
            <names variable="translator"/>
          </substitute>
        </names>
      </if>
      <else>
        <text value="N.N."/>
      </else>
    </choose>
  </macro>
  <macro name="Plaats uitgeverij">
    <choose>
      <if match="any" variable="publisher-place">
        <text variable="publisher-place"/>
      </if>
      <else>
        <text value="s.l."/>
      </else>
    </choose>
  </macro>
  <macro name="Datum publicatie">
    <choose>
      <if match="any" variable="issued">
        <date date-parts="year" form="numeric" variable="issued"/>
      </if>
      <else>
        <text value="s.a."/>
      </else>
    </choose>
  </macro>
  <macro name="Pagina citaat">
    <text variable="locator" prefix=", "/>
  </macro>
  <macro name="Titel citaat">
    <choose>
      <if match="any" variable="title-short">
        <text variable="title-short" font-style="italic"/>
      </if>
      <else>
        <text variable="title" font-style="italic"/>
      </else>
    </choose>
  </macro>
  <macro name="Titel citaat (tijdschrift)">
    <choose>
      <if match="any" variable="title-short">
        <text variable="title-short" quotes="true"/>
      </if>
      <else>
        <text variable="title" quotes="true"/>
      </else>
    </choose>
  </macro>
  <macro name="Ibid-with-locator">
    <group suffix=".">
      <text value="Ibid." text-case="capitalize-first"/>
      <text variable="locator" prefix=", "/>
    </group>
  </macro>
  <macro name="Jaartal website">
    <choose>
      <if match="any" variable="accessed">
        <date form="text" variable="accessed"/>
      </if>
      <else>
        <date form="text" variable="issued"/>
      </else>
    </choose>
  </macro>
  <macro name="Jaartal krant">
    <choose>
      <if match="any" variable="issued">
        <date form="text" date-parts="year-month-day" variable="issued"/>
      </if>
      <else>
        <text value="s.d."/>
      </else>
    </choose>
  </macro>
  <macro name="Jaartal tijdschrift">
    <choose>
      <if match="any" variable="issued">
        <date date-parts="year" form="text" variable="issued"/>
      </if>
      <else>
        <text value="s.a."/>
      </else>
    </choose>
  </macro>
  <macro name="Uitgeverij">
    <choose>
      <if match="any" variable="publisher">
        <text variable="publisher"/>
      </if>
      <else>
        <text value="s.e."/>
      </else>
    </choose>
  </macro>
  <macro name="Naam biblio (sectie)">
    <choose>
      <if match="any" variable="container-author">
        <names variable="container-author">
          <name and="text" delimiter-precedes-et-al="never" delimiter-precedes-last="never" et-al-min="4" et-al-use-first="3" et-al-subsequent-min="4" et-al-subsequent-use-first="1">
            <name-part name="family" font-variant="small-caps"/>
          </name>
          <substitute/>
        </names>
      </if>
      <else>
        <text value="N.N."/>
      </else>
    </choose>
  </macro>
  <citation et-al-min="4" et-al-use-first="1" disambiguate-add-names="true" disambiguate-add-givenname="true">
    <layout suffix="." delimiter="; ">
      <choose>
        <if position="ibid-with-locator">
          <text macro="Ibid-with-locator"/>
        </if>
        <else>
          <choose>
            <if match="any" position="ibid">
              <text value="Ibidem." text-case="capitalize-first"/>
            </if>
            <else>
              <choose>
                <if type="article-journal article-newspaper post-weblog chapter entry-encyclopedia" match="any">
                  <group suffix=".">
                    <text macro="Naam citaat" suffix=", "/>
                    <text macro="Titel citaat (tijdschrift)"/>
                    <text macro="Pagina citaat"/>
                  </group>
                </if>
                <if type="webpage" match="any">
                  <group suffix=".">
                    <choose>
                      <if match="any" variable="container-title">
                        <text variable="container-title" font-style="italic"/>
                      </if>
                      <else-if match="any" variable="title-short">
                        <text variable="title-short" font-style="italic"/>
                      </else-if>
                      <else>
                        <text variable="title" font-style="italic"/>
                      </else>
                    </choose>
                    <choose>
                      <if match="any" variable="accessed issued">
                        <text value="website geraadpleegd op" prefix=", " suffix=" "/>
                        <text macro="Jaartal website"/>
                      </if>
                    </choose>
                  </group>
                </if>
                <if type="interview" match="any">
                  <group suffix=".">
                    <names variable="author" prefix="Interview met "/>
                    <choose>
                      <if match="all" variable="abstract archive">
                        <names variable="interviewer" prefix=", afgenomen door "/>
                      </if>
                    </choose>
                    <text variable="note" prefix=" te "/>
                    <date form="text" variable="issued" prefix=" op "/>
                  </group>
                </if>
                <if type="manuscript paper-conference" match="any">
                  <choose>
                    <if match="all" variable="title author">
                      <text macro="Naam citaat" suffix=", "/>
                      <text macro="Titel citaat"/>
                      <text macro="Pagina citaat"/>
                    </if>
                    <else>
                      <text value="ARCHIEFSTUK  (manueel in te voeren)" font-variant="normal"/>
                    </else>
                  </choose>
                </if>
                <if type="report" match="any">
                  <choose>
                    <if match="any" variable="author collection-title">
                      <choose>
                        <if match="none" variable="collection-title">
                          <text macro="Naam citaat" suffix=", "/>
                          <text macro="Titel citaat"/>
                        </if>
                        <else>
                          <text macro="Titel citaat (tijdschrift)"/>
                        </else>
                      </choose>
                    </if>
                    <else>
                      <text macro="Titel citaat" font-style="italic"/>
                    </else>
                  </choose>
                  <text macro="Pagina citaat"/>
                </if>
                <else>
                  <group suffix=".">
                    <choose>
                      <if type="report" match="any"/>
                      <else>
                        <text macro="Naam citaat" suffix=", "/>
                      </else>
                    </choose>
                    <text macro="Titel citaat"/>
                    <text macro="Pagina citaat"/>
                  </group>
                </else>
              </choose>
            </else>
          </choose>
        </else>
      </choose>
    </layout>
  </citation>
  <bibliography and="text" delimiter-precedes-et-al="never" delimiter-precedes-last="never" et-al-min="4" et-al-use-first="3" hanging-indent="true">
    <sort>
      <key macro="Naam biblio"/>
      <key variable="issued"/>
      <key variable="volume"/>
      <key variable="issue"/>
      <key variable="title"/>
    </sort>
    <layout suffix=".">
      <choose>
        <if type="book chapter entry-encyclopedia" match="any">
          <group suffix=".">
            <text macro="Naam biblio"/>
            <choose>
              <if type="book" match="any">
                <text variable="title" font-style="italic" prefix=", "/>
              </if>
              <else>
                <text variable="title" quotes="true" prefix=", "/>
              </else>
            </choose>
            <choose>
              <if type="chapter entry-encyclopedia" match="any">
                <group>
                  <text term="in" prefix=", " suffix=": "/>
                  <choose>
                    <if match="any" variable="container-author editor collection-editor">
                      <names variable="container-author">
                        <name and="text" delimiter-precedes-et-al="never" delimiter-precedes-last="never" et-al-min="4" et-al-use-first="3" et-al-subsequent-min="4" et-al-subsequent-use-first="1">
                          <name-part name="family" font-variant="small-caps"/>
                        </name>
                        <substitute>
                          <names variable="editor"/>
                          <names variable="collection-editor"/>
                        </substitute>
                      </names>
                      <text value="(red.)" prefix=" "/>
                    </if>
                    <else>
                      <text value="N.N."/>
                    </else>
                  </choose>
                  <text variable="container-title" font-style="italic" prefix=", "/>
                  <choose>
                    <if type="entry-encyclopedia" match="all" variable="number-of-volumes">
                      <text variable="number-of-volumes" prefix=", dln. "/>
                    </if>
                  </choose>
                  <choose>
                    <if match="any" variable="volume">
                      <text variable="volume" prefix=", dl. "/>
                    </if>
                  </choose>
                </group>
              </if>
            </choose>
            <text macro="Plaats uitgeverij" prefix=", "/>
            <text macro="Uitgeverij" prefix=": "/>
            <date date-parts="year" form="text" variable="issued" prefix=", "/>
            <choose>
              <if match="any" variable="original-date">
                <date date-parts="year" form="text" variable="original-date" prefix=" [" suffix="]"/>
              </if>
            </choose>
            <choose>
              <if type="chapter" match="any">
                <text variable="page" prefix=", "/>
              </if>
            </choose>
          </group>
        </if>
        <if type="thesis" match="any">
          <group suffix=".">
            <text macro="Naam biblio"/>
            <text variable="title" font-style="italic" prefix=", "/>
            <choose>
              <if match="any" variable="genre">
                <text value="onuitgegeven" prefix=", "/>
                <text variable="genre" prefix=" "/>
              </if>
            </choose>
            <text value="vakgebied" prefix=", "/>
            <text variable="note" prefix=" "/>
            <text variable="publisher" prefix=", "/>
            <text macro="Datum publicatie" prefix=", "/>
          </group>
        </if>
        <if type="article-journal article-newspaper article-magazine" match="any">
          <group suffix=".">
            <text macro="Naam biblio" suffix=", "/>
            <text variable="title" quotes="true"/>
            <text variable="container-title" font-style="italic" prefix=", "/>
            <choose>
              <if match="all" variable="volume issue">
                <text variable="volume" prefix=", "/>
                <text variable="issue" prefix="/"/>
              </if>
              <if match="any" variable="volume">
                <text variable="volume" prefix=", "/>
              </if>
              <if match="any" variable="issue">
                <text variable="issue" prefix=", "/>
              </if>
            </choose>
            <choose>
              <if type="article-newspaper" match="any">
                <text macro="Jaartal krant" prefix=", "/>
              </if>
              <else>
                <text macro="Jaartal tijdschrift" prefix=", "/>
              </else>
            </choose>
            <text variable="page" prefix=", "/>
          </group>
        </if>
        <if type="webpage" match="any">
          <group suffix=".">
            <text variable="title" font-style="italic"/>
            <choose>
              <if match="any" variable="container-title">
                <text variable="container-title" prefix=", "/>
              </if>
            </choose>
            <text variable="URL" prefix=", &lt;&#160;" suffix="&#160;&gt;"/>
            <choose>
              <if match="any" variable="accessed issued">
                <text value="geraadpleegd op" prefix=", " suffix=" "/>
                <text macro="Jaartal website"/>
              </if>
            </choose>
          </group>
        </if>
        <if type="interview" match="any">
          <group suffix=".">
            <choose>
              <if match="all" variable="interviewer archive">
                <text variable="archive" suffix=", "/>
                <text variable="archive_location" suffix=", "/>
              </if>
            </choose>
            <names variable="author" prefix="Interview met "/>
            <choose>
              <if match="all" variable="interviewer archive">
                <names variable="interviewer" prefix=", afgenomen door "/>
              </if>
            </choose>
            <text variable="note" prefix=" te "/>
            <date form="text" variable="issued" prefix=" op "/>
          </group>
        </if>
        <if type="manuscript paper-conference" match="any">
          <group suffix=".">
            <choose>
              <if match="all" variable="title author">
                <text macro="Naam biblio" suffix=", "/>
                <text variable="title" font-style="italic"/>
                <choose>
                  <if match="any" variable="genre">
                    <text variable="genre" prefix=", "/>
                  </if>
                  <else-if type="paper-conference" match="any">
                    <text value="onuitgegeven congrespaper" prefix=", "/>
                  </else-if>
                  <else>
                    <text value="onuitgegeven handschrift" prefix=", "/>
                  </else>
                </choose>
                <text variable="publisher-place" prefix=", "/>
                <text variable="archive" prefix=", "/>
                <text macro="Datum publicatie" prefix=", "/>
              </if>
              <else>
                <text variable="publisher-place" suffix=", "/>
                <text variable="archive"/>
                <text variable="archive_location" prefix=", "/>
                <text variable="call-number" prefix=", "/>
              </else>
            </choose>
          </group>
        </if>
        <if type="report" match="any">
          <group suffix=".">
            <choose>
              <if match="none" variable="collection-title">
                <names variable="author" suffix=", ">
                  <name and="text" et-al-min="4" et-al-use-first="3" et-al-subsequent-min="4" et-al-subsequent-use-first="1" name-as-sort-order="first">
                    <name-part name="family" font-variant="small-caps"/>
                  </name>
                </names>
                <text variable="title" font-style="italic"/>
                <names variable="collection-editor" prefix=", bewerkt door ">
                  <name and="text" et-al-min="4" et-al-use-first="3" et-al-subsequent-min="4" et-al-subsequent-use-first="1">
                    <name-part name="family" font-variant="small-caps"/>
                  </name>
                </names>
              </if>
              <else>
                <names variable="collection-editor" suffix=" (red.), ">
                  <name and="text" et-al-min="4" et-al-use-first="3" et-al-subsequent-min="4" et-al-subsequent-use-first="1" name-as-sort-order="first">
                    <name-part name="family" font-variant="small-caps"/>
                  </name>
                </names>
                <text variable="collection-title" font-style="italic"/>
              </else>
            </choose>
            <text macro="Plaats uitgeverij" prefix=", " suffix=": "/>
            <text macro="Uitgeverij"/>
            <text macro="Datum publicatie" prefix=", "/>
          </group>
        </if>
        <if type="post-weblog" match="any">
          <group suffix=".">
            <text macro="Naam biblio" suffix=", "/>
            <text variable="title" quotes="true"/>
            <text variable="container-title" font-style="italic" prefix=", "/>
            <text variable="note" prefix=", "/>
            <text macro="Datum publicatie" prefix=", "/>
            <text variable="URL" prefix=", &lt;&#160;" suffix="&#160;&gt;"/>
            <text macro="Jaartal website" prefix=", geraadpleegd op "/>
          </group>
        </if>
      </choose>
    </layout>
  </bibliography>
</style>
