          <names variable="collection-editor">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="collection-editor">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
          </names>
        </else>
      </choose>
      <choose>
        <if variable="editor collection-editor" match="any">
          <names variable="translator">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="translator">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
          </names>
        </else>
      </choose>
      <choose>
        <if variable="editor collection-editor translator" match="any">
          <names variable="reviewed-author">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="reviewed-author">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
          </names>
        </else>
      </choose>
      <choose>
        <if variable="editor collection-editor translator reviewed-author" match="any">
          <names variable="editorial-director">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="editorial-director">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
          </names>
        </else>
      </choose>
      <choose>
        <if variable="editor collection-editor translator reviewed-author editorial-director" match="any">
          <names variable="compiler">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="compiler">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
          </names>
        </else>
      </choose>
      <choose>
        <if variable="editor collection-editor translator reviewed-author editorial-director compiler" match="any">
          <names variable="director">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="director">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
