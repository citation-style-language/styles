                <date variable="accessed" form="text"/>
              </group>
              <text variable="URL"/>
            </group>
          </group>
        </else-if>
        <else-if type="map" match="any">
          <group>
            <group delimiter=". " suffix=". ">
              <text macro="contributors"/>
            </group>
            <group delimiter=", " suffix=". ">
              <text variable="genre" text-case="capitalize-first"/>
              <text variable="scale"/>
            </group>
            <text variable="collection-title" text-case="title" suffix=". "/>
            <group delimiter=", " suffix=". ">
              <text variable="archive"/>
              <text variable="archive-place"/>
              <text variable="archive_location" text-case="capitalize-first"/>
            </group>
            <group delimiter=", " suffix=". ">
              <group delimiter=": ">
                <text variable="publisher-place"/>
                <text variable="publisher"/>
              </group>
              <text macro="date-in-text-ii"/>
            </group>
            <group delimiter=": ">
              <text term="accessed" text-case="capitalize-first"/>
              <date variable="accessed" form="text" suffix=". "/>
            </group>
            <text variable="URL"/>
          </group>
        </else-if>
        <else-if type="legal_case" match="any">
          <group>
            <group delimiter=". " suffix=". ">
              <text variable="authority"/>
              <text macro="date-in-text"/>
            </group>
            <group delimiter=", ">
              <text variable="number"/>
              <text variable="volume"/>
              <text variable="container-title"/>
              <text variable="page"/>
              <text variable="references"/>
            </group>
            <group>
              <choose>
                <if is-uncertain-date="issued">
                  <date variable="issued" form="text" prefix="[" suffix="?]"/>
                </if>
                <else>
                  <date variable="issued" form="text" prefix=", " suffix=". "/>
                </else>
              </choose>
              <choose>
                <if variable="accessed URL" match="any">
                  <group delimiter=" ">
                    <group delimiter=": " suffix=". ">
                      <text term="accessed" text-case="capitalize-first"/>
                      <date variable="accessed" form="text"/>
                    </group>
                    <text variable="URL"/>
                  </group>
                </if>
              </choose>
            </group>
          </group>
        </else-if>
        <else-if type="personal_communication" match="any">
          <group>
            <group delimiter=". " suffix=". ">
              <names variable="author">
                <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
              </names>
              <text macro="date-in-text"/>
            </group>
            <choose>
              <if variable="note">
                <text variable="title" text-case="title" quotes="true"/>
                <text value=". "/>
              </if>
              <else>
                <text variable="title" text-case="title" font-style="italic"/>
                <text value=". "/>
              </else>
            </choose>
            <group delimiter=", " suffix=". ">
              <text variable="annote"/>
              <choose>
                <if variable="division" match="any">
                  <text variable="division"/>
                </if>
                <else>
                  <choose>
                    <if is-uncertain-date="issued">
                      <date variable="issued" form="text" prefix="[" suffix="?]"/>
                    </if>
                    <else>
                      <date variable="issued" form="text"/>
                    </else>
                  </choose>
                </else>
              </choose>
            </group>
            <choose>
              <if variable="annote note" match="any">
                <group delimiter=". " prefix="Alıcı: ">
                  <names variable="recipient"/>
                </group>
              </if>
            </choose>
            <group delimiter=": ">
              <text term="accessed" text-case="capitalize-first"/>
              <date variable="accessed" form="text" suffix=". "/>
            </group>
            <text variable="URL"/>
          </group>
        </else-if>
        <else-if type="document" match="any">
          <group>
            <choose>
              <if variable="medium" match="any">
                <group suffix=", ">
                  <text variable="title" text-case="title" font-style="italic" suffix=". "/>
                  <text macro="date-in-text" suffix=". "/>
                  <text variable="medium"/>
                </group>
                <group delimiter=": ">
                  <text variable="publisher-place"/>
                  <text variable="publisher"/>
                </group>
                <group delimiter=", " suffix=". ">
                  <choose>
                    <if is-uncertain-date="issued">
                      <date variable="issued" form="text" prefix="[" suffix="?]"/>
                    </if>
                    <else>
                      <date variable="issued" form="text" prefix=", " suffix=""/>
                    </else>
                  </choose>
                  <text variable="number"/>
                </group>
                <group delimiter=", " suffix=". ">
                  <text variable="archive"/>
                  <text variable="archive_location"/>
                </group>
                <group delimiter=": " suffix=". ">
                  <text term="accessed" text-case="capitalize-first"/>
                  <date variable="accessed" form="text"/>
                </group>
                <text variable="URL"/>
              </if>
              <else-if variable="genre" match="any">
                <group delimiter=". " suffix=". ">
                  <names variable="author">
                    <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
                  </names>
                  <text macro="date"/>
                </group>
                <text variable="title" text-case="title" quotes="true" suffix=". "/>
                <group delimiter=", " suffix=", ">
                  <text variable="genre"/>
                  <group delimiter=", ">
                    <date variable="issued" delimiter=" ">
                      <date-part name="day"/>
                      <date-part name="month"/>
                      <date-part name="year"/>
                    </date>
                  </group>
                </group>
                <text variable="publisher" suffix=". "/>
                <group delimiter=", " suffix=". ">
                  <text variable="archive"/>
                  <text variable="archive_location"/>
                </group>
                <group delimiter=": " suffix=". ">
                  <text term="accessed" text-case="capitalize-first"/>
                  <date variable="accessed" form="text"/>
                </group>
                <text variable="URL"/>
              </else-if>
              <else-if variable="jurisdiction" match="any">
                <group delimiter=". " suffix=". ">
                  <names variable="author">
                    <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
                  </names>
                </group>
                <group delimiter=", " suffix=", ">
                  <text variable="genre"/>
                </group>
                <text variable="publisher" suffix=". "/>
                <text variable="archive" suffix=". "/>
                <group delimiter=", " suffix=". ">
                  <text variable="archive_location"/>
                  <text variable="page"/>
                </group>
                <group delimiter=": " suffix=". ">
                  <text term="accessed" text-case="capitalize-first"/>
                  <date variable="accessed" form="text"/>
                </group>
                <text variable="URL"/>
              </else-if>
              <else>
                <text macro="date"/>
              </else>
            </choose>
          </group>
        </else-if>
        <else-if type="software" match="any">
          <group>
            <group delimiter=" - " suffix=", ">
              <names variable="author">
                <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
              </names>
            </group>
            <group delimiter=". " suffix=". ">
              <text variable="title" text-case="title" font-style="italic"/>
              <text macro="date-in-text"/>
            </group>
            <text variable="version" suffix=". "/>
            <group delimiter=", " suffix=". ">
              <text variable="medium" text-case="capitalize-first"/>
              <text variable="genre" text-case="capitalize-first"/>
            </group>
            <text value="Yazılım" suffix=", "/>
            <group delimiter=", " suffix=". ">
              <group delimiter=": ">
                <text variable="publisher-place"/>
                <text variable="publisher"/>
              </group>
              <choose>
                <if is-uncertain-date="issued">
                  <date variable="issued" form="text" prefix="[" suffix="?]"/>
                </if>
                <else>
                  <date variable="issued" form="text"/>
                </else>
              </choose>
            </group>
            <group delimiter=". ">
              <group delimiter=": ">
                <text term="accessed" text-case="capitalize-first"/>
                <date variable="accessed" form="text"/>
              </group>
              <choose>
                <if variable="DOI">
                  <text variable="DOI" prefix="https://doi.org/"/>
                </if>
                <else>
                  <text variable="URL"/>
                </else>
              </choose>
            </group>
          </group>
        </else-if>
        <else-if type="patent" match="any">
          <group>
            <group delimiter=". " suffix=". ">
              <names variable="author">
                <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
              </names>
              <text macro="date-in-text"/>
            </group>
            <text variable="title" text-case="title" font-style="italic" suffix=". "/>
            <text variable="annote" suffix=", "/>
            <group delimiter=". " suffix=", ">
              <text variable="authority"/>
            </group>
            <group delimiter=", " suffix=". ">
              <text variable="number"/>
              <text variable="call-number"/>
              <text variable="publisher-place"/>
              <choose>
                <if is-uncertain-date="submitted">
                  <date variable="submitted" form="text"/>
                </if>
                <else>
                  <date variable="submitted" form="text"/>
                </else>
              </choose>
              <choose>
                <if is-uncertain-date="issued">
                  <date variable="issued" form="text"/>
                </if>
                <else>
                  <date variable="issued" form="text"/>
                </else>
              </choose>
            </group>
            <group delimiter=": ">
              <text term="accessed" text-case="capitalize-first"/>
              <date variable="accessed" form="text" suffix=". "/>
            </group>
            <group delimiter=" ">
              <text variable="URL"/>
              <choose>
                <if variable="references">
                  <text variable="references" prefix="(" suffix=")"/>
                </if>
              </choose>
            </group>
          </group>
        </else-if>
        <else-if type="thesis" match="any">
          <group>
            <group delimiter=". ">
              <names variable="author">
                <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
              </names>
              <choose>
                <if is-uncertain-date="issued">
                  <date variable="issued" form="text" prefix="[" suffix="?]. "/>
                </if>
                <else>
                  <date variable="issued" form="text"/>
                </else>
              </choose>
              <text variable="title" text-case="title" quotes="true" suffix=". "/>
            </group>
            <text variable="genre" suffix=" Tezi, "/>
            <group delimiter=": " suffix=". ">
              <text variable="publisher-place"/>
              <text variable="publisher"/>
            </group>
            <group delimiter=": ">
              <text term="accessed" text-case="capitalize-first"/>
              <date variable="accessed" form="text" suffix=". "/>
            </group>
            <text variable="URL"/>
          </group>
        </else-if>
        <else-if type="chapter" match="any">
          <group>
            <choose>
              <if variable="division section" match="any">
                <names variable="author" delimiter=", " suffix=". ">
                  <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
                </names>
                <text macro="date" suffix=". "/>
                <group>
                  <choose>
                    <if variable="interviewer section" match="all">
                      <text variable="title" text-case="title" quotes="true" suffix=". "/>
                    </if>
                    <else-if variable="interviewer">
                      <text variable="title" text-case="title" quotes="false" font-style="italic" suffix=", "/>
                    </else-if>
                    <else-if variable="section" match="any">
                      <text variable="title" text-case="title" quotes="true" suffix=". "/>
                    </else-if>
                    <else>
                      <text variable="title" text-case="title" quotes="false" font-style="italic" suffix=". "/>
                    </else>
                  </choose>
                  <group suffix=". ">
                    <choose>
                      <if variable="interviewer section" match="all">
                        <text macro="interviewer" prefix="" suffix=""/>
                      </if>
                    </choose>
                  </group>
                  <group delimiter=", " suffix=". ">
                    <names variable="editor">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                    <choose>
                      <if variable="editor" match="any">
                        <names variable="collection-editor">
                          <label form="verb-short" suffix=" "/>
                          <name delimiter=" - "/>
                        </names>
                      </if>
                      <else>
                        <names variable="collection-editor">
                          <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                          <name delimiter=" - "/>
                        </names>
                      </else>
                    </choose>
                    <choose>
                      <if variable="editor collection-editor" match="any">
                        <names variable="translator">
                          <label form="verb-short" suffix=" "/>
                          <name delimiter=" - "/>
                        </names>
                      </if>
                      <else>
                        <names variable="translator">
                          <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                          <name delimiter=" - "/>
                        </names>
                      </else>
                    </choose>
                    <choose>
                      <if variable="editor collection-editor translator" match="any">
                        <names variable="reviewed-author">
                          <label form="verb-short" suffix=" "/>
                          <name delimiter=" - "/>
                        </names>
                      </if>
                      <else>
                        <names variable="reviewed-author">
                          <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                          <name delimiter=" - "/>
                        </names>
                      </else>
                    </choose>
                    <choose>
                      <if variable="editor collection-editor translator reviewed-author" match="any">
                        <names variable="editorial-director">
                          <label form="verb-short" suffix=" "/>
                          <name delimiter=" - "/>
                        </names>
                      </if>
                      <else>
                        <names variable="editorial-director">
                          <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                          <name delimiter=" - "/>
                        </names>
                      </else>
                    </choose>
                    <choose>
                      <if variable="editor collection-editor translator reviewed-author editorial-director" match="any">
                        <names variable="compiler">
                          <label form="verb-short" suffix=" "/>
                          <name delimiter=" - "/>
                        </names>
                      </if>
                      <else>
                        <names variable="compiler">
                          <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                          <name delimiter=" - "/>
                        </names>
                      </else>
                    </choose>
                    <choose>
                      <if variable="editor collection-editor translator reviewed-author editorial-director compiler" match="any">
                        <names variable="director">
                          <label form="verb-short" suffix=" "/>
                          <name delimiter=" - "/>
                        </names>
                      </if>
                      <else>
                        <names variable="director">
                          <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                          <name delimiter=" - "/>
                        </names>
                      </else>
                    </choose>
                  </group>
                  <names variable="container-author" suffix=", "/>
                </group>
                <group delimiter=" " suffix=", ">
                  <text variable="container-title" font-style="italic"/>
                  <choose>
                    <if variable="section" match="none">
                      <text term="in"/>
                    </if>
                  </choose>
                </group>
                <choose>
                  <if variable="division section" match="any">
                    <group delimiter=", " suffix=", ">
                      <names variable="series-creator"/>
                    </group>
                  </if>
                </choose>
                <text macro="locators-chapter" suffix=""/>
                <group suffix=". ">
                  <group delimiter=": ">
                    <text variable="publisher-place"/>
                    <text variable="publisher"/>
                  </group>
                </group>
                <group delimiter=": " suffix=". ">
                  <text term="accessed" text-case="capitalize-first"/>
                  <date variable="accessed" form="text"/>
                </group>
                <group delimiter=". ">
                  <choose>
                    <if variable="DOI">
                      <text variable="DOI" prefix="https://doi.org/"/>
                    </if>
                    <else>
                      <text variable="URL"/>
                    </else>
                  </choose>
                </group>
              </if>
            </choose>
          </group>
        </else-if>
        <else-if type="standard" match="any">
          <group delimiter=". " suffix=". ">
            <text macro="contributors"/>
            <date variable="issued">
              <date-part name="year"/>
            </date>
          </group>
          <text macro="title"/>
          <text macro="container-contributors"/>
          <text macro="locators-chapter"/>
          <text macro="collection-title-journal"/>
          <group delimiter=", ">
            <text macro="publisher"/>
            <group delimiter=", " suffix=", ">
              <date variable="issued" delimiter=" ">
                <date-part name="day"/>
                <date-part name="month"/>
                <date-part name="year"/>
              </date>
            </group>
          </group>
          <text variable="number" suffix=". "/>
          <text macro="access"/>
        </else-if>
        <else>
          <choose>
            <if variable="author editor collection-editor translator reviewed-author compiler director curator" match="none">
              <group delimiter=". " suffix=". ">
                <text macro="title"/>
                <text macro="date"/>
              </group>
            </if>
            <else-if variable="note annote" match="all">
              <group delimiter=". ">
                <text macro="title"/>
                <text macro="date"/>
                <text macro="contributors"/>
              </group>
            </else-if>
            <else>
              <group delimiter=". ">
                <text macro="contributors"/>
                <text macro="date"/>
                <text macro="title"/>
              </group>
            </else>
          </choose>
          <text macro="description"/>
          <text macro="secondary-contributors"/>
          <text macro="container-title" prefix=". "/>
          <text macro="container-contributors"/>
          <text macro="locators-chapter"/>
          <text macro="collection-title-journal" prefix=", " suffix=", "/>
          <text macro="locators"/>
          <text macro="collection-title" prefix=". "/>
          <text macro="issue"/>
          <text macro="locators-article"/>
          <choose>
            <if variable="DOI URL" match="none">
              <choose>
                <if variable="accessed archive archive_location archive-place" match="none">
                  <text value="."/>
                </if>
                <else>
                  <text macro="access" prefix=". "/>
                </else>
              </choose>
            </if>
            <else>
              <text macro="access" prefix=". "/>
            </else>
          </choose>
        </else>
      </choose>
    </layout>
  </bibliography>
