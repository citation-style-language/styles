    </group>
  </macro>
  <macro name="secondary-contributors">
    <group>
      <choose>
        <if type="interview" variable="section page editor publisher-place publisher ISBN" match="all"/>
        <else-if type="book">
          <choose>
            <if match="none" variable="section">
              <group delimiter=", " suffix=". ">
                <names variable="editor">
                  <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                  <name delimiter=" - "/>
                </names>
                <choose>
                  <if variable="editor" match="any">
                    <names variable="collection-editor">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="collection-editor">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
                <choose>
                  <if variable="editor collection-editor" match="any">
                    <names variable="translator">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="translator">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
                <choose>
                  <if variable="editor collection-editor translator" match="any">
                    <names variable="reviewed-author">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="reviewed-author">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
                <choose>
                  <if variable="editor collection-editor translator reviewed-author" match="any">
                    <names variable="editorial-director">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="editorial-director">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
                <choose>
                  <if variable="editor collection-editor translator reviewed-author editorial-director" match="any">
                    <names variable="compiler">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="compiler">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
                <choose>
                  <if variable="editor collection-editor translator reviewed-author editorial-director compiler" match="any">
                    <names variable="director">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="director">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
              </group>
            </if>
          </choose>
        </else-if>
        <else-if type="chapter entry-dictionary entry-encyclopedia paper-conference motion_picture broadcast" match="none">
          <names variable="editor translator editorial-director collection-editor curator reviewed-author original-author director" delimiter=". " suffix="">
            <label form="verb-short" text-case="capitalize-first" suffix=" "/>
            <name delimiter=" - " initialize-with=". "/>
          </names>
        </else-if>
        <else>
          <group delimiter=" ">
            <names variable="collection-editor editorial-director reviewed-author">
              <name form="short" delimiter=" - " initialize-with=". "/>
              <label form="short" prefix=" (" suffix=")"/>
              <substitute>
                <names variable="collection-editor"/>
              </substitute>
            </names>
            <text macro="recipient-note"/>
          </group>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="container-translator">
    <choose>
      <if type="chapter" match="any">
        <names variable="translator">
          <label form="verb-short" suffix=" "/>
          <name/>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="container-contributors">
    <choose>
      <if type="chapter" variable="section" match="all"/>
      <else-if type="chapter" variable="keyword" match="all"/>
      <else-if type="entry-encyclopedia" match="any">
        <group delimiter=", " suffix=", ">
          <choose>
            <if variable="container-author" match="any">
              <names variable="container-author">
                <name delimiter=" - "/>
              </names>
            </if>
          </choose>
          <choose>
            <if variable="container-author author" match="all">
              <group delimiter=". ">
                <names variable="editor translator" delimiter=", ">
                  <label form="verb" suffix=" "/>
                  <name and="text" delimiter=", "/>
                </names>
              </group>
            </if>
            <else>
              <names variable="editor collection-editor editorial-director reviewed-author original-author director">
                <label form="verb-short" text-case="lowercase" suffix=" "/>
                <name delimiter=" - "/>
              </names>
            </else>
          </choose>
        </group>
      </else-if>
      <else-if type="chapter entry-dictionary paper-conference" match="any">
        <group delimiter=", " suffix=", ">
          <choose>
            <if variable="author">
              <choose>
                <if variable="container-author" match="any">
                  <names variable="container-author">
                    <name delimiter=" - "/>
                  </names>
                </if>
              </choose>
              <choose>
                <if variable="container-author author" match="all">
                  <group delimiter=". ">
                    <names variable="editor translator" delimiter=", ">
                      <label form="verb" suffix=" "/>
                      <name and="text" delimiter=", "/>
                    </names>
                  </group>
                </if>
                <else>
                  <names variable="editor collection-editor editorial-director reviewed-author original-author director">
                    <label form="verb-short" text-case="lowercase" suffix=" "/>
                    <name delimiter=" - "/>
                  </names>
                </else>
              </choose>
            </if>
          </choose>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="recipient-note">
    <names variable="recipient" delimiter=", ">
      <label form="verb" text-case="lowercase" suffix=" "/>
      <name and="text" delimiter=", "/>
    </names>
  </macro>
  <macro name="contributors-note">
    <choose>
      <if type="book" variable="annote division" match="all"/>
      <else-if type="book" variable="keyword" match="all">
        <choose>
          <if variable="host" match="any">
            <text variable="keyword"/>
          </if>
        </choose>
      </else-if>
      <else-if type="book" variable="note annote" match="all"/>
      <else-if type="entry-encyclopedia" variable="URL" match="all">
        <choose>
          <if variable="author" match="any">
            <group delimiter=" ">
              <names variable="author">
                <name form="short" delimiter=" - " initialize-with=". "/>
                <label form="short" prefix=" (" suffix=")"/>
              </names>
            </group>
          </if>
        </choose>
      </else-if>
      <else-if type="report" variable="keyword" match="all">
        <text variable="keyword"/>
      </else-if>
      <else-if type="motion_picture" match="any">
        <names variable="contributor">
          <name form="short"/>
        </names>
      </else-if>
      <else-if type="graphic">
        <choose>
          <if variable="illustrator">
            <names variable="illustrator">
              <name form="short" delimiter=" - " initialize-with=". "/>
            </names>
          </if>
          <else-if variable="author PMID" match="all">
            <names variable="author">
              <name form="short" delimiter=" - " initialize-with=". "/>
              <label form="short" prefix=" (" suffix=")"/>
            </names>
          </else-if>
          <else-if variable="script-writer" match="any">
            <names variable="author">
              <name form="short"/>
            </names>
          </else-if>
          <else-if variable="abstract" match="none">
            <names variable="contributor">
              <name form="short"/>
            </names>
          </else-if>
        </choose>
      </else-if>
      <else-if type="personal_communication" match="any">
        <names variable="author">
          <name form="short"/>
        </names>
      </else-if>
      <else-if type="document" variable="genre issued keyword" match="all">
        <text variable="keyword"/>
      </else-if>
      <else-if type="dataset">
        <text variable="division"/>
      </else-if>
      <else-if type="patent" variable="keyword" match="all">
        <text variable="keyword"/>
      </else-if>
      <else-if type="webpage" variable="note" match="all"/>
      <else>
        <group delimiter=" ">
          <names variable="author">
            <name form="short" delimiter=" - " initialize-with=". "/>
            <label form="short" prefix=" (" suffix=")"/>
            <substitute>
              <names variable="editor"/>
              <names variable="translator"/>
            </substitute>
          </names>
          <text macro="recipient-note"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="editor">
    <choose>
      <if type="entry-encyclopedia" match="none">
        <names variable="editor">
          <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
          <label form="short" prefix=" (" suffix=")"/>
        </names>
      </if>
      <else>
        <names variable="author">
          <substitute>
            <text term="editor" form="short" suffix=". "/>
            <names variable="editor">
              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
            </names>
          </substitute>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="translator">
    <names variable="translator">
      <name name-as-sort-order="first" and="text" sort-separator=", " delimiter=", " delimiter-precedes-last="always"/>
      <label form="verb-short" prefix=", "/>
    </names>
  </macro>
  <macro name="recipient">
    <choose>
      <if type="personal_communication" variable="recipient" match="all"/>
      <else>
        <group delimiter=" ">
          <choose>
            <if type="personal_communication">
              <choose>
                <if variable="genre">
                  <text variable="genre" text-case="capitalize-first"/>
                </if>
                <else>
                  <text term="letter" text-case="capitalize-first"/>
                </else>
              </choose>
            </if>
          </choose>
          <text macro="recipient-note"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="contributors-anonymous">
    <group delimiter=". ">
      <choose>
        <if variable="author" match="none">
          <choose>
            <if variable="status" match="any">
              <text term="anonymous"/>
            </if>
          </choose>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="contributors">
    <group delimiter=". ">
      <choose>
        <if variable="author contributor editor collection-editor translator reviewed-author compiler director curator performer script-writer" match="none">
          <choose>
            <if variable="annote note PMID" match="all">
              <group delimiter=". " suffix=". ">
                <text variable="title" font-style="italic"/>
                <text macro="date-sort"/>
              </group>
            </if>
            <else-if type="report" match="any">
              <text variable="publisher"/>
            </else-if>
            <else-if variable="issued">
              <group delimiter=". " suffix=". ">
                <text variable="title"/>
                <text macro="date-sort" suffix="."/>
                <text variable="collection-title" font-style="italic"/>
              </group>
            </else-if>
            <else>
              <group delimiter=". ">
                <text variable="title" font-style="italic"/>
                <text macro="date-sort"/>
              </group>
            </else>
          </choose>
        </if>
        <else-if type="map">
          <group delimiter=". ">
            <names variable="author">
              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
            </names>
            <text macro="date-in-text"/>
            <text variable="title" font-style="italic"/>
          </group>
        </else-if>
        <else-if variable="annote note PMID" match="all">
          <choose>
            <if variable="author translator" match="any">
              <group delimiter=". " suffix=". ">
                <text variable="title" font-style="italic"/>
                <text macro="date-sort"/>
                <names variable="author">
                  <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
                </names>
              </group>
            </if>
            <else>
              <group delimiter=". " suffix=". ">
                <text variable="title" font-style="italic"/>
                <text macro="date-sort"/>
              </group>
            </else>
          </choose>
        </else-if>
        <else-if variable="division PMID" match="all">
          <group delimiter=". ">
            <text macro="title"/>
            <text macro="date-sort"/>
            <names variable="author">
              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="never"/>
            </names>
          </group>
        </else-if>
        <else-if type="graphic">
          <choose>
            <if variable="PMID part-number" match="all">
              <group suffix=". ">
                <group delimiter=". " suffix=". ">
                  <text macro="title"/>
                  <text macro="date"/>
                </group>
                <text variable="collection-title" font-style="italic" suffix=", "/>
                <text variable="medium" text-case="capitalize-first" suffix=". "/>
                <names variable="author contributor">
                  <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="never"/>
                </names>
              </group>
            </if>
            <else>
              <group delimiter=". " suffix=". ">
                <names variable="author contributor">
                  <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="never"/>
                </names>
                <text macro="date"/>
              </group>
            </else>
          </choose>
        </else-if>
        <else-if variable="note annote PMID" match="all">
          <group suffix=". ">
            <names variable="author">
              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="never"/>
            </names>
          </group>
        </else-if>
        <else-if variable="division section" match="any">
          <names variable="author">
            <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="never"/>
          </names>
        </else-if>
        <else-if type="book" variable="status medium" match="all">
          <group>
            <names variable="author" prefix="[" suffix="] ">
              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="never"/>
            </names>
            <names variable="contributor" prefix="" suffix="">
              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
            </names>
          </group>
        </else-if>
        <else-if type="document" variable="host keyword annote" match="all">
          <group>
            <names variable="author" prefix="[" suffix="] ">
              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="never"/>
            </names>
            <names variable="contributor" prefix="" suffix="">
              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
            </names>
          </group>
        </else-if>
        <else-if type="book" variable="note annote" match="all"/>
        <else-if type="motion_picture">
          <group suffix=". ">
            <names variable="author">
              <name name-as-sort-order="first" and="text" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
              <substitute>
                <names variable="director" suffix=" (yön.). ">
                  <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
                </names>
              </substitute>
            </names>
          </group>
        </else-if>
        <else-if type="webpage" variable="note annote" match="all"/>
        <else-if type="entry-encyclopedia">
          <names variable="author">
            <name name-as-sort-order="first" and="text" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
            <substitute>
              <names variable="editor">
                <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
                <label form="verb-short" prefix=" (" suffix=")"/>
              </names>
            </substitute>
          </names>
        </else-if>
        <else-if type="broadcast" match="any">
          <names variable="author">
            <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
            <substitute>
              <names variable="director" suffix=" (sun.). ">
                <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
              </names>
            </substitute>
          </names>
        </else-if>
        <else-if type="chapter map legal_case graphic software legislation" match="none">
          <names variable="author">
            <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
            <substitute>
              <choose>
                <if type="report">
                  <text variable="publisher" suffix=". "/>
                </if>
                <else-if type="entry-encyclopedia" match="none">
                  <text macro="editor"/>
                </else-if>
              </choose>
              <text macro="translator"/>
              <choose>
                <if type="article-magazine article-newspaper webpage post-weblog" match="any">
                  <text variable="container-title"/>
                </if>
              </choose>
            </substitute>
          </names>
        </else-if>
      </choose>
      <text macro="recipient"/>
    </group>
  </macro>
  <macro name="recipient-short">
    <names variable="recipient">
      <label form="verb" text-case="lowercase" suffix=" "/>
      <name form="short" and="text" delimiter=", "/>
    </names>
  </macro>
  <macro name="contributors-short">
    <choose>
      <if variable="host keyword" match="all">
        <text variable="keyword"/>
      </if>
      <else-if type="dataset" variable="division" match="all">
        <text variable="division"/>
      </else-if>
      <else-if type="graphic" variable="illustrator" match="all">
        <text macro="title-note"/>
      </else-if>
      <else>
        <names variable="author">
          <name form="short" delimiter=" - " initialize-with=". "/>
          <substitute>
            <names variable="contributor"/>
            <names variable="editor"/>
            <names variable="translator"/>
            <names variable="director"/>
            <text macro="substitute-title"/>
            <text macro="title"/>
          </substitute>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="contributors-sort">
    <choose>
      <if type="motion_picture software legislation">
        <choose>
          <if variable="author" match="none">
            <text variable="title"/>
          </if>
        </choose>
      </if>
      <else-if type="book" variable="note annote" match="all">
        <text variable="title"/>
      </else-if>
      <else-if type="book" variable="note section" match="all">
        <text variable="section"/>
      </else-if>
      <else-if type="webpage" variable="note annote" match="all">
        <text variable="title"/>
      </else-if>
      <else-if type="entry-dictionary">
        <choose>
          <if variable="author" match="none">
            <text variable="publisher"/>
          </if>
        </choose>
      </else-if>
      <else-if type="entry-encyclopedia">
        <choose>
          <if variable="author" match="none">
            <text variable="title"/>
          </if>
        </choose>
      </else-if>
      <else-if type="dataset report">
        <choose>
          <if variable="author" match="none">
            <text variable="publisher"/>
          </if>
        </choose>
      </else-if>
      <else-if type="legal_case">
        <choose>
          <if variable="author" match="none">
            <text variable="authority"/>
          </if>
        </choose>
      </else-if>
      <else-if type="graphic">
        <choose>
          <if variable="contributor" match="any">
            <choose>
              <if variable="abstract" match="any">
                <text variable="title"/>
              </if>
            </choose>
          </if>
          <else>
            <choose>
              <if variable="abstract" match="none">
                <text variable="title"/>
              </if>
            </choose>
          </else>
        </choose>
      </else-if>
      <else-if type="document" variable="jurisdiction" match="all">
        <text variable="archive"/>
      </else-if>
    </choose>
    <names variable="author contributor editor">
      <name name-as-sort-order="all" and="text" sort-separator=", " delimiter=", " delimiter-precedes-last="always"/>
      <substitute>
        <names variable="contributor"/>
        <names variable="editor"/>
        <names variable="translator"/>
        <text macro="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="interviewer-note">
    <choose>
      <if variable="ISBN" match="none">
        <names variable="interviewer" delimiter=", ">
          <label form="verb" text-case="lowercase" suffix=" "/>
          <name and="text" delimiter=", "/>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="interviewer">
    <names variable="interviewer" delimiter=" - ">
      <label form="verb" text-case="capitalize-first" suffix=": "/>
      <name delimiter=" - "/>
    </names>
  </macro>
  <macro name="title-note">
    <choose>
      <if variable="title" match="none">
        <text variable="genre"/>
      </if>
      <else-if type="dataset">
        <text variable="title" quotes="true" prefix="Aranan: " form="short"/>
      </else-if>
      <else-if type="document" variable="jurisdiction" match="all">
        <text variable="title" text-case="title" quotes="false" form="short"/>
      </else-if>
      <else-if type="document" variable="medium number publisher-place" match="all">
        <text variable="title" text-case="title" quotes="false" font-style="italic" form="short"/>
        <group prefix=" (" suffix=")">
          <text variable="publisher" suffix=", "/>
          <group delimiter=".">
            <date variable="issued" form="numeric">
              <date-part name="day" form="numeric-leading-zeros"/>
              <date-part name="month"/>
              <date-part name="year"/>
            </date>
          </group>
        </group>
      </else-if>
      <else-if type="document" variable="genre issued" match="all">
        <text variable="title" text-case="title" quotes="true"/>
        <group prefix=" (" suffix=")">
          <text variable="genre" suffix=", "/>
          <date variable="issued" form="text"/>
        </group>
      </else-if>
      <else-if type="book" variable="annote" match="all">
        <choose>
          <if variable="annote note PMCID" match="all">
            <text variable="title" text-case="title" form="short"/>
          </if>
          <else-if variable="annote note" match="all">
            <text variable="title" text-case="title" font-style="italic" form="short"/>
          </else-if>
          <else-if variable="annote jurisdiction" match="all">
            <text variable="title" text-case="title" form="short" font-style="italic"/>
          </else-if>
          <else-if variable="annote division" match="all">
            <group delimiter=", ">
              <text variable="title" text-case="title" form="short" font-style="italic"/>
              <text variable="keyword"/>
            </group>
          </else-if>
          <else-if variable="annote"/>
          <else>
            <text variable="title" text-case="title" form="short"/>
          </else>
        </choose>
      </else-if>
      <else-if type="book manuscript map" match="any">
        <text variable="title" text-case="title" font-style="italic" form="short"/>
        <group delimiter=" " prefix=", ">
          <text term="version"/>
          <text variable="version"/>
        </group>
      </else-if>
      <else-if type="thesis" match="any">
        <text variable="title" text-case="title" quotes="true" form="short"/>
      </else-if>
      <else-if type="article-journal paper-conference entry-dictionary" match="any">
        <text variable="title" quotes="true" form="short"/>
      </else-if>
      <else-if type="interview" match="any">
        <text variable="title" quotes="true" form="short"/>
      </else-if>
      <else-if variable="reviewed-author">
        <text variable="title" font-style="italic" prefix="review of "/>
      </else-if>
      <else-if type="report">
        <text variable="title" font-style="italic" form="short"/>
      </else-if>
      <else-if type="webpage">
        <choose>
          <if variable="note" match="any">
            <text variable="title" font-style="italic" form="short"/>
          </if>
          <else>
            <text variable="title" quotes="true" form="short"/>
          </else>
        </choose>
      </else-if>
      <else-if type="legislation" match="any">
        <text variable="title" text-case="title" form="short" font-style="italic"/>
      </else-if>
      <else-if type="motion_picture" variable="annote" match="all">
        <text variable="title" text-case="title" form="short"/>
      </else-if>
      <else-if type="motion_picture broadcast song software patent" match="any">
        <text variable="title" text-case="title" font-style="italic" form="short"/>
      </else-if>
      <else-if type="personal_communication" match="any">
        <choose>
          <if variable="note" match="any">
            <text variable="title" text-case="title" quotes="true" form="short"/>
          </if>
          <else>
            <text variable="title" text-case="title" font-style="italic" form="short"/>
          </else>
        </choose>
      </else-if>
      <else-if type="graphic" match="any">
        <choose>
          <if variable="part-title" match="any">
            <group delimiter=", ">
              <text variable="title" text-case="title" form="short"/>
              <text variable="part-title" font-style="italic"/>
            </group>
          </if>
          <else-if variable="collection-title genre PMID" match="any">
            <text variable="title" text-case="title" form="short"/>
          </else-if>
          <else>
            <group delimiter=", ">
              <text variable="title" text-case="title" font-style="italic" form="short"/>
            </group>
          </else>
        </choose>
      </else-if>
      <else-if type="legal_case" match="any">
        <text variable="title" form="short" text-case="title"/>
      </else-if>
      <else-if type="chapter">
        <text variable="title" font-style="italic" quotes="false"/>
      </else-if>
      <else>
        <text variable="title" quotes="true"/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if variable="division PMID" match="all">
        <text variable="title" text-case="title" font-style="italic"/>
      </if>
      <else-if variable="note annote PMID" match="all">
        <choose>
          <if variable="author editor collection-editor translator reviewed-author compiler director curator" match="none">
            <text variable="title" text-case="title" font-style="italic" suffix=". "/>
          </if>
          <else>
            <text variable="title" text-case="title" font-style="italic"/>
          </else>
        </choose>
      </else-if>
      <else-if type="document" variable="genre" match="all"/>
      <else-if type="document" variable="annote jurisdiction" match="all">
        <text variable="title" text-case="title" form="short"/>
      </else-if>
      <else-if type="book" variable="annote" match="all">
        <choose>
          <if variable="annote note PMCID" match="all">
            <text variable="title" text-case="title" form="short"/>
          </if>
          <else-if variable="annote note" match="all">
            <text variable="title" text-case="title" font-style="italic" form="short"/>
          </else-if>
          <else-if variable="annote jurisdiction" match="all">
            <text variable="title" text-case="title" font-style="italic"/>
          </else-if>
          <else-if variable="annote division" match="all">
            <group delimiter=", ">
              <text variable="title" text-case="title" font-style="italic" suffix=". "/>
              <text variable="keyword"/>
            </group>
          </else-if>
          <else-if variable="annote host" match="all">
            <group delimiter=", ">
              <text variable="title" text-case="title" font-style="italic" suffix=". "/>
            </group>
          </else-if>
          <else>
            <text variable="title" text-case="title" form="short" font-style="italic" suffix=". "/>
          </else>
        </choose>
      </else-if>
      <else-if type="article-newspaper">
        <text variable="title" quotes="true"/>
      </else-if>
      <else-if variable="PMID" match="any">
        <choose>
          <if type="entry-dictionary">
            <text variable="title" quotes="false" font-style="italic"/>
          </if>
          <else>
            <text variable="title"/>
          </else>
        </choose>
      </else-if>
      <else-if variable="author editor collection-editor translator reviewed-author compiler director curator" match="none">
        <choose>
          <if type="entry-dictionary" variable="number-of-volumes edition event-place" match="none">
            <text variable="title" text-case="title" font-style="italic"/>
          </if>
          <else-if type="entry-dictionary" variable="number-of-volumes edition event-place" match="all">
            <text variable="title" text-case="title" font-style="italic" suffix=". "/>
          </else-if>
          <else>
            <text variable="title" text-case="title" font-style="italic"/>
          </else>
        </choose>
      </else-if>
      <else-if type="chapter" variable="section" match="all"/>
      <else-if variable="title" match="none">
        <choose>
          <if type="personal_communication" match="none">
            <text variable="genre" text-case="capitalize-first"/>
          </if>
        </choose>
      </else-if>
      <else-if type="dataset">
        <text variable="title" quotes="true" prefix="Aranan: "/>
      </else-if>
      <else-if type="graphic" match="any"/>
      <else-if type="song" match="any"/>
      <else-if type="map" match="any"/>
      <else-if type="software" match="any"/>
      <else-if type="legislation" match="any"/>
      <else-if type="motion_picture" match="any"/>
      <else-if type="manuscript" match="any">
        <text variable="title" text-case="title" font-style="italic"/>
      </else-if>
      <else-if type="book">
        <choose>
          <if variable="editor translator collection-editor reviewed-author editorial-director compiler director" match="any">
            <text variable="title" text-case="title" font-style="italic" suffix=". "/>
          </if>
          <else-if variable="host keyword">
            <choose>
              <if variable="publisher publisher-place issued" match="any">
                <text variable="title" text-case="title" font-style="italic" suffix=". "/>
              </if>
              <else>
                <text variable="title" text-case="title" font-style="italic"/>
              </else>
            </choose>
          </else-if>
          <else>
            <text variable="title" text-case="title" font-style="italic" suffix=". "/>
          </else>
        </choose>
        <choose>
          <if variable="section" match="any">
            <text macro="interviewer" prefix="(" suffix=")"/>
            <text macro="secondary-contributors-nehir" prefix=", "/>
            <group delimiter=":" prefix=", " suffix=". ">
              <text variable="volume"/>
              <text variable="page"/>
            </group>
          </if>
        </choose>
      </else-if>
      <else-if type="standard">
        <text variable="title" text-case="title" font-style="italic" suffix=". "/>
      </else-if>
      <else-if variable="reviewed-author">
        <group delimiter=", ">
          <text variable="title" font-style="italic" prefix="Review of "/>
          <names variable="reviewed-author">
            <label form="verb-short" text-case="lowercase" suffix=" "/>
            <name and="text" delimiter=", "/>
          </names>
        </group>
      </else-if>
      <else-if type="bill" match="any">
        <text variable="title"/>
      </else-if>
      <else-if type="chapter" variable="interviewer genre" match="all">
        <group delimiter=". ">
          <text variable="title" quotes="true"/>
          <text macro="description"/>
        </group>
      </else-if>
      <else-if type="chapter" variable="interviewer" match="all">
        <group delimiter=" ">
          <text variable="title" quotes="true"/>
          <text macro="description"/>
        </group>
      </else-if>
      <else-if type="chapter" variable="keyword" match="all">
        <group delimiter=" ">
          <text variable="title" quotes="false" font-style="italic" suffix="."/>
        </group>
      </else-if>
      <else>
        <choose>
          <if type="thesis" match="any"/>
          <else-if type="patent" match="any"/>
          <else-if type="webpage">
            <choose>
              <if variable="note" match="any">
                <text variable="title" font-style="italic"/>
              </if>
              <else>
                <text variable="title" quotes="true"/>
              </else>
            </choose>
          </else-if>
          <else-if type="article-journal">
            <choose>
              <if variable="translator" match="any">
                <text variable="title" quotes="true" suffix=". "/>
              </if>
              <else>
                <text variable="title" quotes="true" suffix="."/>
              </else>
            </choose>
          </else-if>
          <else-if type="entry-dictionary" match="none">
            <text variable="title" quotes="true" suffix="."/>
          </else-if>
          <else>
            <text variable="title" text-case="title" font-style="italic"/>
          </else>
        </choose>
      </else>
    </choose>
  </macro>
  <macro name="title-short">
    <choose>
      <if variable="title" match="none">
        <choose>
          <if type="interview">
            <text term="interview"/>
          </if>
          <else-if type="manuscript speech" match="any">
            <text variable="genre" form="short"/>
          </else-if>
        </choose>
      </if>
      <else-if type="book" match="any">
        <text variable="title" text-case="title" form="short" font-style="italic"/>
      </else-if>
      <else-if type="interview" match="any">
        <text variable="title" form="short"/>
      </else-if>
      <else-if type="bill" match="any">
        <text variable="title"/>
      </else-if>
      <else>
        <text variable="title" text-case="title" form="short" quotes="true"/>
      </else>
    </choose>
  </macro>
  <macro name="date-disambiguate">
    <choose>
      <if disambiguate="true" type="personal_communication" match="any">
        <text macro="issued"/>
      </if>
    </choose>
  </macro>
  <macro name="description-note">
    <choose>
      <if type="motion_picture" match="any">
        <group delimiter=", ">
          <text variable="part-number"/>
          <text variable="part-title"/>
          <names variable="director" prefix="yön. "/>
        </group>
      </if>
      <else-if type="broadcast" match="any">
        <text variable="number" prefix="Bölüm " suffix=", "/>
        <text variable="container-title" quotes="true" suffix=", "/>
        <group delimiter=". " prefix="sun. ">
          <names variable="director">
            <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always" form="short"/>
          </names>
        </group>
        <choose>
          <if variable="division">
            <text variable="division"/>
          </if>
        </choose>
      </else-if>
      <else-if type="graphic" variable="issued genre publisher-place" match="all">
        <names variable="contributor"/>
      </else-if>
      <else-if type="song graphic" match="any"/>
      <else-if type="personal_communication" variable="genre" match="all"/>
      <else-if type="document" variable="medium number publisher-place" match="all"/>
      <else-if type="document" variable="genre issued" match="all"/>
      <else-if type="software" match="all">
        <text variable="version"/>
      </else-if>
      <else-if type="article-journal entry-dictionary entry-encyclopedia map report" variable="medium" match="all"/>
      <else-if type="webpage" match="any"/>
      <else-if type="chapter" variable="interviewer" match="all"/>
      <else>
        <group delimiter=", ">
          <text variable="medium"/>
          <choose>
            <if variable="title" match="none"/>
            <else-if type="thesis" match="any"/>
            <else-if type="manuscript speech" match="any"/>
            <else-if type="book" variable="genre" match="all"/>
            <else-if type="legal_case">
              <group delimiter=", ">
                <text variable="number"/>
              </group>
            </else-if>
            <else>
              <text variable="genre"/>
            </else>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="description">
    <group delimiter=", ">
      <choose>
        <if variable="title" match="none"/>
        <else-if type="thesis speech" match="any"/>
        <else-if type="report" match="any"/>
        <else-if type="motion_picture" match="any"/>
        <else-if type="personal_communication" match="any"/>
        <else-if type="document" match="any"/>
        <else-if type="graphic" match="any"/>
        <else-if type="map" match="any"/>
        <else-if type="software" match="any"/>
        <else-if variable="annote genre keyword medium note source status" match="all"/>
        <else-if type="webpage" match="any"/>
        <else-if type="manuscript" match="any"/>
        <else-if type="chapter" variable="interviewer genre" match="all">
          <text macro="interviewer" prefix="" suffix=""/>
        </else-if>
        <else-if type="chapter" variable="interviewer" match="all">
          <text macro="interviewer" prefix="(" suffix=")"/>
        </else-if>
        <else>
          <text variable="genre" text-case="capitalize-first" suffix=". "/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="container-title-note">
    <group delimiter=" ">
      <choose>
        <if type="entry-dictionary" match="any">
          <text variable="container-title" quotes="true"/>
        </if>
        <else-if type="chapter entry-encyclopedia paper-conference" match="any"></else-if>
        <else-if type="webpage">
          <text variable="container-title"/>
        </else-if>
        <else-if type="post-weblog">
          <text variable="container-title" text-case="title" font-style="italic" suffix=" (blog)"/>
        </else-if>
        <else-if type="bill" match="any"/>
        <else>
          <text variable="container-title" text-case="title" font-style="italic"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="container-title">
    <choose>
      <if type="article-journal">
        <choose>
          <if variable="volume issue issued" match="any">
            <text variable="container-title" text-case="title" font-style="italic" suffix=" "/>
          </if>
          <else>
            <text variable="container-title" text-case="title" font-style="italic"/>
          </else>
        </choose>
      </if>
      <else>
        <group delimiter=" ">
          <choose>
            <if type="entry-encyclopedia paper-conference" match="any">
              <choose>
                <if variable="page" match="any">
                  <text variable="container-title" text-case="title" font-style="italic" suffix=", "/>
                </if>
                <else>
                  <text variable="container-title" text-case="title" font-style="italic" suffix=""/>
                </else>
              </choose>
            </if>
            <else-if type="chapter" variable="section" match="all"/>
            <else-if type="chapter" variable="keyword annote" match="all">
              <text variable="container-title" font-style="italic"/>
              <text term="in"/>
            </else-if>
            <else-if type="chapter" variable="keyword" match="all">
              <text variable="container-title" font-style="italic"/>
              <text term="in"/>
            </else-if>
            <else-if type="webpage" match="any">
              <choose>
                <if variable="note" match="any">
                  <text variable="container-title" text-case="title" font-style="italic"/>
                </if>
                <else>
                  <text variable="container-title" text-case="title" quotes="true"/>
