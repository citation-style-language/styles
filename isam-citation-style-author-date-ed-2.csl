                  </if>
                  <else>
                    <text variable="edition" text-case="capitalize-first" suffix="."/>
                  </else>
                </choose>
              </else>
            </choose>
          </if>
          <else>
            <choose>
              <if is-numeric="edition">
                <group delimiter=" " suffix=". ">
                  <number variable="edition" suffix="."/>
                  <text term="edition"/>
                </group>
              </if>
              <else>
                <choose>
                  <if variable="number-of-volumes">
                    <text variable="edition" suffix=". "/>
                  </if>
                  <else>
