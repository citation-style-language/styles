      <if type="bill article-journal chapter paper-conference entry-encyclopedia" match="none">
        <text macro="locators"/>
      </if>
    </choose>
  </macro>
  <macro name="locators">
    <choose>
      <if type="article-journal">
        <group delimiter=", ">
          <text macro="collection-title-journal"/>
          <text variable="volume"/>
          <group delimiter=" ">
            <text variable="issue"/>
          </group>
        </group>
      </if>
      <else-if type="bill" match="any">
        <text macro="legal-cites"/>
      </else-if>
      <else-if type="book" variable="note annote PMID" match="all">
        <group delimiter=", " suffix=". ">
          <group delimiter=" ">
            <number variable="number-of-volumes" form="numeric"/>
            <text term="volume"/>
          </group>
          <text macro="edition"/>
        </group>
      </else-if>
      <else-if type="book report" match="any">
        <group delimiter=", ">
          <choose>
            <if type="book"/>
            <else>
              <group delimiter=" ">
                <number variable="volume" form="numeric" suffix="."/>
                <text term="volume"/>
              </group>
            </else>
          </choose>
          <group suffix=". ">
            <group delimiter=" ">
              <number variable="number-of-volumes" form="numeric"/>
              <text term="volume"/>
            </group>
            <text macro="edition"/>
          </group>
        </group>
      </else-if>
      <else-if variable="division jurisdiction medium genre status chapter-number part-number ISBN URL DOI" type="webpage" match="all">
        <choose>
          <if type="entry-dictionary" match="any">
            <group delimiter=", ">
              <group delimiter=" ">
                <number variable="number-of-volumes" form="numeric"/>
                <text term="volume"/>
              </group>
              <text macro="edition"/>
            </group>
          </if>
        </choose>
      </else-if>
      <else-if type="entry-encyclopedia paper-conference" match="any">
        <group delimiter=". ">
          <text macro="edition"/>
          <choose>
            <if variable="page" match="none">
              <group delimiter=" ">
                <text term="volume" form="short" text-case="capitalize-first"/>
                <number variable="volume" form="numeric"/>
              </group>
            </if>
          </choose>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="locators-newspaper">
    <choose>
      <if type="article-newspaper">
        <group delimiter=", ">
          <group delimiter=". ">
            <number variable="edition"/>
            <text term="edition"/>
          </group>
          <group delimiter=" ">
            <text term="section" form="short"/>
            <text variable="section"/>
          </group>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="event-note">
