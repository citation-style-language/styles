  <macro name="publisher">
    <choose>
      <if type="thesis" match="any"/>
      <else-if type="report" match="any"/>
      <else-if type="motion_picture" match="any"/>
      <else-if type="song" match="any"/>
      <else-if type="document" variable="medium" match="all"/>
      <else-if type="dataset"/>
      <else-if type="document" variable="genre issued" match="all"/>
      <else-if type="document" variable="jurisdiction" match="all"/>
      <else-if type="document" variable="medium" match="all"/>
      <else-if type="software" match="any"/>
      <else-if type="interview" match="any"/>
      <else-if type="patent" match="any"/>
      <else-if type="standard" match="any">
        <text variable="publisher-place" suffix=": "/>
        <text variable="publisher"/>
      </else-if>
      <else-if type="speech">
        <text variable="event-place"/>
      </else-if>
      <else-if type="entry-dictionary" variable="annote" match="all">
        <text variable="publisher" font-style="italic"/>
      </else-if>
      <else-if type="book" variable="annote jurisdiction" match="all">
        <group delimiter=": ">
          <text variable="publisher-place"/>
          <choose>
            <if variable="publisher">
              <text variable="publisher"/>
            </if>
            <else>
              <text value="[y.y.]"/>
            </else>
          </choose>
        </group>
      </else-if>
      <else-if type="book" variable="note annote PMID" match="all">
        <group delimiter=": " suffix=". ">
          <text variable="publisher-place"/>
          <choose>
            <if variable="publisher">
              <text variable="publisher"/>
            </if>
            <else>
              <text value="[y.y.]"/>
            </else>
          </choose>
        </group>
      </else-if>
      <else-if type="entry-encyclopedia" variable="publisher publisher-place" match="any">
        <group>
          <text variable="publisher-place" suffix=": "/>
          <text variable="publisher"/>
        </group>
      </else-if>
      <else>
        <choose>
          <if variable="annote jurisdiction" match="all">
            <text variable="publisher-place"/>
          </if>
          <else>
            <group delimiter=": ">
              <text variable="publisher-place"/>
              <choose>
                <if variable="publisher">
                  <text variable="publisher"/>
                </if>
                <else>
                  <text value="[y.y.]"/>
                </else>
              </choose>
            </group>
          </else>
        </choose>
      </else>
    </choose>
  </macro>
  <macro name="compiler">
