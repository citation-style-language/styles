  <macro name="locators-chapter">
    <choose>
      <if type="entry-dictionary entry-encyclopedia paper-conference" match="any">
        <choose>
          <if variable="author container-author" match="all"/>
          <else>
            <choose>
              <if variable="page" match="any">
                <group delimiter=". ">
                  <group>
                    <text variable="volume" suffix=":"/>
                    <text variable="page"/>
                  </group>
                  <group delimiter=" ">
                    <number variable="number-of-volumes" form="numeric"/>
                    <text term="volume"/>
                  </group>
                </group>
              </if>
              <else-if variable="page" match="any">
                <group delimiter=". ">
                  <group>
                    <text variable="volume" suffix=":"/>
                    <text variable="page"/>
                  </group>
                  <group delimiter=" ">
                    <number variable="number-of-volumes" form="numeric"/>
                    <text term="volume"/>
                  </group>
                </group>
              </else-if>
              <else>
                <group delimiter=". " prefix=". ">
                  <group>
                    <text variable="volume"/>
                  </group>
                  <group delimiter=" ">
                    <number variable="number-of-volumes" form="numeric"/>
                    <text term="volume"/>
                  </group>
                </group>
              </else>
            </choose>
          </else>
        </choose>
      </if>
    </choose>
    <choose>
      <if type="chapter" variable="division" match="all">
        <group delimiter=". " suffix=". ">
          <group delimiter=":">
            <text variable="volume"/>
            <text variable="page"/>
          </group>
          <group delimiter=", ">
            <group>
              <number variable="number-of-volumes" form="numeric" suffix=" "/>
              <text term="volume"/>
            </group>
            <text macro="edition"/>
          </group>
        </group>
      </if>
      <else-if type="chapter" variable="section" match="all">
        <group delimiter=". " suffix=". ">
          <group delimiter=":">
            <text variable="volume"/>
            <text variable="page"/>
          </group>
          <group delimiter=", ">
            <group>
              <number variable="number-of-volumes" form="numeric" suffix=" "/>
              <text term="volume"/>
            </group>
            <text macro="edition"/>
          </group>
        </group>
      </else-if>
      <else-if type="chapter" variable="interviewer issue volume genre" match="all">
        <group>
          <text variable="volume" suffix=", "/>
          <text variable="issue"/>
          <choose>
            <if is-uncertain-date="issued">
              <date variable="issued" form="text" prefix="[" suffix="?]"/>
            </if>
            <else>
              <date variable="issued" form="text" prefix=" (" suffix=")"/>
            </else>
          </choose>
          <text variable="page" prefix=": "/>
        </group>
      </else-if>
      <else-if type="chapter" match="any">
        <choose>
          <if variable="compiler" match="any">
            <group delimiter=", ">
              <text macro="compiler"/>
              <group>
                <text variable="volume" suffix=":"/>
                <text variable="page"/>
              </group>
              <text macro="edition"/>
            </group>
          </if>
          <else>
            <choose>
              <if variable="page">
                <group delimiter=". ">
                  <group>
                    <text variable="volume" suffix=":"/>
                    <text variable="page"/>
                  </group>
                  <group delimiter=" ">
                    <number variable="number-of-volumes" form="numeric"/>
                    <text term="volume"/>
                  </group>
                  <text macro="edition"/>
                  <text macro="compiler"/>
                </group>
              </if>
            </choose>
          </else>
        </choose>
      </else-if>
    </choose>
