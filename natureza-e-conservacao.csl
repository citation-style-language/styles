<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.0" class="in-text" demote-non-dropping-particle="never" default-locale="pt-BR">
  <info>
    <title>Natureza &amp; Conservação (Brazilian Journal of Nature Conservation)</title>
    <id>http://www.zotero.org/styles/natureza-e-conservacao</id>
    <link href="http://www.zotero.org/styles/natureza-e-conservacao" rel="self"/>
    <link href="http://www.abecol.org.br/" rel="documentation"/>
    <author>
      <name>Mario José</name>
      <email>gnumario [at-mark] gmail [dot-mark] com</email>
    </author>
    <category citation-format="author-date"/>
    <category field="biology"/>
    <issn>1679-0073</issn>
    <issn>2178-3675</issn>
    <updated>2011-06-12T23:27:00+00:00</updated>
    <summary>Brazilian Journal of Nature Conservation</summary>
