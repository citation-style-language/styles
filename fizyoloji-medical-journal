<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="never" default-locale="en-US">
  <info>
    <title>Fizyoloji Medical Journal</title>
    <title-short>Fizyoloji Med J</title-short>
    <id>http://www.zotero.org/styles/fizyoloji-medical-journal</id>
    <link rel="self" href="http://www.zotero.org/styles/fizyoloji-medical-journal"/>
    <link href="https://www.fizyoloji.net/authorsguide" rel="documentation"/>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <summary>Style for Fizyoloji Medical Journal (Fizyoloji TÄ±p Dergisi)</summary>
    <updated>2024-07-24T18:16:51+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <names variable="author" suffix=".">
      <name delimiter=" &amp; " delimiter-precedes-last="always" et-al-min="3" et-al-use-first="1" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
    </names>
  </macro>
  <macro name="issued">
    <group prefix=" " suffix=".">
      <choose>
        <if type="article-journal article" match="any">
          <date date-parts="year" form="numeric" variable="issued"/>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="chapter-specifics">
    <choose>
      <if type="chapter paper-conference" match="any">
        <text term="in" text-case="capitalize-first" font-style="normal" prefix=". " suffix=": "/>
        <names variable="editor" delimiter=", " suffix=", ">
          <name delimiter=" &amp; " delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
          <label form="short" plural="never" prefix=" (" suffix=")"/>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="patent-specifics">
    <text variable="number" prefix=". "/>
  </macro>
  <macro name="container-title">
    <choose>
      <if type="bill book chapter graphic legal_case legislation motion_picture paper-conference report song" match="any">
        <text variable="container-title"/>
      </if>
      <else>
        <text variable="container-title" form="short" strip-periods="true" prefix=". " suffix=","/>
        <text variable="volume" prefix=" "/>
        <text variable="issue" prefix="(" suffix="): "/>
        <text variable="page"/>
      </else>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" " prefix=", ">
          <number variable="edition" form="ordinal"/>
          <text term="edition"/>
        </group>
      </if>
      <else>
        <text variable="edition" suffix="."/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <choose>
      <if type="book" match="any">
        <group delimiter=". " prefix=". ">
          <group delimiter=", ">
            <text variable="publisher"/>
            <date date-parts="year" form="text" variable="issued"/>
          </group>
        </group>
      </if>
      <else-if type="chapter" match="any">
        <group prefix=". ">
          <text variable="publisher"/>
          <date date-parts="year" form="numeric" variable="issued" prefix=", "/>
          <text variable="page" prefix=": "/>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="title">
    <group delimiter=" ">
      <text variable="title"/>
      <text variable="version" prefix="(" suffix=")"/>
    </group>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout prefix="(" suffix=")" delimiter=", ">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography entry-spacing="1" line-spacing="2" second-field-align="flush">
    <layout suffix=".">
      <text variable="citation-number" suffix=". "/>
      <text macro="author"/>
      <text macro="issued"/>
      <text macro="title" prefix=" "/>
      <text macro="chapter-specifics"/>
      <text macro="patent-specifics"/>
      <text macro="container-title"/>
      <text macro="edition"/>
      <text macro="publisher"/>
    </layout>
  </bibliography>
</style>
