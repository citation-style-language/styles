  </info>

  <locale xml:lang="pt">
    <terms>
      <term name="editor" form="short">
      </term>
      <term name="collection-editor" form="short">
        <single>ed.</single>
        <multiple>eds.</multiple>
      </term>
      <term name="accessed">acedido em</term>
      <term name="cited" form="short">atual.</term>
      <term name="cited">consultado a</term>
      <term name="in">in</term>
      <term name="director" form="short">
        <single>dir.</single>
        <multiple>dirs.</multiple>
      </term>
      <term name="page" form="short">p.</term>
      <term name="no date" form="short">s.d.</term>
    </terms>
  </locale>

  <macro name="author">
    <names variable="author">
      <name name-as-sort-order="all" sort-separator=", " initialize="false" initialize-with=". " delimiter="; " delimiter-precedes-last="always">
        <name-part name="family" text-case="uppercase"/>
        <name-part name="given"/>
      </name>
      <et-al font-style="italic"/>
      <label form="short" prefix=", "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text macro="title"/>
      </substitute>
    </names>
  </macro>

  <macro name="author-short">
    <names variable="author">
      <name form="short" name-as-sort-order="all" sort-separator=", " and="text" initialize="false" initialize-with=". " delimiter=", " delimiter-precedes-last="never"/>
      <et-al font-style="italic"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if type="bill book graphic legal_case legislation motion_picture song patent" match="any">
            <text variable="title" form="short" font-style="italic"/>
          </if>
          <else>
            <text variable="title" form="short" quotes="false" font-style="italic"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>

  <macro name="editor-org">
    <choose>
      <if type="chapter entry-dictionary entry-encyclopedia paper-conference" match="any">
        <names variable="editor" delimiter="; ">
          <label form="short" prefix="" suffix=" "/>
          <name delimiter="; " delimiter-precedes-last="always">
            <name-part name="given" text-case="capitalize-first"/>
            <name-part name="family" text-case="capitalize-first"/>
          </name>
        </names>
      </if>
    </choose>
  </macro>

  <macro name="title">
    <choose>
      <if type="book chapter entry-dictionary entry-encyclopedia" match="any">
        <text variable="title"/>
      </if>
      <else>
        <text variable="title" font-style="italic"/>
      </else>
    </choose>
  </macro>

  <macro name="container-title">
    <group>
      <choose>
        <if type="article-magazine article-newspaper article-journal chapter entry-dictionary paper-conference article bill legislation" match="any">
          <text variable="container-title" form="long" font-weight="normal" font-style="italic"/>
        </if>
        <else>
          <text variable="container-title" font-weight="normal" font-style="italic"/>
        </else>
      </choose>
    </group>
  </macro>

  <macro name="pages">
    <group delimiter=" ">
      <label plural="never" variable="page" form="short" text-case="lowercase"/>
      <text variable="page"/>
    </group>
  </macro>

  <macro name="issued-year">
    <choose>
      <if variable="issued" match="any">
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </if>
      <else>
        <text term="no date" form="short" prefix="[" suffix="]"/>
      </else>
    </choose>
  </macro>

  <macro name="collection">
    <group delimiter="; " prefix=" (" suffix=")">
      <text variable="collection-title"/>
      <text variable="collection-number"/>
    </group>
  </macro>

  <macro name="access">
    <group delimiter=". ">
      <group delimiter=": ">
        <text term="accessed" text-case="capitalize-first"/>
        <text variable="URL"/>
      </group>
      <group delimiter=": ">
        <text term="cited" text-case="capitalize-first"/>
        <date variable="accessed">
          <date-part name="day" suffix=" "/>
          <date-part name="month" form="short" suffix=". " text-case="lowercase"/>
          <date-part name="year"/>
        </date>
      </group>
    </group>
  </macro>

  <macro name="edition">
    <choose>
      <if type="book chapter" match="any">
        <choose>
          <if is-numeric="edition">
            <group delimiter=" ">
              <number variable="edition" form="ordinal" suffix="."/>
              <text term="edition" form="short" suffix="."/>
            </group>
          </if>
          <else>
            <text variable="edition"/>
          </else>
        </choose>
      </if>
    </choose>
  </macro>

  <macro name="citation-locator">
    <group delimiter=" ">
      <label variable="locator" form="short"/>
      <text variable="locator"/>
    </group>
  </macro>

  <macro name="locators">
    <choose>
      <if type="thesis" match="any">
        <text variable="page" prefix=". " suffix=" f."/>
      </if>
    </choose>
  </macro>

  <!-- CITAÇÃO -->
  <citation et-al-min="4" et-al-use-first="1" collapse="year">
    <sort>
      <key macro="author"/>
      <key variable="issued"/>
    </sort>
    <layout prefix="(" suffix=")" delimiter="; ">
      <group>
        <text suffix=", " macro="author-short"/>
        <text macro="issued-year"/>
        <text prefix=", " macro="citation-locator"/>
      </group>
    </layout>
  </citation>

  <bibliography hanging-indent="false" et-al-min="4" et-al-use-first="1" entry-spacing="0">
    <sort>
      <key macro="author"/>
      <key variable="issued"/>
    </sort>
    <layout>
      <choose>

        <if type="book">
          <group delimiter=". ">
            <group delimiter=" – ">
              <text macro="author"/>
              <text variable="title" font-style="italic"/>
            </group>
            <choose>
              <if variable="volume">
                <group delimiter=" ">
                  <label variable="volume" form="short" text-case="capitalize-first"/>
                  <text variable="volume"/>
                </group>
              </if>
            </choose>
            <text macro="edition"/>
            <choose>
              <if variable="number-of-volumes">
                <text variable="number-of-volumes"/>
              </if>
            </choose>
            <group delimiter=", ">
              <choose>
                <if variable="publisher-place">
                  <group delimiter=": ">
                    <text variable="publisher-place"/>
                    <choose>
                      <if variable="publisher">
                        <text variable="publisher"/>
                      </if>
                    </choose>
                  </group>
                </if>
              </choose>
              <date variable="issued">
                <date-part name="year"/>
              </date>
              <choose>
                <if variable="number-of-pages">
                  <group delimiter=" ">
                    <text variable="number-of-pages"/>
                    <label plural="never" variable="number-of-pages" form="short"/>
                  </group>
                </if>
              </choose>
            </group>
            <choose>
              <if match="any" variable="collection-title collection-number">
                <text macro="collection"/>
              </if>
            </choose>
            <text value=""/>
          </group>
        </if>

<else-if type="chapter entry-dictionary entry-encyclopedia" match="any">
  <group delimiter=". ">
    <group delimiter=" – ">
      <text macro="author"/>
      <text variable="title"/>
    </group>

    <group>
      <text term="in" text-case="capitalize-first" suffix=" "/>
      <choose>
        <if variable="container-author">
          <names variable="container-author">
            <name name-as-sort-order="all" sort-separator=", " delimiter=", " delimiter-precedes-last="always">
              <name-part name="family" text-case="uppercase"/>
              <name-part name="given"/>
            </name>
            <label form="short" prefix=", " suffix=""/>
          </names>
        </if>
        <else-if variable="event">
          <text variable="event"/>
          <choose>
            <if variable="event-place">
              <text variable="event-place" prefix=", "/>
            </if>
          </choose>
          <choose>
            <if variable="issued">
              <date variable="issued" prefix=", ">
                <date-part name="year"/>
              </date>
            </if>
          </choose>
        </else-if>
      </choose>
      <text value=" – "/>
      <text macro="container-title"/>
    </group>

    <choose>
      <if variable="volume">
        <text variable="volume"/>
      </if>
    </choose>

    <choose>
      <if variable="editor">
        <names variable="editor" delimiter="; ">
          <label form="short"/>
          <name delimiter="; " delimiter-precedes-last="always">
            <name-part name="given" text-case="capitalize-first"/>
            <name-part name="family" text-case="capitalize-first"/>
          </name>
        </names>
      </if>
    </choose>

    <group delimiter=", ">
      <choose>
        <if variable="publisher-place publisher" match="any">
          <group delimiter=": ">
            <choose>
              <if variable="publisher-place">
                <text variable="publisher-place"/>
              </if>
            </choose>
            <choose>
              <if variable="publisher">
                <text variable="publisher"/>
              </if>
            </choose>
          </group>
        </if>
      </choose>
      <date variable="issued">
        <date-part name="year"/>
      </date>
      <text macro="pages"/>
    </group>

    <choose>
      <if match="any" variable="collection-title collection-number">
        <text macro="collection"/>
      </if>
    </choose>

    <text value=" "/>
  </group>
</else-if>

        <else-if type="article-journal paper-conference">
          <group delimiter=". ">
            <group delimiter=" – ">
              <text macro="author"/>
              <text variable="title"/>
            </group>
            <text macro="container-title"/>
            <group delimiter=" ">
              <choose>
                <if variable="volume">
                  <choose>
                    <if variable="issue">
                      <group delimiter=":">
                        <text variable="volume"/>
                        <text variable="issue"/>
                      </group>
                    </if>
                    <else>
                      <text variable="volume"/>
                    </else>
                  </choose>
                </if>
                <else>
                  <choose>
                    <if variable="issue">
                      <text variable="issue"/>
                    </if>
                  </choose>
                </else>
              </choose>
              <group prefix="(" suffix=")">
                <date variable="issued">
                  <date-part name="year"/>
                </date>
              </group>
              <text variable="page"/>
            </group>
            <choose>
              <if variable="DOI">
                <text variable="DOI" prefix="doi: "/>
              </if>
            </choose>
            <text value=" "/>
          </group>
        </else-if>

        <else-if type="article-newspaper">
          <group delimiter=". ">
            <group delimiter=" – ">
              <text macro="author"/>
              <text variable="title"/>
            </group>
            <text macro="container-title"/>
            <group prefix="(" suffix=")">
              <date variable="issued">
                <date-part name="day" suffix=" "/>
                <date-part name="month" form="short" suffix=". " text-case="lowercase"/>
                <date-part name="year"/>
              </date>
            </group>
            <text variable="page"/>
            <text value=" "/>
          </group>
        </else-if>

        <else-if type="webpage post post-weblog" match="any">
          <group delimiter=". ">
            <group delimiter=" – ">
              <text macro="author"/>
              <text macro="title"/>
            </group>
            <choose>
              <if variable="URL">
                <group delimiter=": ">
                  <text term="accessed" text-case="capitalize-first"/>
                  <text variable="URL"/>
                </group>
              </if>
            </choose>
            <choose>
              <if variable="accessed">
                <group delimiter=" ">
                  <text term="cited" text-case="capitalize-first"/>
                  <group>
                    <date variable="accessed">
                      <date-part name="day"/>
                    </date>
                    <text value=" / "/>
                    <date variable="accessed">
                      <date-part name="month" form="numeric"/>
                    </date>
                    <text value=" / "/>
                    <date variable="accessed">
                      <date-part name="year"/>
                    </date>
                  </group>
                </group>
              </if>
            </choose>
            <text value=" "/>
          </group>
        </else-if>

        <else-if type="thesis">
          <group delimiter=". ">
            <group delimiter=" - ">
              <text macro="author"/>
              <text macro="title"/>
            </group>
            <text variable="collection-title"/>
            <group>
              <choose>
                <if variable="publisher-place">
                  <text variable="publisher-place" suffix=": "/>
                </if>
              </choose>
              <choose>
                <if variable="publisher">
                  <text variable="publisher"/>
                </if>
              </choose>
              <text macro="issued-year" prefix=", "/>
            </group>
            <text macro="locators"/>
            <group>
              <text variable="genre"/>
              <text value="."/>
            </group>
          </group>
        </else-if>
        <else-if type="manuscript">
          <group>
            <text macro="author" suffix=" - "/>
            <text macro="title"/>
            <text variable="collection-title" suffix=". "/>
            <text macro="locators"/>
            <text macro="access" prefix=" "/>
            <text variable="archive" prefix=" " suffix=". "/>
          </group>
        </else-if>

        <else-if type="webpage post post-weblog" match="any">
          <group delimiter=". ">
            <group delimiter=" - ">
              <text macro="author"/>
              <text macro="title"/>
            </group>
            <choose>
              <if variable="URL">
                <group delimiter=": ">
                  <text term="accessed" text-case="capitalize-first"/>
                  <text variable="URL"/>
                </group>
              </if>
            </choose>
            <choose>
              <if variable="accessed">
                <group delimiter=" ">
                  <text term="cited" text-case="capitalize-first"/>
                  <group>
                    <date variable="accessed">
                      <date-part name="day"/>
                    </date>
                    <text value=" / "/>
                    <date variable="accessed">
                      <date-part name="month" form="numeric"/>
                    </date>
                    <text value=" / "/>
                    <date variable="accessed">
                      <date-part name="year"/>
                    </date>
                  </group>
                </group>
              </if>
            </choose>
            <text value=" "/>
          </group>
        </else-if>
        <else-if type="report">
          <group>
            <text macro="author" suffix=" - "/>
            <text macro="title"/>
            <text variable="collection-title" suffix=". "/>
            <text macro="edition" prefix=". "/>
            <text macro="locators"/>
            <text macro="access" prefix=" "/>
          </group>
        </else-if>
        <else-if type="patent">
          <group>
            <text macro="title" suffix=". "/>
            <text variable="collection-title" suffix=". "/>
            <text macro="author" suffix=". "/>
            <text variable="publisher-place" suffix=" "/>
            <text variable="number" prefix="patente "/>
            <text macro="locators"/>
            <text macro="access" prefix=" "/>
          </group>
        </else-if>

        <else>
          <choose>
            <if variable="title">
              <group delimiter=". ">
                <group delimiter=" - ">
                  <text variable="archive"/>
                  <group delimiter=", ">
                    <choose>
                      <if variable="archive_location">
                        <text variable="archive_location"/>
                      </if>
                    </choose>
                    <choose>
                      <if variable="source">
                        <text variable="source"/>
                      </if>
                    </choose>
                  </group>
                </group>
                <group delimiter=", ">
                  <text macro="title"/>
                  <date variable="issued">
                    <date-part name="year"/>
                  </date>
                </group>
                <text value=" "/>
              </group>
            </if>
            <else>
              <group delimiter=". ">
                <text variable="archive"/>
                <group delimiter=", ">
                  <choose>
                    <if variable="archive_location">
                      <text variable="archive_location"/>
                    </if>
                  </choose>
                  <choose>
                    <if variable="source">
                      <text variable="source"/>
                    </if>
                  </choose>
                </group>
                <text value=""/>
              </group>
            </else>
          </choose>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>