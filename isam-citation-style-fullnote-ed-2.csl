  <macro name="secondary-contributors-nehir">
    <group delimiter=", " suffix="">
      <names variable="editor">
        <label form="verb-short" suffix=" "/>
        <name delimiter=" - "/>
      </names>
      <choose>
        <if variable="editor" match="any">
          <names variable="collection-editor">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="collection-editor">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
          </names>
        </else>
      </choose>
      <choose>
        <if variable="editor collection-editor" match="any">
          <names variable="translator">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="translator">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
          </names>
        </else>
      </choose>
      <choose>
        <if variable="editor collection-editor translator" match="any">
          <names variable="reviewed-author">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="reviewed-author">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
          </names>
        </else>
      </choose>
      <choose>
        <if variable="editor collection-editor translator reviewed-author" match="any">
          <names variable="editorial-director">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="editorial-director">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
          </names>
        </else>
      </choose>
      <choose>
        <if variable="editor collection-editor translator reviewed-author editorial-director" match="any">
          <names variable="compiler">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="compiler">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
          </names>
        </else>
      </choose>
      <choose>
        <if variable="editor collection-editor translator reviewed-author editorial-director compiler" match="any">
          <names variable="director">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="director">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
          </names>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="secondary-contributors">
    <choose>
      <if type="interview" variable="section page editor publisher-place publisher ISBN" match="all"/>
      <else-if type="book">
        <choose>
          <if match="none" variable="section">
            <group delimiter=", " suffix="">
              <names variable="editor">
                <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                <name delimiter=" - "/>
              </names>
              <choose>
                <if variable="editor" match="any">
                  <names variable="collection-editor">
                    <label form="verb-short" suffix=" "/>
                    <name delimiter=" - "/>
                  </names>
                </if>
                <else>
                  <names variable="collection-editor">
                    <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                    <name delimiter=" - "/>
                  </names>
                </else>
              </choose>
              <choose>
                <if variable="editor collection-editor" match="any">
                  <names variable="translator">
                    <label form="verb-short" suffix=" "/>
                    <name delimiter=" - "/>
                  </names>
                </if>
                <else>
                  <names variable="translator">
                    <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                    <name delimiter=" - "/>
                  </names>
                </else>
              </choose>
              <choose>
                <if variable="editor collection-editor translator" match="any">
                  <names variable="reviewed-author">
                    <label form="verb-short" suffix=" "/>
                    <name delimiter=" - "/>
                  </names>
                </if>
                <else>
                  <names variable="reviewed-author">
                    <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                    <name delimiter=" - "/>
                  </names>
                </else>
              </choose>
              <choose>
                <if variable="editor collection-editor translator reviewed-author" match="any">
                  <names variable="editorial-director">
                    <label form="verb-short" suffix=" "/>
                    <name delimiter=" - "/>
                  </names>
                </if>
                <else>
                  <names variable="editorial-director">
                    <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                    <name delimiter=" - "/>
                  </names>
                </else>
              </choose>
              <choose>
                <if variable="editor collection-editor translator reviewed-author editorial-director" match="any">
                  <names variable="compiler">
                    <label form="verb-short" suffix=" "/>
                    <name delimiter=" - "/>
                  </names>
                </if>
                <else>
                  <names variable="compiler">
                    <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                    <name delimiter=" - "/>
                  </names>
                </else>
              </choose>
              <choose>
                <if variable="editor collection-editor translator reviewed-author editorial-director compiler" match="any">
                  <names variable="director">
                    <label form="verb-short" suffix=" "/>
                    <name delimiter=" - "/>
                  </names>
                </if>
                <else>
                  <names variable="director">
                    <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                    <name delimiter=" - "/>
                  </names>
                </else>
              </choose>
            </group>
          </if>
        </choose>
      </else-if>
      <else-if type="chapter entry-dictionary entry-encyclopedia paper-conference motion_picture broadcast" match="none">
        <names variable="editor translator editorial-director collection-editor curator reviewed-author original-author director" delimiter=". " suffix="">
          <label form="verb-short" text-case="capitalize-first" suffix=" "/>
          <name delimiter=" - " initialize-with=". "/>
        </names>
      </else-if>
    </choose>
  </macro>
  <macro name="container-translator">
    <choose>
      <if type="chapter" match="any">
        <names variable="translator">
          <label form="verb-short" suffix=" "/>
          <name/>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="container-contributors">
    <choose>
      <if type="chapter" variable="section" match="all"/>
      <else-if type="chapter" variable="keyword" match="all"/>
      <else-if type="entry-encyclopedia" match="any">
        <group delimiter=", ">
          <choose>
            <if variable="container-author" match="any">
              <names variable="container-author">
                <name delimiter=" - "/>
              </names>
            </if>
          </choose>
          <choose>
            <if variable="container-author author" match="all">
              <group delimiter=". ">
                <names variable="editor translator" delimiter=", ">
                  <label form="verb" suffix=" "/>
                  <name and="text" delimiter=", "/>
                </names>
              </group>
            </if>
            <else>
              <names variable="editor collection-editor editorial-director reviewed-author original-author director">
                <label form="verb-short" text-case="lowercase" suffix=" "/>
                <name delimiter=" - "/>
              </names>
            </else>
          </choose>
        </group>
      </else-if>
      <else-if type="chapter entry-dictionary paper-conference" match="any">
        <group delimiter=", ">
          <choose>
            <if variable="author">
              <choose>
                <if variable="container-author" match="any">
                  <names variable="container-author">
                    <name delimiter=" - "/>
                  </names>
                </if>
              </choose>
              <choose>
                <if variable="container-author author" match="all">
                  <group delimiter=". ">
                    <names variable="editor translator" delimiter=", ">
                      <label form="verb" suffix=" "/>
                      <name and="text" delimiter=", "/>
                    </names>
                  </group>
                </if>
                <else>
                  <names variable="editor collection-editor editorial-director reviewed-author original-author director">
                    <label form="verb-short" text-case="lowercase" suffix=" "/>
                    <name delimiter=" - "/>
                  </names>
                </else>
              </choose>
            </if>
          </choose>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="recipient-note">
    <names variable="recipient" delimiter=", ">
      <label form="verb" text-case="lowercase" suffix=" "/>
      <name and="text" delimiter=", "/>
    </names>
  </macro>
  <macro name="contributors-note">
    <choose>
      <if type="book" variable="annote division" match="all"/>
      <else-if type="book" variable="division" match="all">
        <choose>
          <if is-numeric="division"/>
        </choose>
      </else-if>
      <else-if type="webpage" variable="division" match="all">
        <choose>
          <if is-numeric="division"/>
        </choose>
      </else-if>
      <else-if type="book" variable="note annote" match="all"/>
      <else-if type="book" variable="keyword" match="all">
        <choose>
          <if variable="host" match="any">
            <text variable="keyword"/>
          </if>
        </choose>
      </else-if>
      <else-if type="entry-encyclopedia" variable="URL" match="all">
        <choose>
          <if variable="author" match="any">
            <group delimiter=" ">
              <names variable="author">
                <name form="short" delimiter=" - " initialize-with=". "/>
                <label form="short" prefix=" (" suffix=")"/>
              </names>
            </group>
          </if>
        </choose>
      </else-if>
      <else-if type="report" variable="keyword" match="all">
        <text variable="keyword"/>
      </else-if>
      <else-if type="motion_picture" match="any">
        <names variable="contributor">
          <name form="short"/>
        </names>
      </else-if>
      <else-if type="graphic">
        <choose>
          <if variable="illustrator">
            <names variable="illustrator">
              <name form="short" delimiter=" - " initialize-with=". "/>
            </names>
          </if>
          <else-if variable="author" match="any">
            <choose>
              <if variable="illustrator" match="any"/>
              <else>
                <names variable="author">
                  <name form="short" delimiter=" - " initialize-with=". "/>
                  <label form="short" prefix=" (" suffix=")"/>
                </names>
              </else>
            </choose>
          </else-if>
          <else-if variable="author PMID" match="all">
            <names variable="author">
              <name form="short" delimiter=" - " initialize-with=". "/>
              <label form="short" prefix=" (" suffix=")"/>
            </names>
          </else-if>
          <else-if variable="script-writer" match="any">
            <names variable="author">
              <name form="short"/>
            </names>
          </else-if>
          <else-if variable="abstract" match="none">
            <names variable="contributor">
              <name form="short"/>
            </names>
          </else-if>
        </choose>
      </else-if>
      <else-if type="personal_communication" match="any">
        <names variable="author">
          <name form="short"/>
        </names>
      </else-if>
      <else-if type="document" variable="genre issued keyword" match="all">
        <text variable="keyword"/>
      </else-if>
      <else-if type="dataset">
        <text variable="division"/>
      </else-if>
      <else-if type="patent" variable="keyword" match="all">
        <text variable="keyword"/>
      </else-if>
      <else-if type="webpage" variable="note" match="all"/>
      <else>
        <group delimiter=" ">
          <names variable="author">
            <name form="short" delimiter=" - " initialize-with=". "/>
            <label form="short" prefix=" (" suffix=")"/>
            <substitute>
              <names variable="editor"/>
              <names variable="translator"/>
            </substitute>
          </names>
          <text macro="recipient-note"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="editor">
    <choose>
      <if type="entry-encyclopedia" match="none">
        <names variable="editor">
          <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
          <label form="short" prefix=" (" suffix=")"/>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="translator">
    <names variable="translator">
      <name name-as-sort-order="first" and="text" sort-separator=", " delimiter=", " delimiter-precedes-last="always"/>
      <label form="verb-short" prefix=", "/>
    </names>
  </macro>
  <macro name="recipient">
    <choose>
      <if type="personal_communication" variable="recipient" match="all"/>
      <else>
        <group delimiter=" ">
          <choose>
            <if type="personal_communication">
              <choose>
                <if variable="genre">
                  <text variable="genre" text-case="capitalize-first"/>
                </if>
                <else>
                  <text term="letter" text-case="capitalize-first"/>
                </else>
              </choose>
            </if>
          </choose>
          <text macro="recipient-note"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="contributors-anonymous">
    <group delimiter=". ">
      <choose>
        <if variable="author" match="none">
          <choose>
            <if variable="status" match="any">
              <text term="anonymous"/>
            </if>
          </choose>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="contributors">
    <group delimiter=". ">
      <choose>
        <if type="document" variable="genre" match="all"/>
        <else-if type="graphic">
          <choose>
            <if type="graphic" variable="issued genre publisher-place PMID part-number abstract" match="all">
              <text macro="title"/>
            </if>
            <else-if variable="PMID part-number" match="all">
              <group suffix=". ">
                <group delimiter=". ">
                  <text macro="title"/>
                </group>
                <names variable="author contributor">
                  <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="never"/>
                </names>
              </group>
            </else-if>
            <else>
              <group delimiter=". " suffix=". ">
                <names variable="author contributor">
                  <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="never"/>
                </names>
              </group>
            </else>
          </choose>
        </else-if>
        <else-if type="book" variable="annote division" match="all">
          <names variable="author">
            <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
            <substitute>
              <choose>
                <if type="entry-encyclopedia" match="none">
                  <text macro="editor"/>
                </if>
              </choose>
              <text macro="translator"/>
              <choose>
                <if type="article-magazine article-newspaper webpage post-weblog" match="any">
                  <text variable="container-title"/>
                </if>
              </choose>
            </substitute>
          </names>
        </else-if>
        <else-if type="book" variable="note annote" match="all"/>
        <else-if type="motion_picture"/>
        <else-if type="webpage" variable="division" match="all">
          <choose>
            <if is-numeric="division"/>
          </choose>
        </else-if>
        <else-if type="entry-encyclopedia">
          <names variable="author">
            <name name-as-sort-order="first" and="text" sort-separator=", " delimiter=", " delimiter-precedes-last="always"/>
            <substitute>
              <text macro="editor"/>
            </substitute>
          </names>
        </else-if>
        <else-if type="graphic">
          <group delimiter=". " suffix=". ">
            <names variable="author contributor">
              <name name-as-sort-order="first" and="text" sort-separator=", " delimiter=", " delimiter-precedes-last="always"/>
            </names>
          </group>
        </else-if>
        <else-if type="chapter song map legal_case patent thesis interview article-newspaper report personal_communication graphic software legislation" match="none">
          <group delimiter=". ">
            <names variable="author">
              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
              <substitute>
                <choose>
                  <if type="entry-encyclopedia" match="none">
                    <text macro="editor"/>
                  </if>
                </choose>
                <text macro="translator"/>
                <choose>
                  <if type="article-magazine article-newspaper webpage post-weblog" match="any">
                    <text variable="container-title"/>
                  </if>
                </choose>
              </substitute>
            </names>
          </group>
        </else-if>
      </choose>
      <text macro="recipient"/>
    </group>
  </macro>
  <macro name="contributors-sort">
    <choose>
      <if type="motion_picture software legislation">
        <choose>
          <if variable="author" match="none">
            <text variable="title"/>
          </if>
        </choose>
      </if>
      <else-if variable="host keyword" match="all">
        <choose>
          <if variable="status medium" match="none">
            <text variable="keyword"/>
            <names variable="author contributor editor">
              <name name-as-sort-order="all" and="text" sort-separator=", " delimiter=", " delimiter-precedes-last="always"/>
              <substitute>
                <names variable="contributor"/>
                <names variable="editor"/>
                <names variable="translator"/>
                <text macro="title"/>
              </substitute>
            </names>
          </if>
        </choose>
      </else-if>
      <else-if type="book" variable="section interviewer author editor" match="all">
        <names variable="author contributor editor">
          <name name-as-sort-order="all" and="text" sort-separator=", " delimiter=", " delimiter-precedes-last="always"/>
          <substitute>
            <names variable="contributor"/>
            <names variable="editor"/>
            <names variable="translator"/>
            <text macro="title"/>
          </substitute>
        </names>
      </else-if>
      <else-if type="book" variable="note annote" match="all">
        <text variable="title"/>
      </else-if>
      <else-if type="book" variable="note section" match="all">
        <text variable="section"/>
      </else-if>
      <else-if type="webpage" variable="note annote" match="all">
        <text variable="title"/>
      </else-if>
      <else-if type="entry-dictionary">
        <choose>
          <if variable="author" match="none">
            <text variable="publisher"/>
          </if>
        </choose>
      </else-if>
      <else-if type="entry-encyclopedia">
        <choose>
          <if variable="author" match="none">
            <text variable="title"/>
          </if>
        </choose>
      </else-if>
      <else-if type="dataset report">
        <choose>
          <if variable="author" match="none">
            <text variable="publisher"/>
          </if>
        </choose>
      </else-if>
      <else-if type="legal_case">
        <choose>
          <if variable="author" match="none">
            <text variable="authority"/>
          </if>
        </choose>
      </else-if>
      <else-if type="graphic" variable="issued genre publisher-place PMID part-number abstract" match="all">
        <text variable="title"/>
      </else-if>
      <else-if type="graphic" variable="issued collection-title collection-number author PMID part-number" match="all">
        <text variable="title"/>
      </else-if>
      <else-if type="document" variable="jurisdiction annote" match="all">
        <text variable="title"/>
      </else-if>
      <else-if variable="division PMID" match="all">
        <text variable="title"/>
      </else-if>
    </choose>
    <names variable="author contributor editor">
      <name name-as-sort-order="all" and="text" sort-separator=", " delimiter=", " delimiter-precedes-last="always"/>
      <substitute>
        <names variable="contributor"/>
        <names variable="editor"/>
        <names variable="translator"/>
        <text macro="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="interviewer">
    <names variable="interviewer" delimiter=" - ">
      <label form="verb" text-case="capitalize-first" suffix=": "/>
      <name delimiter=" - "/>
    </names>
  </macro>
  <macro name="title-note">
    <choose>
      <if variable="title" match="none">
        <text variable="genre"/>
      </if>
      <else-if type="dataset">
        <text variable="title" quotes="true" prefix="Aranan: " form="short"/>
      </else-if>
      <else-if type="document" variable="jurisdiction" match="all">
        <text variable="title" text-case="title" quotes="false" form="short"/>
      </else-if>
      <else-if type="document" variable="medium number publisher-place" match="all">
        <text variable="title" text-case="title" quotes="false" font-style="italic" form="short"/>
        <group prefix=" (" suffix=")">
          <text variable="publisher" suffix=", "/>
          <group delimiter=".">
            <date variable="issued" form="numeric">
              <date-part name="day" form="numeric-leading-zeros"/>
              <date-part name="month"/>
              <date-part name="year"/>
            </date>
          </group>
        </group>
      </else-if>
      <else-if type="document" variable="genre issued" match="all">
        <text variable="title" text-case="title" quotes="true"/>
        <group prefix=" (" suffix=")">
          <text variable="genre" suffix=", "/>
          <date variable="issued" form="text"/>
        </group>
      </else-if>
      <else-if type="book" variable="annote" match="all">
        <choose>
          <if variable="annote note PMCID" match="all">
            <text variable="title" text-case="title" form="short" prefix="1" suffix="2"/>
          </if>
          <else-if variable="annote division" match="all">
            <group delimiter=", ">
              <text variable="title" text-case="title" form="short" font-style="italic"/>
              <text variable="keyword"/>
            </group>
          </else-if>
          <else-if variable="division" match="all">
            <choose>
              <if is-numeric="division">
                <text variable="title" text-case="title" font-style="italic" form="short"/>
              </if>
            </choose>
          </else-if>
          <else-if variable="annote jurisdiction" match="all">
            <text variable="title" text-case="title" form="short" font-style="italic"/>
          </else-if>
          <else-if variable="note annote">
            <text variable="title" text-case="title" form="short" font-style="italic"/>
          </else-if>
          <else-if variable="annote"/>
          <else>
            <text variable="title" text-case="title" form="short"/>
          </else>
        </choose>
      </else-if>
      <else-if type="book" variable="annote" match="all">
        <choose>
          <if variable="annote note PMCID" match="all">
            <text variable="title" text-case="title" form="short"/>
          </if>
          <else-if variable="annote note" match="all">
            <text variable="title" text-case="title" font-style="italic" form="short"/>
          </else-if>
          <else-if variable="annote jurisdiction" match="all">
            <text variable="title" text-case="title" form="short" font-style="italic"/>
          </else-if>
          <else-if variable="annote division" match="all">
            <group delimiter=", ">
              <text variable="title" text-case="title" form="short" font-style="italic"/>
              <text variable="keyword"/>
            </group>
          </else-if>
          <else-if variable="annote"/>
          <else>
            <text variable="title" text-case="title" form="short"/>
          </else>
        </choose>
      </else-if>
      <else-if type="book manuscript map" match="any">
        <text variable="title" text-case="title" font-style="italic" form="short"/>
        <group delimiter=" " prefix=", ">
          <text term="version"/>
          <text variable="version"/>
        </group>
      </else-if>
      <else-if type="thesis" match="any">
        <text variable="title" text-case="title" quotes="true" form="short"/>
      </else-if>
      <else-if type="entry-dictionary" match="any">
        <text variable="title" form="short" font-style="italic"/>
      </else-if>
      <else-if type="article-journal paper-conference" match="any">
        <text variable="title" quotes="true" form="short"/>
      </else-if>
      <else-if type="interview" match="any">
        <text variable="title" quotes="true" form="short"/>
      </else-if>
      <else-if variable="reviewed-author">
        <text variable="title" font-style="italic" prefix="review of "/>
      </else-if>
      <else-if type="report">
        <text variable="title" font-style="italic" form="short"/>
      </else-if>
      <else-if type="webpage">
        <choose>
          <if variable="division" match="any">
            <choose>
              <if is-numeric="division">
                <text variable="title" font-style="italic" form="short"/>
              </if>
            </choose>
          </if>
          <else>
            <text variable="title" quotes="true" form="short"/>
          </else>
        </choose>
      </else-if>
      <else-if type="legislation" match="any">
        <text variable="title" text-case="title" form="short"/>
      </else-if>
      <else-if type="motion_picture" variable="annote" match="all">
        <text variable="title" text-case="title" form="short"/>
      </else-if>
      <else-if type="motion_picture broadcast song software patent" match="any">
        <text variable="title" text-case="title" font-style="italic" form="short"/>
      </else-if>
      <else-if type="personal_communication" match="any">
        <choose>
          <if variable="note" match="any">
            <text variable="title" text-case="title" quotes="true" form="short"/>
          </if>
          <else>
            <text variable="title" text-case="title" font-style="italic" form="short"/>
          </else>
        </choose>
      </else-if>
      <else-if type="graphic" match="any">
        <choose>
          <if variable="part-title" match="any">
            <group delimiter=", ">
              <text variable="title" text-case="title" form="short"/>
              <text variable="part-title" font-style="italic"/>
            </group>
          </if>
          <else-if variable="collection-title genre PMID" match="any">
            <text variable="title" text-case="title" form="short"/>
          </else-if>
          <else-if variable="PMCID" match="any">
            <text variable="title" text-case="title" font-style="italic" form="short"/>
          </else-if>
          <else>
            <group delimiter=", ">
              <text variable="title" text-case="title" font-style="italic" form="short"/>
            </group>
          </else>
        </choose>
      </else-if>
      <else-if type="legal_case" match="any">
        <text variable="title" text-case="title"/>
      </else-if>
      <else-if type="chapter">
        <text variable="title" font-style="italic" quotes="false"/>
      </else-if>
      <else>
        <text variable="title" quotes="true"/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if type="chapter" variable="section" match="all"/>
      <else-if variable="title" match="none">
        <choose>
          <if type="personal_communication" match="none">
            <text variable="genre" text-case="capitalize-first"/>
          </if>
        </choose>
      </else-if>
      <else-if type="dataset">
        <text variable="title" quotes="true" prefix="Aranan: "/>
      </else-if>
      <else-if type="song" match="any"/>
      <else-if type="map" match="any"/>
      <else-if type="software" match="any"/>
      <else-if type="legislation" match="any"/>
      <else-if type="motion_picture" match="any"/>
      <else-if type="document" variable="genre" match="all"/>
      <else-if type="book manuscript standard" match="any">
        <text variable="title" text-case="title" font-style="italic"/>
        <choose>
          <if variable="section" match="any">
            <text macro="interviewer" prefix=" (" suffix=")"/>
            <text macro="secondary-contributors-nehir" prefix=", "/>
            <group delimiter=":" prefix=", ">
              <text variable="volume"/>
              <text variable="page"/>
            </group>
          </if>
        </choose>
      </else-if>
      <else-if variable="reviewed-author">
        <group delimiter=", ">
          <text variable="title" font-style="italic" prefix="Review of "/>
          <names variable="reviewed-author">
            <label form="verb-short" text-case="lowercase" suffix=" "/>
            <name and="text" delimiter=", "/>
          </names>
        </group>
      </else-if>
      <else-if type="bill" match="any">
        <text variable="title"/>
      </else-if>
      <else-if type="chapter" variable="interviewer genre" match="all">
        <group delimiter=". ">
          <text variable="title" quotes="true"/>
          <text macro="description"/>
        </group>
      </else-if>
      <else-if type="chapter" variable="interviewer" match="all">
        <group delimiter=" ">
          <text variable="title" quotes="true"/>
          <text macro="description"/>
        </group>
      </else-if>
      <else-if type="chapter" variable="keyword" match="all">
        <group delimiter=" ">
          <text variable="title" quotes="false" font-style="italic" suffix="."/>
        </group>
      </else-if>
      <else>
        <choose>
          <if type="thesis" match="any"/>
          <else-if type="patent" match="any"/>
          <else-if type="webpage">
            <choose>
              <if variable="division PMID" match="all">
                <group delimiter=". ">
                  <text variable="title" font-style="italic"/>
                  <names variable="author">
                    <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
                    <substitute>
                      <text macro="editor"/>
                      <text macro="translator"/>
                    </substitute>
                  </names>
                </group>
              </if>
              <else>
                <text variable="title" quotes="true"/>
              </else>
            </choose>
          </else-if>
          <else-if type="entry-dictionary" match="any">
            <choose>
              <if variable="author" match="none">
                <text variable="title" text-case="title" font-style="italic" suffix=". "/>
              </if>
              <else>
                <text variable="title" text-case="title" font-style="italic"/>
              </else>
            </choose>
          </else-if>
          <else-if type="article-journal article-newspaper" match="any">
            <text variable="title" text-case="title" quotes="true" suffix="."/>
          </else-if>
          <else-if type="entry-encyclopedia paper-conference" match="any">
            <text variable="title" text-case="title" quotes="true"/>
          </else-if>
          <else-if type="graphic" match="any">
            <text variable="title" text-case="title"/>
          </else-if>
          <else>
            <text variable="title" text-case="title" font-style="italic"/>
          </else>
        </choose>
      </else>
    </choose>
  </macro>
  <macro name="description-note">
    <choose>
      <if type="motion_picture" match="any">
        <group delimiter=", ">
          <text variable="part-number"/>
          <text variable="part-title"/>
          <names variable="director" prefix="yön. "/>
        </group>
      </if>
      <else-if type="broadcast" match="any">
        <text variable="number" prefix="Bölüm " suffix=", "/>
        <text variable="container-title" quotes="true" suffix=", "/>
        <group delimiter=". " prefix="sun. ">
          <names variable="director">
            <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always" form="short"/>
          </names>
        </group>
        <choose>
          <if variable="division">
            <text variable="division"/>
          </if>
        </choose>
      </else-if>
      <else-if type="graphic" variable="issued genre publisher-place" match="all">
        <names variable="contributor"/>
      </else-if>
      <else-if type="song graphic" match="any"/>
      <else-if type="personal_communication" variable="genre" match="all"/>
      <else-if type="document" variable="medium number publisher-place" match="all"/>
      <else-if type="document" variable="genre issued" match="all"/>
      <else-if type="software" match="all">
        <text variable="version"/>
      </else-if>
      <else-if type="article-journal entry-dictionary entry-encyclopedia map report" variable="medium" match="all"/>
      <else-if type="webpage" match="any"/>
      <else-if type="chapter" variable="interviewer" match="all"/>
      <else>
        <group delimiter=", ">
          <text variable="medium"/>
          <choose>
            <if variable="title" match="none"/>
            <else-if type="thesis" match="any"/>
            <else-if type="manuscript speech" match="any"/>
            <else-if type="book" variable="genre" match="all"/>
            <else-if type="legal_case">
              <group delimiter=", ">
                <text variable="number"/>
              </group>
            </else-if>
            <else>
              <text variable="genre"/>
            </else>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="description">
    <group delimiter=", ">
      <choose>
        <if variable="title" match="none"/>
        <else-if type="thesis speech" match="any"/>
        <else-if type="report" match="any"/>
        <else-if type="motion_picture" match="any"/>
        <else-if type="personal_communication" match="any"/>
        <else-if type="document" match="any"/>
        <else-if type="graphic" match="any"/>
        <else-if type="map" match="any"/>
        <else-if type="software" match="any"/>
        <else-if variable="annote genre keyword medium note source status" match="all"/>
        <else-if type="webpage" match="any"/>
        <else-if type="manuscript" match="any"/>
        <else-if type="chapter" variable="interviewer genre" match="all">
          <text macro="interviewer" prefix="" suffix=""/>
        </else-if>
        <else-if type="chapter" variable="interviewer" match="all">
          <text macro="interviewer" prefix="(" suffix=")"/>
        </else-if>
        <else>
          <text variable="genre" text-case="capitalize-first"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="container-title-note">
    <group delimiter=" ">
      <choose>
        <if type="entry-dictionary" match="any">
          <text variable="container-title" quotes="true"/>
        </if>
        <else-if type="chapter entry-encyclopedia paper-conference" match="any"></else-if>
        <else-if type="webpage">
          <text variable="container-title"/>
        </else-if>
        <else-if type="post-weblog">
          <text variable="container-title" text-case="title" font-style="italic" suffix=" (blog)"/>
        </else-if>
        <else-if type="bill" match="any"/>
        <else>
          <text variable="container-title" text-case="title" font-style="italic"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="container-title">
    <group delimiter=" ">
      <choose>
        <if type="entry-encyclopedia paper-conference" match="any">
          <text variable="container-title" text-case="title" font-style="italic"/>
        </if>
        <else-if type="chapter" variable="section" match="all"/>
        <else-if type="chapter" variable="keyword annote" match="all">
          <text variable="container-title" font-style="italic"/>
          <text term="in"/>
        </else-if>
        <else-if type="chapter" variable="keyword" match="all">
          <text variable="container-title" font-style="italic"/>
          <text term="in"/>
        </else-if>
        <else-if type="webpage" match="any">
          <choose>
            <if variable="note" match="any">
              <text variable="container-title" text-case="title" font-style="italic"/>
            </if>
            <else>
              <text variable="container-title" text-case="title" quotes="true"/>
            </else>
          </choose>
        </else-if>
        <else-if type="post-weblog">
          <text variable="container-title" text-case="title" font-style="italic" suffix=" (blog)"/>
        </else-if>
        <else-if type="legislation" match="any"/>
        <else-if type="broadcast" match="any"/>
        <else-if type="bill" match="any"/>
        <else-if type="entry-dictionary" match="any"/>
        <else>
          <text variable="container-title" text-case="title" font-style="italic"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="collection-title">
    <choose>
      <if type="graphic map software motion_picture report" match="none">
        <choose>
          <if match="none" type="article-journal">
            <choose>
              <if match="none" is-numeric="collection-number">
                <group delimiter=", ">
                  <text variable="collection-title" text-case="title"/>
                  <text variable="collection-number"/>
                </group>
              </if>
              <else>
                <group delimiter=" ">
                  <text variable="collection-title" text-case="title"/>
                  <text variable="collection-number"/>
                </group>
              </else>
            </choose>
          </if>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="collection-title-journal">
    <choose>
      <if type="article-journal">
        <group delimiter=" ">
          <text variable="collection-title"/>
          <text variable="collection-number"/>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="edition-note">
    <choose>
      <if type="book chapter paper-conference report" match="any">
        <choose>
          <if is-numeric="edition">
            <group delimiter=" ">
              <number variable="edition" form="ordinal"/>
              <text term="edition"/>
            </group>
          </if>
          <else>
            <text variable="edition"/>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if type="entry-encyclopedia entry-dictionary book chapter paper-conference report" match="any">
        <choose>
          <if is-numeric="edition">
            <group delimiter=" ">
              <number variable="edition" suffix="."/>
              <text term="edition"/>
            </group>
          </if>
          <else>
            <choose>
              <if variable="number-of-volumes">
                <text variable="edition" suffix="."/>
              </if>
              <else>
                <text variable="edition" text-case="capitalize-first" suffix="."/>
              </else>
            </choose>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="locators-note-join-with-space">
    <choose>
      <if type="article-journal" variable="volume" match="all">
        <choose>
          <if match="none" variable="collection-title">
            <text macro="locators-note"/>
          </if>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="locators-note">
    <choose>
      <if type="article-journal">
        <group delimiter=", ">
          <text macro="collection-title-journal"/>
        </group>
      </if>
      <else-if type="bill" match="any">
        <text macro="legal-cites"/>
      </else-if>
      <else-if type="book chapter" match="any"></else-if>
      <else-if type="paper-conference" match="any"/>
      <else-if type="report" match="any">
        <group delimiter=", ">
          <text macro="edition-note"/>
          <group delimiter=" ">
            <text term="volume" form="short"/>
            <number variable="volume" form="numeric"/>
          </group>
          <choose>
            <if variable="locator" match="none">
              <group delimiter=" ">
                <number variable="number-of-volumes" form="numeric"/>
                <text term="volume" form="short" plural="true"/>
              </group>
            </if>
          </choose>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="legal-cites">
    <choose>
      <if type="bill" match="any">
        <group delimiter=", ">
          <choose>
            <if variable="number">
              <text variable="number" prefix="Pub. L. No. "/>
              <group delimiter=" ">
                <text term="section" form="symbol"/>
                <text variable="section"/>
              </group>
              <group delimiter=" ">
                <text variable="volume"/>
                <text variable="container-title"/>
                <text variable="page-first"/>
              </group>
            </if>
            <else>
              <group delimiter=" ">
                <text variable="volume"/>
                <text variable="container-title"/>
                <text term="section" form="symbol"/>
                <text variable="section"/>
              </group>
            </else>
          </choose>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="locators-join-with-space">
    <choose>
      <if type="article-journal" variable="issue volume" match="all">
        <choose>
          <if match="none" variable="collection-title">
            <text macro="locators"/>
          </if>
        </choose>
      </if>
      <else-if type="article-journal" variable="issue" match="all">
        <choose>
          <if match="none" variable="collection-title">
            <text macro="locators"/>
          </if>
        </choose>
      </else-if>
    </choose>
  </macro>
  <macro name="locators-join-with-comma">
    <choose>
      <if type="bill chapter paper-conference" match="any">
        <text macro="locators"/>
      </if>
      <else-if type="article-journal">
        <choose>
          <if variable="issue volume" match="none">
            <text macro="locators"/>
          </if>
          <else-if match="any" variable="collection-title">
            <text macro="locators"/>
          </else-if>
        </choose>
      </else-if>
    </choose>
  </macro>
  <macro name="locators-join-with-period">
    <choose>
      <if type="bill article-journal chapter paper-conference entry-encyclopedia" match="none">
        <text macro="locators"/>
      </if>
    </choose>
  </macro>
  <macro name="locators">
    <choose>
      <if type="article-journal">
        <group delimiter=", ">
          <text macro="collection-title-journal"/>
          <text variable="volume"/>
          <group delimiter=" ">
            <text variable="issue"/>
          </group>
        </group>
      </if>
      <else-if type="bill" match="any">
        <text macro="legal-cites"/>
      </else-if>
      <else-if type="book report" match="any">
        <group delimiter=", ">
          <choose>
            <if type="book"/>
            <else>
              <group delimiter=" ">
                <number variable="volume" form="numeric" suffix="."/>
                <text term="volume"/>
              </group>
            </else>
          </choose>
          <group delimiter=" ">
            <number variable="number-of-volumes" form="numeric"/>
            <text term="volume"/>
          </group>
          <text macro="edition"/>
        </group>
      </else-if>
      <else-if type="entry-dictionary" match="any">
        <group delimiter=", ">
          <group delimiter=" ">
            <number variable="number-of-volumes" form="numeric"/>
            <text term="volume"/>
          </group>
          <text macro="edition"/>
        </group>
      </else-if>
      <else-if type="entry-dictionary entry-encyclopedia paper-conference" match="any">
        <group delimiter=". ">
          <text macro="edition"/>
          <choose>
            <if variable="page" match="none">
              <group delimiter=" ">
                <text term="volume" form="short" text-case="capitalize-first"/>
                <number variable="volume" form="numeric"/>
              </group>
            </if>
          </choose>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="event-note">
    <text variable="event"/>
  </macro>
  <macro name="event">
    <choose>
      <if variable="title">
        <group delimiter=" ">
          <choose>
            <if variable="genre">
              <text term="presented at"/>
            </if>
            <else>
              <text term="presented at" text-case="capitalize-first"/>
            </else>
          </choose>
          <text variable="event"/>
        </group>
      </if>
      <else>
        <group delimiter=" ">
          <text term="presented at" text-case="capitalize-first"/>
          <text variable="event"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="originally-published">
    <group delimiter=", ">
      <group delimiter=": ">
        <text variable="original-publisher-place"/>
        <text variable="original-publisher"/>
      </group>
      <choose>
