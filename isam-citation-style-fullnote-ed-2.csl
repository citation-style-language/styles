                            <group delimiter=". ">
                              <group delimiter=". ">
                                <text macro="contributors-anonymous"/>
                                <choose>
                                  <if type="article-newspaper" match="any"/>
                                  <else-if type="report" match="any"/>
                                  <else-if type="interview" match="any"/>
                                  <else-if type="personal_communication" match="any"/>
                                </choose>
                                <choose>
                                  <if type="chapter" variable="keyword" match="all"/>
                                  <else-if type="book" variable="status medium" match="all">
                                    <group>
                                      <names variable="author" prefix="[" suffix="] ">
                                        <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="never"/>
                                      </names>
                                      <names variable="contributor" prefix="" suffix="">
                                        <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
                                      </names>
                                    </group>
                                  </else-if>
                                  <else-if variable="division PMID"/>
                                  <else-if type="book" variable="status"/>
                                  <else>
                                    <text macro="contributors"/>
                                  </else>
                                </choose>
                                <choose>
                                  <if type="book">
                                    <text macro="title"/>
                                  </if>
                                  <else-if type="article-newspaper" match="any"/>
                                  <else-if type="report" match="any"/>
                                  <else-if type="webpage" match="any"/>
                                  <else-if type="interview" match="any"/>
                                  <else-if type="personal_communication" match="any"/>
                                  <else-if type="manuscript" match="any">
                                    <text macro="title"/>
                                  </else-if>
                                  <else-if variable="author" match="none"/>
                                  <else-if type="chapter" variable="keyword" match="all"/>
                                  <else>
                                    <text macro="title"/>
                                  </else>
                                </choose>
                                <choose>
                                  <if type="book" variable="note annote" match="all">
                                    <group delimiter=". ">
                                      <names variable="author">
                                        <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
                                      </names>
                                    </group>
                                  </if>
                                  <else-if variable="division PMID" match="all">
                                    <text macro="contributors"/>
                                  </else-if>
                                </choose>
                              </group>
                              <choose>
                                <if type="chapter" variable="interviewer" match="all"/>
                                <else>
                                  <text macro="description"/>
                                </else>
                              </choose>
                              <text macro="secondary-contributors"/>
                              <choose>
                                <if type="chapter" variable="keyword" match="none">
                                  <text macro="container-translator"/>
                                </if>
                              </choose>
                              <group delimiter=". ">
                                <choose>
                                  <if type="webpage" match="all">
                                    <text macro="title"/>
                                    <choose>
                                      <if variable="note annote" match="all">
                                        <group delimiter=". ">
                                          <names variable="author">
                                            <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
                                          </names>
                                        </group>
                                      </if>
                                    </choose>
                                    <text macro="container-contributors"/>
                                  </if>
                                </choose>
                              </group>
                              <group>
                                <choose>
                                  <if type="article-journal" variable="interviewer" match="all">
                                    <text macro="interviewer" prefix="" suffix=""/>
                                  </if>
                                </choose>
                              </group>
                              <group delimiter=", ">
                                <choose>
                                  <if type="chapter" variable="keyword" match="all"/>
                                  <else-if type="chapter" match="any">
                                    <choose>
                                      <if variable="translator">
                                        <text macro="container-title"/>
                                        <text macro="container-contributors"/>
                                      </if>
                                      <else-if variable="status">
                                        <text macro="container-title"/>
                                        <text macro="container-contributors"/>
                                      </else-if>
                                      <else-if variable="interviewer">
                                        <text macro="container-title"/>
                                        <text macro="container-contributors"/>
                                      </else-if>
                                      <else-if variable="keyword">
                                        <text macro="container-title"/>
                                        <text macro="container-contributors"/>
                                      </else-if>
                                      <else>
                                        <text macro="container-contributors"/>
                                        <text macro="container-title"/>
                                      </else>
                                    </choose>
                                  </else-if>
                                  <else-if type="paper-conference entry-encyclopedia" match="any">
                                    <choose>
                                      <if variable="author editor" match="any">
                                        <text macro="container-title"/>
                                        <choose>
                                          <if variable="edition" match="any">
                                            <group delimiter=". ">
                                              <text variable="edition"/>
                                              <choose>
                                                <if is-numeric="edition" match="any">
                                                  <text term="edition"/>
                                                </if>
                                              </choose>
                                            </group>
                                          </if>
                                          <else-if variable="editor" match="all">
                                            <choose>
                                              <if type="paper-conference entry-encyclopedia" match="none">
                                                <text macro="editor"/>
                                              </if>
                                            </choose>
                                          </else-if>
                                        </choose>
                                        <text macro="container-contributors"/>
                                      </if>
                                    </choose>
                                  </else-if>
                                  <else-if type="article-newspaper" match="any"/>
                                  <else-if type="webpage legal_case" match="none">
                                    <text macro="container-contributors"/>
                                    <text macro="container-title"/>
                                  </else-if>
                                </choose>
                                <choose>
                                  <if type="book" match="any">
                                    <text term="collection-editor" text-case="lowercase"/>
                                  </if>
                                  <else-if type="chapter" variable="keyword" match="all"/>
                                  <else-if type="chapter" variable="section" match="all"/>
                                  <else>
                                    <text term="collection-editor"/>
                                    <names variable="collection-editor">
                                      <label form="verb-short" text-case="lowercase" suffix=". "/>
                                      <name delimiter=" - "/>
                                    </names>
                                  </else>
                                </choose>
                                <choose>
                                  <if type="book article-journal entry-encyclopedia entry-dictionary legislation motion_picture legal_case paper-conference chapter interview" match="none">
                                    <text variable="volume" suffix=""/>
                                  </if>
                                </choose>
                              </group>
                              <text macro="locators-join-with-period"/>
                            </group>
                            <text macro="locators-join-with-comma"/>
                            <choose>
                              <if type="chapter" variable="keyword" match="all"/>
                              <else-if type="chapter" variable="section" match="all"/>
                              <else>
                                <text macro="locators-chapter"/>
                              </else>
                            </choose>
                          </group>
                          <text macro="locators-join-with-space"/>
                        </group>
                        <choose>
                          <if type="entry-encyclopedia" match="any">
                            <choose>
                              <if variable="locator" match="none">
                                <group delimiter=" ">
                                  <number variable="number-of-volumes" form="numeric"/>
                                  <text term="volume" form="verb" plural="false"/>
                                </group>
                              </if>
                            </choose>
                          </if>
                        </choose>
                        <text macro="collection-title"/>
                        <choose>
                          <if type="chapter" variable="keyword" match="all"/>
                          <else-if type="chapter" variable="section" match="all"/>
                          <else>
                            <text macro="issue-join-with-period"/>
                          </else>
                        </choose>
                      </group>
                      <text macro="issue-join-with-space"/>
                    </group>
                    <choose>
                      <if type="article-newspaper" match="none"></if>
                    </choose>
                  </group>
                  <text macro="locators-journal-join-with-colon"/>
                </group>
                <choose>
                  <if type="legislation report motion_picture" match="none">
                    <text macro="access"/>
                  </if>
                </choose>
              </group>
            </else>
          </choose>
          <choose>
            <if type="graphic" match="all"/>
            <else-if type="manuscript" match="all"></else-if>
            <else-if type="interview" variable="status" match="all"/>
            <else-if type="article-newspaper document chapter" match="any">
              <text value=""/>
            </else-if>
            <else-if type="report"></else-if>
            <else-if type="thesis"/>
            <else-if type="motion_picture">
              <text value=""/>
            </else-if>
            <else-if type="personal_communication" variable="annote" match="all">
              <choose>
                <if variable="section" match="none">
                  <text value=""/>
                </if>
                <else>
                  <text value="."/>
                </else>
              </choose>
            </else-if>
            <else>
              <choose>
                <if variable="accessed URL DOI" match="any">
                  <text value=""/>
                </if>
                <else>
                  <text value="."/>
                </else>
              </choose>
            </else>
          </choose>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>
