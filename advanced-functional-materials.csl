<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0">
  <info>
    <title>Advanced Functional Materials</title>
    <id>http://www.zotero.org/styles/advanced-functional-materials</id>
    <link href="http://www.zotero.org/styles/advanced-functional-materials" rel="self">
    </link>
    <link href="http://www.zotero.org/styles/american-chemical-society" rel="template">
    </link>
    <author>
      <name>Julian Onions</name>
      <email>julian.onions@gmail.com</email>
    </author>
    <contributor>
      <name>Ivan Bushmarinov</name>
      <email>ib@ineos.ac.ru</email>
    </contributor>
    <category citation-format="numeric">
    </category>
    <category field="chemistry">
    </category>
    <updated>2012-07-11T00:09:44+00:00</updated>
    <summary>Style for the Wiley Journal Advanced Functional Materials</summary>
    <link href="http://onlinelibrary.wiley.com/store/10.1002/%28ISSN%291616-3028/asset/homepages/2126_guide_for_authors_2009_2.pdf" rel="documentation">
    </link>
    <rights>This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License: http://creativecommons.org/licenses/by-sa/3.0/</rights>
    <issn>1616-3028</issn>
  </info>
  <macro name="editor">
    <names variable="editor">
      <name sort-separator=", " initialize-with=". " name-as-sort-order="all" delimiter="; " delimiter-precedes-last="always">
      </name>
      <label form="short" prefix=", " text-case="capitalize-first" suffix="." strip-periods="true">
      </label>
    </names>
  </macro>
  <macro name="author">
    <names variable="author">
      <name delimiter="m " delimiter-precedes-last="always" initialize-with=". ">
      </name>
      <label form="short" prefix=", " suffix="." text-case="capitalize-first" strip-periods="true">
      </label>
    </names>
  </macro>
  <macro name="access">
    <text variable="URL">
    </text>
  </macro>
  <macro name="publisher">
    <group delimiter=": ">
      <text variable="publisher">
      </text>
      <text variable="publisher-place">
      </text>
    </group>
  </macro>
  <macro name="volume">
    <group delimiter=" ">
      <text term="volume" form="short" suffix="." text-case="capitalize-first" strip-periods="true">
      </text>
      <text variable="volume">
      </text>
    </group>
  </macro>
  <macro name="series">
    <text variable="collection-title">
    </text>
  </macro>
  <macro name="pages">
    <label variable="page" form="short" suffix=". " strip-periods="true">
    </label>
    <text variable="page">
    </text>
  </macro>
  <macro name="book-container">
    <group delimiter=" ">
      <text term="in" text-case="capitalize-first">
      </text>
      <text variable="container-title" font-style="italic">
      </text>
    </group>
  </macro>
  <macro name="issued">
    <date variable="issued" delimiter=" ">
      <date-part name="year">
      </date-part>
    </date>
  </macro>
  <macro name="full-issued">
    <date variable="issued" delimiter=" ">
      <date-part name="month" form="long" suffix=" ">
      </date-part>
      <date-part name="day" suffix=", ">
      </date-part>
      <date-part name="year">
      </date-part>
    </date>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number variable="edition" form="ordinal">
	  </number>
          <text term="edition" form="short" suffix="." strip-periods="true">
	  </text>
        </group>
      </if>
      <else>
        <text variable="edition" suffix=".">
	</text>
      </else>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number">
      </key>
    </sort>
    <layout delimiter="," vertical-align="sup" prefix="[" suffix="]">
      <text variable="citation-number">
      </text>
    </layout>
  </citation>
  <bibliography second-field-align="margin" entry-spacing="0">
    <layout suffix=".">
      <text variable="citation-number" prefix="[" suffix="] ">
      </text>
      <text macro="author" suffix=", ">
      </text>
      <choose>
        <if type="article-magazine">
          <group delimiter=" ">
            <text variable="container-title" font-style="italic" suffix=".">
	    </text>
            <text macro="edition">
	    </text>
            <text macro="publisher">
	    </text>
            <text macro="full-issued" suffix=",">
	    </text>
            <text macro="pages">
	    </text>
          </group>
        </if>
        <else-if type="thesis">
          <group delimiter=", ">
            <group delimiter=". ">
              <text variable="title">
	      </text>
              <text variable="genre">
	      </text>
            </group>
            <text macro="publisher">
	    </text>
            <text macro="issued">
	    </text>
            <text macro="volume">
	    </text>
            <text macro="pages">
	    </text>
          </group>
        </else-if>
        <else-if type="bill book graphic legal_case motion_picture report song" match="any">
          <group delimiter="; ">
            <text variable="title" font-style="italic">
	    </text>
            <text macro="editor" prefix=" ">
	    </text>
            <text macro="series">
	    </text>
            <text macro="edition">
	    </text>
            <group delimiter=", ">
              <text macro="publisher">
	      </text>
              <text macro="issued">
	      </text>
            </group>
            <group delimiter=", ">
              <text macro="volume">
	      </text>
              <text macro="pages">
	      </text>
            </group>
          </group>
        </else-if>
        <else-if type="chapter paper-conference" match="any">
          <group delimiter="; ">
            <text macro="book-container">
	    </text>
            <text macro="editor">
	    </text>
            <text macro="series">
	    </text>
            <group delimiter=", ">
              <text macro="publisher">
	      </text>
              <text macro="issued">
	      </text>
            </group>
            <group delimiter=", ">
              <text macro="volume">
	      </text>
              <text macro="pages">
	      </text>
            </group>
          </group>
        </else-if>
        <else-if type="article-journal">
          <group delimiter=" ">
            <text variable="container-title" font-style="italic" form="short">
	    </text>
            <group delimiter=", ">
              <text macro="issued" font-weight="bold">
	      </text>
              <text variable="volume" font-style="italic">
	      </text>
              <text variable="page-first" form="long">
	      </text>
            </group>
          </group>
        </else-if>
        <else>
          <group delimiter=" ">
            <group delimiter=". ">
              <text variable="title">
	      </text>
              <text variable="container-title" font-style="italic" form="short">
	      </text>
            </group>
            <group delimiter=", ">
              <text macro="issued" font-weight="bold">
	      </text>
              <text variable="volume" font-style="italic">
	      </text>
              <text variable="page">
	      </text>
            </group>
          </group>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>
