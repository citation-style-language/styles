    <id>http://www.zotero.org/styles/journal-of-community-health</id>
    <link href="http://www.zotero.org/styles/journal-of-community-health" rel="self"/>
    <link href="http://www.springer.com/public+health/journal/10900" rel="documentation"/>
    <author>
