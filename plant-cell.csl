<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="sort-only">
   <info>
      <title>Plant Cell</title>
      <id>http://www.zotero.org/styles/plant-cell</id>
      <link href="http://www.zotero.org/styles/plant-cell" rel="self"/>
      <author>
         <name>Sebastian Karcher</name>
      </author>
      <contributor>
         <name>Richard Karnesky</name>
         <email>karnesky+zotero@gmail.com</email>
         <uri>http://arc.nucapt.northwestern.edu/Richard_Karnesky</uri>
      </contributor>
      <contributor>
         <name>Gal Haspel</name>
         <email>gal.haspel@gmail.com</email>
      </contributor>
      <category citation-format="author-date"/>
      <category field="biology"/>
      <updated>2011-04-03T04:35:15+00:00</updated>
      <summary>Plant Cell</summary>
      <link href="http://www.plantcell.org/site/misc/ifora.xhtml" rel="documentation"/>
