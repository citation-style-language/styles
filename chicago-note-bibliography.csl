  </info>
  <locale xml:lang="en">
    <terms>
      <term form="short" name="chapter-number">chap.</term>
      <term form="verb-short" name="collection-editor">ed.</term>
      <term form="verb-short" name="contributor">with</term>
      <term form="verb-short" name="editor">ed.</term>
      <term form="verb" name="editortranslator">edited and translated by</term>
      <term form="verb-short" name="editortranslator">ed. and trans.</term>
      <term form="short" name="personal-communication">pers. comm.</term>
      <term name="post-weblog">blog</term>
      <term form="short" name="translator">trans.</term>
      <term form="verb-short" name="translator">trans.</term>
    </terms>
  </locale>
  <macro name="recipient">
    <names variable="recipient">
      <label form="verb" suffix=" "/>
      <name/>
    </names>
  </macro>
  <macro name="communication-recipient-note">
    <group delimiter=" ">
      <choose>
        <if type="personal_communication">
          <choose>
            <if variable="genre">
              <text variable="genre"/>
            </if>
            <else>
              <text form="short" term="personal-communication"/>
            </else>
          </choose>
        </if>
      </choose>
      <text macro="recipient"/>
    </group>
  </macro>
  <macro name="communication-recipient">
    <group delimiter=" ">
      <choose>
        <if type="personal_communication">
          <choose>
            <if variable="genre">
              <text text-case="capitalize-first" variable="genre"/>
            </if>
            <else>
              <text term="personal-communication" text-case="capitalize-first"/>
            </else>
          </choose>
        </if>
      </choose>
      <text macro="recipient"/>
    </group>
  </macro>
  <macro name="substitute-title">
    <choose>
      <if match="any" type="article-magazine article-newspaper review review-book">
        <text macro="container-title"/>
      </if>
      <else-if match="any" type="post-weblog webpage">
        <text macro="container-title"/>
      </else-if>
    </choose>
  </macro>
  <macro name="contributors-short">
    <group delimiter=" ">
      <names variable="composer">
        <name form="short"/>
        <substitute>
          <names variable="author"/>
          <names variable="editor-translator"/>
          <names variable="editor"/>
          <names variable="translator"/>
          <names variable="director"/>
          <names variable="compiler"/>
          <names variable="host"/>
          <names variable="collection-editor"/>
          <text macro="substitute-title"/>
          <text macro="title-short"/>
        </substitute>
      </names>
      <choose>
        <if match="all" type="personal_communication" variable="author recipient">
          <names variable="recipient">
            <label form="verb-short" suffix=" "/>
            <name form="short"/>
          </names>
        </if>
        <else-if type="personal_communication">
          <text macro="communication-recipient-note"/>
        </else-if>
      </choose>
    </group>
  </macro>
  <macro name="contributors">
    <group delimiter=". ">
      <names variable="composer">
        <name delimiter-precedes-last="always" name-as-sort-order="first"/>
        <label form="short" prefix=", "/>
        <substitute>
          <names variable="author"/>
          <names variable="editor-translator"/>
          <names variable="editor"/>
          <names variable="translator"/>
          <names variable="director"/>
          <names variable="compiler"/>
          <names variable="host"/>
          <names variable="collection-editor"/>
          <text macro="substitute-title"/>
          <text macro="title"/>
        </substitute>
      </names>
      <text macro="communication-recipient"/>
    </group>
  </macro>
  <macro name="reviewed-author">
    <names variable="reviewed-author">
      <label form="verb-short" suffix=" "/>
      <name/>
    </names>
  </macro>
  <macro name="title-short">
    <choose>
      <if type="interview">
        <text term="interview"/>
      </if>
      <else-if match="none" variable="title">
        <choose>
          <if match="any" type="manuscript speech">
            <text form="short" variable="genre"/>
          </if>
        </choose>
      </else-if>
      <else-if match="any" type="book graphic motion_picture pamphlet song">
        <text font-style="italic" form="short" text-case="title" variable="title"/>
      </else-if>
      <else-if type="legal_case" variable="title-short">
        <text font-style="italic" form="short" variable="title"/>
      </else-if>
      <else-if match="any" type="patent">
        <text form="short" variable="title"/>
      </else-if>
      <else-if match="any" type="bill legal_case legislation">
        <text variable="title"/>
      </else-if>
      <else-if type="post">
        <text form="short" quotes="true" variable="title"/>
      </else-if>
      <else>
        <text form="short" quotes="true" text-case="title" variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <group delimiter=" ">
      <choose>
        <if match="none" variable="title">
          <choose>
            <if match="none" type="personal_communication speech thesis">
              <text text-case="capitalize-first" variable="genre"/>
            </if>
          </choose>
        </if>
        <else-if match="any" type="bill book graphic legislation motion_picture pamphlet song">
          <group delimiter=" ">
            <text font-style="italic" text-case="title" variable="title"/>
            <group delimiter=" " prefix="(" suffix=")">
              <text term="version"/>
              <text variable="version"/>
            </group>
          </group>
        </else-if>
        <else-if variable="reviewed-author">
          <choose>
            <if variable="reviewed-title">
              <group delimiter=". ">
                <text quotes="true" text-case="title" variable="title"/>
                <group delimiter=", ">
                  <text font-style="italic" prefix="Review of " text-case="title" variable="reviewed-title"/>
                  <text macro="reviewed-author"/>
                </group>
              </group>
            </if>
            <else>
              <group delimiter=", ">
                <text font-style="italic" prefix="Review of " text-case="title" variable="title"/>
                <text macro="reviewed-author"/>
              </group>
            </else>
          </choose>
        </else-if>
        <else-if match="any" type="legal_case patent">
          <text variable="title"/>
        </else-if>
        <else-if type="post">
          <text quotes="true" variable="title"/>
        </else-if>
        <else>
          <text quotes="true" text-case="title" variable="title"/>
        </else>
      </choose>
      <text prefix="[" suffix="]" text-case="title" variable="original-title"/>
    </group>
  </macro>
  <macro name="date-disambiguate">
    <choose>
      <if disambiguate="true" match="any" type="personal_communication">
        <text macro="issued"/>
      </if>
    </choose>
  </macro>
  <macro name="interviewer">
    <names variable="interviewer">
      <label form="verb" suffix=" " text-case="capitalize-first"/>
      <name/>
    </names>
  </macro>
  <macro name="scale-dimensions">
    <choose>
      <if type="map">
        <text variable="scale"/>
      </if>
      <else-if type="graphic">
        <text variable="dimensions"/>
      </else-if>
    </choose>
  </macro>
  <macro name="genre-number">
    <choose>
      <if match="none" type="personal_communication speech thesis">
        <group delimiter=" ">
          <text text-case="capitalize-first" variable="genre"/>
          <choose>
            <if type="report">
              <text variable="number"/>
            </if>
          </choose>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="description">
    <group delimiter=". ">
      <choose>
        <if match="any" type="interview" variable="interviewer">
          <text macro="interviewer"/>
        </if>
        <else-if type="patent">
          <group delimiter=" ">
            <text variable="authority"/>
            <text variable="number"/>
          </group>
        </else-if>
      </choose>
      <text macro="scale-dimensions"/>
      <text macro="genre-number"/>
    </group>
  </macro>
  <macro name="secondary-contributors">
    <choose>
      <if match="none" type="chapter entry-dictionary entry-encyclopedia paper-conference">
        <group delimiter=". ">
          <choose>
            <if match="any" variable="volume volume-title">
              <names variable="collection-editor">
                <label form="verb" suffix=" " text-case="capitalize-first"/>
                <name/>
              </names>
            </if>
            <else>
              <names variable="editor translator" delimiter=". ">
                <label form="verb" suffix=" " text-case="capitalize-first"/>
                <name/>
              </names>
            </else>
          </choose>
          <names variable="director">
            <label form="verb" suffix=" " text-case="capitalize-first"/>
            <name/>
          </names>
          <names variable="contributor">
            <label form="verb" suffix=" " text-case="capitalize-first"/>
            <name/>
          </names>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="container-title-main">
    <choose>
      <if match="any" type="post webpage">
        <text text-case="title" variable="container-title"/>
      </if>
      <else-if type="post-weblog">
        <group delimiter=" ">
          <text font-style="italic" text-case="title" variable="container-title"/>
          <text prefix="(" suffix=")" term="post-weblog"/>
        </group>
      </else-if>
      <else-if match="none" type="bill legal_case legislation">
        <text font-style="italic" text-case="title" variable="container-title"/>
      </else-if>
    </choose>
  </macro>
  <macro name="container-volume-title">
    <group delimiter=", ">
      <group delimiter=" ">
        <text form="short" term="volume"/>
        <number form="numeric" variable="volume"/>
      </group>
      <text font-style="italic" text-case="title" variable="volume-title"/>
    </group>
  </macro>
  <macro name="container-title">
    <group delimiter=", ">
      <group delimiter=" ">
        <choose>
          <if match="any" type="chapter entry-dictionary entry-encyclopedia paper-conference">
            <choose>
              <if variable="chapter-number">
                <label form="short" text-case="capitalize-first" variable="chapter-number"/>
                <text variable="chapter-number"/>
                <text term="in"/>
              </if>
              <else>
                <text term="in" text-case="capitalize-first"/>
              </else>
            </choose>
          </if>
        </choose>
        <text macro="container-title-main"/>
      </group>
      <choose>
        <if match="any" type="chapter entry-dictionary entry-encyclopedia paper-conference">
          <text macro="container-volume-title"/>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="container-contributors">
    <choose>
      <if match="any" type="chapter entry-dictionary entry-encyclopedia paper-conference">
        <group delimiter=", ">
          <names variable="container-author">
            <label form="verb" suffix=" "/>
            <name/>
          </names>
          <names variable="editor translator" delimiter=". ">
            <label form="verb" suffix=" "/>
            <name/>
          </names>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if match="none" type="article-newspaper">
        <choose>
          <if is-numeric="edition">
            <group delimiter=" ">
              <number form="ordinal" variable="edition"/>
              <text form="short" term="edition"/>
            </group>
          </if>
          <else>
            <text text-case="capitalize-first" variable="edition"/>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="locators-journal">
    <group delimiter=", ">
      <text variable="collection-title"/>
      <text variable="volume"/>
      <group delimiter=" ">
        <text form="short" term="issue"/>
        <text variable="issue"/>
      </group>
    </group>
  </macro>
  <macro name="collection-title">
    <choose>
      <if match="none" type="article-journal">
        <choose>
          <if is-numeric="collection-number" match="none">
            <group delimiter=", ">
              <text text-case="title" variable="collection-title"/>
              <text variable="collection-number"/>
            </group>
          </if>
          <else>
            <group delimiter=" ">
              <text text-case="title" variable="collection-title"/>
              <text variable="collection-number"/>
            </group>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="number-of-volumes">
    <choose>
      <if match="none" variable="volume volume-title">
        <group delimiter=" ">
          <number form="numeric" variable="number-of-volumes"/>
          <text form="short" plural="true" term="volume"/>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="legal-cites">
    <choose>
      <if type="legal_case">
        <group delimiter=" ">
          <choose>
            <if variable="container-title">
              <text variable="volume"/>
              <text variable="container-title"/>
              <group delimiter=" ">
                <label form="symbol" variable="section"/>
                <text variable="section"/>
              </group>
              <group delimiter=", ">
                <text variable="page"/>
                <choose>
                  <if locator="page">
                    <text variable="locator"/>
                  </if>
                  <else>
                    <group delimiter=" ">
                      <label form="short" variable="locator"/>
                      <text variable="locator"/>
                    </group>
                  </else>
                </choose>
              </group>
            </if>
            <else>
              <group delimiter=" ">
                <label variable="number"/>
                <text variable="number"/>
              </group>
            </else>
          </choose>
        </group>
      </if>
      <else-if match="any" type="bill legislation">
        <group delimiter=", ">
          <choose>
            <if variable="number">
              <text variable="number"/>
              <text variable="authority"/>
              <group delimiter=" ">
                <label form="symbol" variable="section"/>
                <text variable="section"/>
              </group>
              <group delimiter=" ">
                <text variable="volume"/>
                <text variable="container-title"/>
                <text variable="page-first"/>
              </group>
            </if>
            <else>
              <group delimiter=" ">
                <text variable="volume"/>
                <text variable="container-title"/>
                <label form="symbol" variable="section"/>
                <text variable="section"/>
              </group>
            </else>
          </choose>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="locators">
    <group delimiter=". ">
      <choose>
        <if type="article-journal">
          <text macro="locators-journal"/>
        </if>
        <else-if match="any" type="bill legal_case legislation">
          <text macro="legal-cites"/>
        </else-if>
        <else-if match="any" type="book graphic motion_picture pamphlet report song">
          <group delimiter=". ">
            <group delimiter=", ">
              <group delimiter=" ">
                <text form="short" term="volume" text-case="capitalize-first"/>
                <number form="numeric" variable="volume"/>
              </group>
              <text font-style="italic" text-case="title" variable="volume-title"/>
              <choose>
                <if variable="volume-title">
                  <names variable="editor translator">
                    <label form="verb" suffix=" "/>
                    <name/>
                  </names>
                </if>
              </choose>
            </group>
            <text macro="edition"/>
            <text macro="number-of-volumes"/>
          </group>
        </else-if>
        <else-if match="any" type="chapter entry-dictionary entry-encyclopedia paper-conference">
          <group delimiter=". ">
            <text macro="edition"/>
            <choose>
              <if match="none" variable="page">
                <group delimiter=" ">
                  <text form="short" term="volume" text-case="capitalize-first"/>
                  <number form="numeric" variable="volume"/>
                </group>
              </if>
            </choose>
          </group>
        </else-if>
      </choose>
      <group delimiter=", ">
        <text macro="collection-title"/>
        <choose>
          <if match="none" variable="volume volume-title">
            <names variable="collection-editor">
              <label form="verb" suffix=" "/>
              <name/>
            </names>
          </if>
        </choose>
      </group>
    </group>
  </macro>
  <macro name="locators-join-with-comma">
    <choose>
      <if match="any" type="bill chapter legal_case legislation paper-conference">
        <text macro="locators"/>
      </if>
      <else-if type="article-journal">
        <choose>
          <if match="none" variable="volume">
            <text macro="locators"/>
          </if>
          <else-if variable="collection-title">
            <text macro="locators"/>
          </else-if>
        </choose>
      </else-if>
    </choose>
  </macro>
  <macro name="locators-join-with-period">
    <choose>
      <if match="none" type="article-journal bill chapter legal_case legislation paper-conference">
        <text macro="locators"/>
      </if>
    </choose>
  </macro>
  <macro name="locators-join-with-space">
    <choose>
      <if type="article-journal" variable="volume">
        <choose>
          <if match="none" variable="collection-title">
            <text macro="locators"/>
          </if>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="issued">
    <choose>
      <if variable="issued">
        <choose>
          <if type="legal_case">
            <group delimiter=" ">
              <text variable="authority"/>
              <choose>
                <if variable="container-title">
                  <date date-parts="year" form="numeric" variable="issued"/>
                </if>
                <else>
                  <date form="text" variable="issued"/>
                </else>
              </choose>
            </group>
          </if>
          <else-if type="article-journal">
            <choose>
              <if match="none" variable="volume">
                <date date-parts="year-month" form="text" variable="issued"/>
              </if>
              <else>
                <date date-parts="year" form="text" variable="issued"/>
              </else>
            </choose>
          </else-if>
          <else-if match="any" type="article-journal book bill chapter legislation motion_picture pamphlet paper-conference song thesis">
            <choose>
              <if is-uncertain-date="issued">
                <date date-parts="year" form="numeric" prefix="[" suffix="?]" variable="issued"/>
              </if>
              <else>
                <date date-parts="year" form="numeric" variable="issued"/>
              </else>
            </choose>
          </else-if>
          <else-if type="patent">
            <group delimiter=", ">
              <group delimiter=" ">
                <text value="filed"/>
                <date form="text" variable="submitted"/>
              </group>
              <group delimiter=" ">
                <choose>
                  <if variable="issued submitted">
                    <text term="and"/>
                  </if>
                </choose>
                <text value="issued"/>
                <date form="text" variable="issued"/>
              </group>
            </group>
          </else-if>
          <else>
            <choose>
              <if is-uncertain-date="issued">
                <date form="text" prefix="[" suffix="?]" variable="issued"/>
              </if>
              <else>
                <date form="text" variable="issued"/>
              </else>
            </choose>
          </else>
        </choose>
      </if>
      <else-if variable="status">
        <text variable="status"/>
      </else-if>
      <else-if match="none" variable="accessed URL">
        <text form="short" term="no date"/>
      </else-if>
    </choose>
  </macro>
  <macro name="event">
    <group delimiter=" ">
      <choose>
        <if variable="genre">
          <text term="presented at"/>
        </if>
        <else>
          <text term="presented at" text-case="capitalize-first"/>
        </else>
      </choose>
      <text variable="event"/>
    </group>
  </macro>
  <macro name="originally-published">
    <group delimiter=", ">
      <group delimiter=": ">
        <!-- <text variable="original-publisher-place"/> -->
        <text text-case="title" variable="original-publisher"/>
      </group>
      <choose>
        <if is-uncertain-date="original-date">
          <date date-parts="year" form="numeric" prefix="[" suffix="?]" variable="original-date"/>
        </if>
        <else>
          <date date-parts="year" form="numeric" variable="original-date"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="reprint">
    <choose>
      <if variable="original-date issued">
        <text text-case="capitalize-first" value="reprint"/>
      </if>
    </choose>
  </macro>
  <macro name="publisher">
    <choose>
      <if type="thesis">
        <text text-case="title" variable="publisher"/>
      </if>
      <else-if type="speech">
        <text variable="event-place"/>
      </else-if>
      <else>
        <group delimiter=": ">
          <!-- <text variable="publisher-place"/> -->
          <text text-case="title" variable="publisher"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="issue-map-graphic">
    <choose>
      <if match="any" type="graphic map">
        <choose>
          <if match="none" variable="publisher publisher-place">
            <text macro="issued"/>
          </if>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="issue">
    <choose>
      <if match="any" type="bill legal_case legislation">
        <text macro="issued" prefix="(" suffix=")"/>
      </if>
      <else-if type="article-journal">
        <choose>
          <if match="any" variable="issue volume">
            <text macro="issued" prefix="(" suffix=")"/>
          </if>
          <else>
            <text macro="issued"/>
          </else>
        </choose>
      </else-if>
      <else-if type="speech">
        <group delimiter=", ">
          <group delimiter=" ">
            <choose>
              <if match="none" variable="title"/>
              <else>
                <text text-case="capitalize-first" variable="genre"/>
              </else>
            </choose>
            <text macro="event"/>
          </group>
          <text variable="event-place"/>
          <text macro="issued"/>
        </group>
      </else-if>
      <else-if match="any" type="article article-newspaper interview">
        <group delimiter=", ">
          <choose>
            <if type="article">
              <text term="preprint" text-case="capitalize-first"/>
            </if>
          </choose>
          <choose>
            <if match="none" type="article-newspaper">
              <text text-case="title" variable="publisher"/>
            </if>
          </choose>
          <text macro="issued"/>
        </group>
      </else-if>
      <else-if match="any" variable="publisher publisher-place">
        <group delimiter=", ">
          <choose>
            <if type="thesis">
              <text text-case="capitalize-first" variable="genre"/>
            </if>
          </choose>
          <group delimiter=". ">
            <text macro="originally-published"/>
            <group delimiter=", ">
              <text macro="reprint"/>
              <text macro="publisher"/>
            </group>
          </group>
          <text macro="issued"/>
        </group>
      </else-if>
      <else-if match="none" type="graphic map">
        <text macro="issued"/>
      </else-if>
    </choose>
  </macro>
  <macro name="issue-join-with-comma">
    <choose>
      <if match="none" type="bill legal_case legislation">
        <choose>
          <if match="none" type="article-journal">
            <choose>
              <if match="any" type="article-magazine article-newspaper interview">
                <choose>
                  <if match="any" variable="author editor">
                    <text macro="issue"/>
                  </if>
                </choose>
              </if>
              <else-if match="none" type="speech" variable="publisher publisher-place">
                <text macro="issue"/>
              </else-if>
            </choose>
          </if>
          <else-if match="none" variable="volume issue">
            <text macro="issue"/>
          </else-if>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="issue-join-with-period">
    <choose>
      <if match="none" type="article-journal bill interview legal_case legislation">
        <choose>
          <if match="any" type="article-magazine article-newspaper">
            <choose>
              <if match="none" variable="author editor">
                <text macro="issue"/>
              </if>
            </choose>
          </if>
          <else-if match="any" type="speech" variable="publisher publisher-place">
            <text macro="issue"/>
          </else-if>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="issue-join-with-space">
    <choose>
      <if type="article-journal">
        <choose>
          <if match="any" variable="issue volume">
            <text macro="issue"/>
          </if>
        </choose>
      </if>
      <else-if match="any" type="bill legal_case legislation">
        <text macro="issue"/>
      </else-if>
    </choose>
  </macro>
  <macro name="locator-case-subsequent">
    <choose>
      <if type="legal_case">
        <group delimiter=" ">
          <text variable="volume"/>
          <text variable="container-title"/>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="point-locator-case-subsequent">
    <choose>
      <if type="legal_case">
        <group delimiter=" ">
          <choose>
            <if locator="page">
              <text term="at"/>
              <text variable="locator"/>
            </if>
            <else>
              <label variable="locator"/>
              <text variable="locator"/>
            </else>
          </choose>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="point-locators-journal-join-with-colon">
    <choose>
      <if type="article-journal">
        <choose>
          <if match="any" variable="volume issue">
            <text variable="page"/>
          </if>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="point-locators-journal-join-with-comma">
    <choose>
      <if type="article-journal">
        <choose>
          <if match="none" variable="volume issue">
            <text variable="page"/>
          </if>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="point-locators-newspaper">
    <choose>
      <if type="article-newspaper">
        <group delimiter=", ">
          <group delimiter=" ">
            <number variable="edition"/>
            <text term="edition"/>
          </group>
          <group delimiter=" ">
            <text form="short" term="section"/>
            <text variable="section"/>
          </group>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="point-locators-subsequent">
    <choose>
      <if type="legal_case" variable="locator">
        <choose>
          <if locator="page">
            <group delimiter=":">
              <text variable="volume"/>
              <text variable="locator"/>
            </group>
          </if>
          <else>
            <group delimiter=" ">
              <label form="short" variable="locator"/>
              <text variable="locator"/>
            </group>
          </else>
        </choose>
      </if>
      <else-if variable="locator">
        <choose>
          <if locator="page" match="none">
            <group delimiter=" ">
              <choose>
                <if match="any" type="book graphic motion_picture pamphlet report song">
                  <choose>
                    <if variable="volume">
                      <group delimiter=", ">
                        <group delimiter=" ">
                          <text form="short" term="volume"/>
                          <number form="numeric" variable="volume"/>
                        </group>
                        <label form="short" variable="locator"/>
                      </group>
                    </if>
                    <else>
                      <label form="short" variable="locator"/>
                    </else>
                  </choose>
                </if>
                <else>
                  <label form="short" variable="locator"/>
                </else>
              </choose>
              <text variable="locator"/>
            </group>
          </if>
          <else-if match="any" type="book graphic motion_picture pamphlet report song">
            <group delimiter=":">
              <number form="numeric" variable="volume"/>
              <text variable="locator"/>
            </group>
          </else-if>
          <else>
            <text variable="locator"/>
          </else>
        </choose>
      </else-if>
    </choose>
  </macro>
  <macro name="archive">
    <choose>
      <if type="thesis">
        <group delimiter=" ">
          <text variable="archive"/>
          <text prefix="(" suffix=")" variable="archive_location"/>
        </group>
      </if>
      <else>
        <group delimiter=". ">
          <text text-case="capitalize-first" variable="archive_location"/>
          <text variable="archive"/>
          <text variable="archive-place"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="URI">
    <choose>
      <if match="none" type="legal_case">
        <choose>
          <if variable="DOI">
            <text prefix="https://doi.org/" variable="DOI"/>
          </if>
          <else>
            <text variable="URL"/>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="access">
    <group delimiter=". ">
      <group delimiter=", ">
        <text text-case="capitalize-first" variable="medium"/>
        <choose>
          <if match="none" type="graphic">
            <text variable="dimensions"/>
          </if>
        </choose>
      </group>
      <choose>
        <if variable="archive-place">
          <text macro="archive"/>
        </if>
      </choose>
      <choose>
        <if match="none" variable="issued">
          <group delimiter=" ">
            <text term="accessed" text-case="capitalize-first"/>
            <date form="text" variable="accessed"/>
          </group>
        </if>
      </choose>
      <choose>
        <if match="any" variable="DOI URL">
          <text macro="URI"/>
        </if>
        <else-if match="none" variable="archive-place">
          <text macro="archive"/>
        </else-if>
      </choose>
    </group>
  </macro>
