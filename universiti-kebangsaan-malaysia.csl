</else-if>
<else-if match="any" type="bill graphic legal_case legislation motion_picture song">
<text form="short" font-style="italic" variable="title"/>
</else-if>
<else>
<text value="Anon"/>
</else>
</choose>
<text macro="issued-year"/>
<text macro="citation-locator"/>
</group>
</layout>
</citation>
