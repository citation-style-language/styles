<macro name="container-contributors">
<choose>
<if match="any" type="chapter paper-conference">
<text suffix=" " font-style="normal" text-case="capitalize-first" term="in"/>
<names delimiter=", " variable="editor translator">
<name initialize-with="." and="symbol"/>
<label form="short" suffix=")." prefix=" (" plural="never"/>
</names>
</if>
</choose>
</macro>
<macro name="secondary-contributors">
<choose>
<if match="none" type="chapter paper-conference">
<names delimiter=", " variable="editor translator" prefix=" ">
<name initialize-with=". " and="symbol" delimiter-precedes-last="never"/>
<label form="short" suffix=")" text-case="capitalize-first" prefix=" (" plural="never"/>
</names>
</if>
</choose>
</macro>
<macro name="author">
<choose>
<if match="all" variable="author">
<names variable="author">
<name initialize-with="." and="symbol" delimiter-precedes-last="never" name-as-sort-order="all"/>
<label form="short" suffix=")" prefix=" (" plural="never"/>
<substitute>
<names variable="editor"/>
<names variable="translator"/>
<names variable="composer"/>
</substitute>
</names>
</if>
<else>
<text value="Anon"/>
</else>
</choose>
</macro>
<macro name="author-short">
<names variable="author">
<name form="short" initialize-with=". " and="symbol" delimiter-precedes-last="never" et-al-use-first="1" et-al-min="4" delimiter-precedes-et-al="never"/>
<substitute>
<names variable="editor"/>
<names variable="translator"/>
</substitute>
</names>
</macro>
<macro name="access">
<choose>
<if type="webpage">
<text variable="URL"/>
<date suffix="]" delimiter=" " variable="accessed" prefix=" [">
<date-part name="day"/>
<date-part name="month"/>
<date-part name="year"/>
</date>
</if>
</choose>
</macro>
<macro name="publisher">
<group delimiter=": ">
<choose>
<if match="none" type="article-journal article-magazine article-newspaper">
<text variable="genre"/>
<choose>
<if match="any" variable="publisher-place">
<text variable="publisher-place"/>
</if>
<else>
<text value="t.tp."/>
</else>
</choose>
<choose>
<if match="any" variable="publisher">
<text variable="publisher"/>
</if>
<else>
<text value="t.pt."/>
</else>
</choose>
</if>
</choose>
</group>
</macro>
<macro name="issued">
<choose>
<if variable="issued">
<group suffix="." prefix=" ">
<date variable="issued" prefix=" ">
<date-part name="year"/>
</date>
</group>
</if>
<else>
<text suffix=")." prefix=" (" value="t.th."/>
</else>
</choose>
</macro>
<macro name="issued-year">
<choose>
<if variable="issued">
<date variable="issued">
<date-part name="year"/>
</date>
</if>
<else>
<text value="t.th."/>
</else>
</choose>
</macro>
<macro name="edition">
<choose>
<if match="any" type="bill book chapter graphic legal_case legislation motion_picture paper-conference report song">
<choose>
<if is-numeric="edition">
<text form="verb-short" term="edition" prefix=" " plural="true"/>
<number text-case="capitalize-first" variable="edition"/>
</if>
<else>
<text suffix="." variable="edition"/>
</else>
</choose>
</if>
</choose>
</macro>
<macro name="locators">
<choose>
<if match="any" type="article-journal article-magazine article-newspaper">
<text font-style="italic" variable="container-title" prefix=". "/>
<text variable="volume" prefix=" "/>
<text suffix=")" variable="issue" prefix="("/>
<text variable="page" prefix=": "/>
</if>
<else-if match="any" type="bill book chapter graphic legal_case legislation motion_picture report song">
<group prefix=". ">
<text macro="container-contributors"/>
<text macro="secondary-contributors"/>
<text variable="container-title" text-case="title" font-style="italic"/>
<text variable="page" prefix=", hlm. "/>
</group>
</else-if>
<else-if match="any" type="paper-conference">
<group>
<text suffix=", " font-style="italic" variable="container-title" prefix=". "/>
<text suffix=" " text-case="lowercase" term="page"/>
<text variable="page"/>
</group>
</else-if>
</choose>
</macro>
<macro name="citation-locator">
<group>
<label form="short" variable="locator"/>
<text variable="locator" prefix=" "/>
</group>
</macro>
<citation et-al-use-first="1" et-al-min="3" collapse="year" disambiguate-add-year-suffix="true">
