<?xml version="1.0" encoding="utf-8"?>
<style class="in-text" version="1.0" and="text" delimiter-precedes-last="never" et-al-min="4" et-al-use-first="1" initialize="false" page-range-format="chicago" initialize-with-hyphen="false" demote-non-dropping-particle="never" default-locale="en-US" xmlns="http://purl.org/net/xbiblio/csl">
  <info>
    <title>American Journal of Sociology</title>
    <title-short>Am. J. Sociol.</title-short>
    <id>http://www.zotero.org/styles/american-journal-of-sociology</id>
    <link href="http://www.zotero.org/styles/american-journal-of-sociology" rel="self"/>
    <link href="http://www.press.uchicago.edu/journals/ajs/instruct.html?journal=ajs" rel="documentation"/>
    <author>
      <name>Jamil Jonna</name>
      <email>rjonna@uoregon.edu</email>
    </author>
    <category citation-format="author-date"/>
    <category field="sociology"/>
    <issn>0002-9602</issn>
    <summary>AJS style based on Permafrost and Periglacial Processes</summary>
    <published>University of Chicago</published>
    <updated>2013-05-17T03:52:18+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <names variable="author">
      <name and="text" name-as-sort-order="first"/>
      <substitute>
        <names variable="editor">
          <label form="short" text-case="lowercase" prefix=" (" suffix=")"/>
          <name and="text" initialize="false" initialize-with="."/>
        </names>
        <text macro="anon"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <group suffix=".">
      <label form="verb" plural="never" text-case="capitalize-first" suffix=" "/>
      <names variable="editor" prefix=" ">
        <name and="text" delimiter-precedes-last="never" initialize="false"/>
      </names>
    </group>
  </macro>
  <macro name="anon">
    <text term="anonymous" form="short"/>
  </macro>
  <macro name="author-short">
    <names variable="author">
      <name form="short" and="text" delimiter=", "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <text macro="anon" text-case="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <group suffix=".">
      <choose>
        <if match="any" variable="DOI">
          <text variable="DOI" prefix="doi:"/>
        </if>
        <else-if type="book chapter thesis" match="none">
          <text variable="URL" text-case="lowercase" strip-periods="false" text-decoration="none" suffix=" "/>
          <choose>
            <if variable="accessed" match="all">
              <date form="numeric" variable="accessed" text-case="lowercase" prefix="(accessed " suffix=")"/>
            </if>
          </choose>
        </else-if>
      </choose>
    </group>
  </macro>
  <macro name="title">
    <group prefix=" " suffix=".">
      <choose>
        <if type="bill book graphic legal_case motion_picture report song thesis" match="any">
          <text variable="title" text-case="title" strip-periods="true" font-style="italic"/>
          <choose>
            <if type="book" match="all" variable="volume">
              <label text-case="lowercase" prefix=", " variable="volume" form="short"/>
              <number variable="volume"/>
            </if>
          </choose>
        </if>
        <else>
          <text variable="title" text-case="title" strip-periods="true" quotes="true"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="publisher">
    <if type="article-journal" match="none">
      <group delimiter=": " prefix=" " suffix=". ">
        <text variable="publisher-place"/>
        <text variable="publisher"/>
      </group>
    </if>
  </macro>
  <macro name="year">
    <choose>
      <if variable="issued" match="none">
        <text term="no date" form="short"/>
      </if>
      <else>
        <group>
          <date date-parts="year" variable="issued">
            <date-part name="year"/>
          </date>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="edition">
    <group prefix=" ">
      <choose>
        <if is-numeric="edition">
          <number variable="edition" form="ordinal"/>
          <text term="edition" form="short" strip-periods="true" prefix=" "/>
        </if>
        <else>
          <text variable="edition" strip-periods="true"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="pages-issue">
    <choose>
      <if type="chapter" match="all" variable="page">
        <group>
          <label text-case="capitalize-first" suffix=" " variable="page" form="short"/>
          <text variable="page" form="short"/>
        </group>
      </if>
      <else-if type="article-newspaper" match="all" variable="page">
        <text variable="page" prefix="(" suffix=")"/>
      </else-if>
      <else-if match="any" variable="issue">
        <text variable="issue" form="short" prefix=" (" suffix=")"/>
        <text variable="page" prefix=": "/>
      </else-if>
      <else>
        <text variable="page" form="short" prefix=":"/>
      </else>
    </choose>
  </macro>
  <macro name="point-locator">
    <choose>
      <if match="none" locator="page">
        <group prefix=", ">
          <label suffix=" " variable="locator" form="short"/>
          <text variable="locator" form="short"/>
        </group>
      </if>
      <else>
        <group prefix=", ">
          <label suffix=" " variable="locator" form="short"/>
          <text variable="locator" form="short"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="container-contributors">
    <text macro="editor"/>
    <choose>
      <if variable="container-author translator" match="any">
        <group delimiter=". " prefix=" ">
          <names variable="translator">
            <label form="verb" plural="never" text-case="capitalize-first" suffix=" "/>
            <name and="text" initialize="false"/>
          </names>
          <names variable="director">
            <label form="verb" text-case="capitalize-first"/>
            <name initialize="false"/>
          </names>
          <names variable="container-author">
            <label form="verb" text-case="capitalize-first" strip-periods="false" suffix=" "/>
            <name and="text" initialize="false"/>
          </names>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="month-day">
    <choose>
      <if match="none" is-uncertain-date="issued">
        <date delimiter="" variable="issued" prefix=" (" suffix=")">
          <date-part name="month" form="short" suffix=" "/>
          <date-part name="day"/>
        </date>
      </if>
      <else>
        <text term="circa" form="verb"/>
      </else>
    </choose>
  </macro>
  <macro name="year-original">
    <choose>
      <if match="all" variable="original-date">
        <date date-parts="year" variable="original-date">
          <date-part name="year" prefix=" (" suffix=") "/>
        </date>
      </if>
    </choose>
  </macro>
  <citation delimiter-precedes-last="never" et-al-min="4" et-al-use-first="1" sort-separator=", " names-delimiter="" near-note-distance="3" disambiguate-add-givenname="true" disambiguate-add-year-suffix="true" collapse="year" cite-group-delimiter="; ">
    <layout delimiter="; " prefix="(" suffix=")">
      <choose>
        <if match="any" position="ibid-with-locator">
          <group delimiter=", ">
            <text term="ibid"/>
          </group>
        </if>
        <else-if match="any" position="ibid">
          <text term="ibid"/>
        </else-if>
        <else>
          <group font-variant="normal" vertical-align="baseline" delimiter=" ">
            <text macro="author-short"/>
            <text macro="year"/>
          </group>
        </else>
      </choose>
      <text macro="point-locator"/>
    </layout>
  </citation>
  <bibliography delimiter-precedes-last="never" et-al-min="10" et-al-use-first="5" initialize="false" initialize-with="." subsequent-author-substitute="———" subsequent-author-substitute-rule="partial-first" hanging-indent="true">
    <sort>
      <key macro="author"/>
      <key variable="title"/>
    </sort>
    <layout>
      <group suffix=".">
        <text macro="author" suffix="."/>
        <text macro="year-original"/>
        <text macro="year" prefix=" "/>
        <text macro="month-day"/>
      </group>
      <choose>
        <if type="bill book graphic legal_case motion_picture report song" match="any">
          <text macro="title"/>
          <group delimiter=" " suffix=".">
            <text macro="edition"/>
          </group>
        </if>
        <else-if type="chapter paper-conference" match="any">
          <text macro="title" prefix=" " suffix="."/>
          <group prefix=" " delimiter=" ">
            <text macro="pages-issue"/>
            <text term="in" text-case="lowercase"/>
            <text variable="container-title" text-case="title" font-style="italic" suffix="."/>
            <text variable="collection-title" suffix="."/>
            <text variable="event" suffix="."/>
          </group>
        </else-if>
        <else-if type="thesis">
          <group prefix=" " suffix="." delimiter=". ">
            <text macro="title"/>
            <text variable="genre"/>
          </group>
        </else-if>
        <else>
          <group>
            <text macro="title" prefix=" "/>
          </group>
          <group prefix=" " suffix=".">
            <text variable="container-title" font-style="italic"/>
            <group prefix=" ">
              <text variable="volume"/>
              <text macro="pages-issue"/>
            </group>
          </group>
        </else>
      </choose>
      <text macro="container-contributors" suffix="."/>
      <text macro="publisher"/>
      <text prefix=" " macro="access"/>
    </layout>
  </bibliography>
</style>
