<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="note" version="1.0" default-locale="en-US">
  <info>
    <title>Vancouver Style - Ganesh Naik</title>
    <title-short>Vancouver Ganesh Naik</title-short>
    <id>http://www.zotero.org/styles/vancouver-ganesh-naik</id>
    <link href="http://www.zotero.org/styles/vancouver-ganesh-naik" rel="self"/>
    <link href="http://www.zotero.org/styles/vancouver" rel="template"/>
    <author>
      <name>Ganesh Naik</name>
    </author>
    <contributor>
      <name>ChatGPT</name>
    </contributor>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <updated>2025-07-11T00:00:00+00:00</updated>
    <summary>Custom Vancouver style with DOI hyperlink, no Internet tag for DOI items, [Internet] and [cited] shown only when DOI is absent</summary>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">
      This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License
    </rights>
  </info>

  <macro name="access">
    <choose>
      <if variable="DOI">
        <text variable="DOI" prefix=" https://doi.org/"/>
      </if>
      <else-if variable="URL">
        <group delimiter=" ">
          <text value="[Internet]."/>
          <group>
            <text value="[cited "/>
            <date variable="accessed" form="text" suffix="]"/>
          </group>
          <text value="Available from:"/>
          <text variable="URL"/>
        </group>
      </else-if>
    </choose>
  </macro>

  <macro name="author">
    <names variable="author">
      <name form="long" and="text" delimiter=", "/>
    </names>
  </macro>

  <macro name="title">
    <text variable="title"/>
  </macro>

  <macro name="container-title">
    <text variable="container-title" form="short"/>
  </macro>

  <macro name="issued">
    <date variable="issued">
      <date-part name="year"/>
    </date>
  </macro>

  <macro name="pages">
    <text variable="page"/>
  </macro>

  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter=", " prefix="[" suffix="]">
      <text variable="citation-number"/>
    </layout>
  </citation>

  <bibliography et-al-min="7" et-al-use-first="6" second-field-align="flush">
    <layout suffix=".">
      <text macro="author"/>
      <text macro="title" prefix=". "/>
      <text macro="container-title" prefix=" "/>
      <text macro="issued" prefix=" "/>
      <text macro="pages" prefix=":"/>
      <text macro="access" prefix=" "/>
    </layout>
  </bibliography>
</style>
