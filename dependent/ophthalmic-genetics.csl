<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.0" default-locale="en-US" page-range-format="expanded" demote-non-dropping-particle="sort-only" class="in-text">
  <info>
    <title>Ophthalmic Genetics</title>
    <title-short>Ophthalmic Genet.</title-short>
    <id>http://csl.mendeley.com/styles/18335961/american-medical-association</id>
    <link href="http://www.zotero.org/styles/ophthalmic-genetics" rel="self"/>
    <link href="http://www.zotero.org/styles/american-medical-association-10th-edition" rel="template"/>
    <link href="https://www.tandfonline.com/action/authorSubmission?show=instructions&journalCode=iopg20#style" rel="documentation"/>
    <author>      
      <name>Svend Gundestrup</name>
      <uri>http://www.mendeley.com/profiles/svend-gundestrup/</uri>
      <email>svend@gundestrup.dk</email>
    </author>
    <contributor>
      <name>Mette Andersen, MD</name>
      <uri>https://www.mendeley.com/profiles/mette-andersen1/</uri>
    </contributor>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <summary>Ophthalmic Genetics style updated to current style type, template originally is The American Medical Association style.</summary>
    <updated>2020-12-22T14:18:05+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en">
    <terms>
      <term name="page-range-delimiter">-</term>
    </terms>
  </locale>
  <macro name="editor">
    <names variable="editor">
      <name delimiter-precedes-last="always" delimiter=", " initialize-with="" sort-separator=" " name-as-sort-order="all"/>
      <label prefix=", " form="short"/>
    </names>
  </macro>
  <macro name="author">
    <group suffix=".">
      <names variable="author">
        <name delimiter-precedes-last="always" delimiter=", " initialize-with="" sort-separator=" " name-as-sort-order="all"/>
        <label prefix=", " form="short"/>
        <substitute>
          <names variable="editor"/>
          <text macro="title"/>
        </substitute>
      </names>
    </group>
  </macro>
  <macro name="access">
    <choose>
      <if type="article-newspaper" match="none">
        <choose>
          <if variable="DOI">
            <text value="doi:"/>
            <text variable="DOI"/>
          </if>
          <else-if variable="URL">
            <group delimiter=". ">
              <choose>
                <if type="webpage post post-weblog" match="any">
                  <date variable="issued" prefix="Published " form="text"/>
                </if>
              </choose>
              <group>
                <text suffix=" " text-case="capitalize-first" term="accessed"/>
                <date variable="accessed">
                  <date-part name="month" suffix=" "/>
                  <date-part name="day" suffix=", "/>
                  <date-part name="year"/>
                </date>
              </group>
              <text variable="URL"/>
            </group>
          </else-if>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if type="bill book graphic legal_case legislation motion_picture report song" match="any">
        <text variable="title" text-case="title" font-style="italic"/>
      </if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <text variable="publisher"/>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number variable="edition" form="ordinal"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text variable="edition" suffix="."/>
      </else>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout vertical-align="baseline" delimiter="," prefix="(" suffix=")">
      <text variable="citation-number"/>
      <group prefix="(" suffix=")">
        <label variable="locator" form="short" strip-periods="true"/>
        <text variable="locator"/>
      </group>
    </layout>
  </citation>
  <bibliography second-field-align="flush" et-al-use-first="3" et-al-min="7" hanging-indent="false">
    <layout>
      <text variable="citation-number" suffix=". "/>
      <text macro="author"/>
      <text prefix=" " suffix="." macro="title"/>
      <choose>
        <if type="bill book graphic legislation motion_picture report song" match="any">
          <group delimiter=" " prefix=" " suffix=".">
            <group delimiter=" ">
              <text form="short" text-case="capitalize-first" term="volume" strip-periods="true"/>
              <text variable="volume" suffix="."/>
            </group>
            <text macro="edition"/>
            <text prefix="(" suffix=")" macro="editor"/>
          </group>
          <text prefix=" " macro="publisher"/>
          <group prefix="; " suffix=".">
            <date variable="issued">
              <date-part name="year"/>
            </date>
            <text variable="page" prefix=":"/>
          </group>
        </if>
        <else-if type="chapter paper-conference entry-dictionary entry-encyclopedia" match="any">
          <group delimiter=" " prefix=" ">
            <text suffix=":" text-case="capitalize-first" term="in"/>
            <text macro="editor"/>
            <text variable="container-title" suffix="." text-case="title" font-style="italic"/>
            <group delimiter=" ">
              <text form="short" text-case="capitalize-first" term="volume" strip-periods="true"/>
              <text variable="volume" suffix="."/>
            </group>
            <text macro="edition"/>
            <text variable="collection-title" suffix="."/>
            <group suffix=".">
              <text macro="publisher"/>
              <group prefix="; " suffix=".">
                <date variable="issued">
                  <date-part name="year"/>
                </date>
                <text variable="page" prefix=":"/>
              </group>
            </group>
          </group>
        </else-if>
        <else-if type="article-newspaper">
          <text variable="container-title" prefix=" " suffix=". " font-style="italic"/>
          <choose>
            <if variable="URL">
              <group delimiter=". " suffix=".">
                <text variable="URL"/>
                <group prefix="Published ">
                  <date variable="issued">
                    <date-part name="month" suffix=" "/>
                    <date-part name="day" suffix=", "/>
                    <date-part name="year"/>
                  </date>
                </group>
                <group>
                  <text suffix=" " text-case="capitalize-first" term="accessed"/>
                  <date variable="accessed">
                    <date-part name="month" suffix=" "/>
                    <date-part name="day" suffix=", "/>
                    <date-part name="year"/>
                  </date>
                </group>
              </group>
            </if>
            <else>
              <group delimiter=":" suffix=".">
                <group>
                  <date variable="issued">
                    <date-part name="month" suffix=" "/>
                    <date-part name="day" suffix=", "/>
                    <date-part name="year"/>
                  </date>
                </group>
                <text variable="page"/>
              </group>
            </else>
          </choose>
        </else-if>
        <else-if type="legal_case">
          <group delimiter=" " prefix=" " suffix=",">
            <text prefix="(" suffix=")" macro="editor"/>
          </group>
          <group delimiter=" " prefix=" ">
            <text variable="container-title"/>
            <text variable="volume"/>
          </group>
          <text variable="page" prefix=", " suffix=" "/>
          <group delimiter=" " prefix="(" suffix=").">
            <text variable="authority"/>
            <date variable="issued">
              <date-part name="year"/>
            </date>
          </group>
        </else-if>
        <else-if type="webpage post post-weblog" match="any">
          <text variable="container-title" prefix=" " suffix="."/>
        </else-if>
        <else-if type="speech">
          <group prefix=" " suffix=":">
            <choose>
              <if variable="genre">
                <text variable="genre" suffix=" "/>
                <text term="presented at"/>
              </if>
              <else>
                <text text-case="capitalize-first" term="presented at"/>
              </else>
            </choose>
          </group>
          <group delimiter="; " prefix=" " suffix=".">
            <text variable="event"/>
            <group>
              <date variable="issued" delimiter=" ">
                <date-part name="month"/>
                <date-part name="day" suffix=","/>
                <date-part name="year"/>
              </date>
            </group>
            <text variable="event-place"/>
          </group>
        </else-if>
        <else>
          <text prefix=" " suffix="." macro="editor"/>
          <group prefix=" " suffix=".">
            <text variable="container-title" form="short" suffix="." font-style="italic" strip-periods="true"/>
            <group delimiter=";" prefix=" ">
              <choose>
                <if variable="issue volume" match="any">
                  <date variable="issued">
                    <date-part name="year"/>
                  </date>
                </if>
                <else>
                  <group delimiter=" ">
                    <text value="Published online"/>
                    <date variable="issued" form="text" date-parts="year-month-day"/>
                  </group>
                </else>
              </choose>
              <group>
                <text variable="volume"/>
                <text variable="issue" prefix="(" suffix=")"/>
              </group>
            </group>
            <text variable="page" prefix=":"/>
          </group>
        </else>
      </choose>
      <text prefix=" " macro="access"/>
    </layout>
  </bibliography>
</style>
