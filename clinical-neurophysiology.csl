<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="sort-only" page-range-format="minimal" default-locale="en-US">
  <info>
    <title>Clinical Neurophysiology</title>
    <id>http://www.zotero.org/styles/clinical-neurophysiology</id>
    <link href="http://www.zotero.org/styles/clinical-neurophysiology" rel="self"/>
    <author>
      <name>Michael Berkowitz</name>
      <email>mberkowi@gmu.edu</email>
    </author>
    <contributor>
      <name>Sean Takats</name>
      <email>stakats@gmu.edu</email>
    </contributor>
    <contributor>
      <name>Sebastian Karcher</name>
    </contributor>
    <issn>1388-2457</issn>
    <category field="medicine"/>
    <category citation-format="numeric"/>
    <updated>2008-09-16T00:00:00+00:00</updated>
    <summary>
      Vancouver author date version for the Elsevier Journal Cliniral Neurophysiology.(Note that the examples in the style guide are unreliable. Check actual articles for citations.
    </summary>
    <link href="http://www.elsevier.com/wps/find/journaldescription.cws_home/601528/authorinstructions" rel="documentation"/>
