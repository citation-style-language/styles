<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="note" version="1.0" demote-non-dropping-particle="sort-only" default-locale="en-GB">
  <info>
    <title>British Journal of Political Science</title>
    <id>http://www.zotero.org/styles/british-journal-of-political-science</id>
    <link href="http://www.zotero.org/styles/british-journal-of-political-science" rel="self"/>
    <link href="http://assets.cambridge.org/JPS/JPS_ifc.pdf" rel="documentation"/>
    <author>
      <name>Sebastian Karcher</name>
    </author>
    <contributor>
      <name>Julian Onions</name>
      <email>julian.onions@gmail.com</email>
    </contributor>
    <contributor>
      <name>Simon Kornblith</name>
      <email>simon@simonster.com</email>
    </contributor>
    <contributor>
      <name>Elena Razlogova</name>
      <email>elena.razlogova@gmail.com</email>
    </contributor>
    <summary/>
    <category field="social_science"/>
    <category citation-format="note"/>
    <issn>0007-1234</issn>
    <issn>1469-2112</issn>
    <updated>2009-12-04T20:22:16+00:00</updated>
