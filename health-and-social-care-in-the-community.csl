<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="sort-only" default-locale="en-US">
  <info>
    <title>Health &amp; Social Care in the Community</title>
    <id>http://www.zotero.org/styles/health-and-social-care-in-the-community</id>
    <link href="http://www.zotero.org/styles/health-and-social-care-in-the-community" rel="self"/>
    <link href="http://www.zotero.org/styles/biomed-research-international" rel="template"/>
    <link href="http://www.zotero.org/styles/asia-and-the-pacific-policy-studies" rel="template"/>
    <link href="https://www.hindawi.com/journals/hsc/2024/8841667/" rel="documentation"/>
    <author>
      <name>Patrick O'Brien</name>
    </author>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <category field="social_science"/>
    <issn>0966-0410</issn>
    <eissn>1365-2524</eissn>
    <updated>2024-04-09T07:27:19+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <group>
      <names variable="author">
        <name and="text" initialize-with="."/>
        <label form="short" strip-periods="false" prefix=" (" suffix=")"/>
        <substitute>
          <names variable="editor"/>
        </substitute>
      </names>
    </group>
  </macro>
  <macro name="editor">
    <names variable="editor">
      <name initialize-with="." delimiter=", " and="text" name-as-sort-order="all" sort-separator=", "/>
      <label form="short" prefix=" (" suffix=")"/>
    </names>
  </macro>
  <macro name="title">
    <choose>
      <if type="bill book graphic legal_case legislation motion_picture report song" match="any">
        <text variable="title" quotes="true"/>
      </if>
      <else>
        <text variable="title" quotes="true"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <text variable="publisher" font-style="italic"/>
    <text variable="publisher-place" prefix=", "/>
  </macro>
  <macro name="page">
    <group>
      <label plural="always" suffix=" " variable="page" form="short"/>
      <text variable="page"/>
    </group>
  </macro>
  <macro name="issued">
    <date variable="issued">
      <date-part name="year"/>
    </date>
  </macro>
  <citation et-al-min="3" et-al-use-first="1" collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout prefix="[" suffix="]" delimiter=", ">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography entry-spacing="0" second-field-align="flush">
    <layout suffix=".">
      <text variable="citation-number" prefix="[" suffix="]"/>
      <group delimiter=", ">
        <text macro="author"/>
        <choose>
          <if type="bill book graphic legal_case legislation motion_picture report song" match="any">
            <group delimiter=", ">
              <text macro="title"/>
              <text macro="publisher"/>
              <date variable="issued">
                <date-part name="year"/>
              </date>
            </group>
          </if>
          <else-if type="article-journal">
            <group delimiter=", ">
              <text macro="title"/>
              <text variable="container-title" font-style="italic"/>
              <group delimiter=" ">
                <label variable="volume" form="short"/>
                <text variable="volume"/>
              </group>
              <group delimiter=" ">
                <label variable="issue" form="short"/>
                <number variable="issue"/>
              </group>
              <text macro="page"/>
              <text macro="issued"/>
            </group>
          </else-if>
          <else-if type="chapter paper-conference" match="any">
            <group delimiter=", ">
              <text macro="title" suffix="."/>
              <choose>
                <if variable="container-title">
                  <group delimiter=", ">
                    <group delimiter=" ">
                      <text term="in"/>
                      <text variable="container-title" font-style="italic"/>
                    </group>
                    <text macro="editor" suffix=" "/>
                    <text macro="page"/>
                    <text macro="publisher"/>
                  </group>
                </if>
                <else>
                  <text term="presented at" text-case="capitalize-first" suffix=" "/>
                  <text variable="event"/>
                  <text variable="event-place" prefix=", "/>
                  <date variable="issued" prefix=" ">
                    <date-part name="month" suffix=" "/>
                    <date-part name="day" suffix=" "/>
                  </date>
                </else>
              </choose>
              <text macro="issued"/>
            </group>
          </else-if>
          <else-if type="webpage post post-weblog" match="any">
            <group delimiter=", ">
              <text macro="title"/>
              <text macro="issued"/>
              <text variable="URL"/>
            </group>
          </else-if>
          <else-if type="speech">
            <group delimiter=" ">
              <text macro="title" suffix="."/>
              <text variable="event" suffix="."/>
              <text variable="event-place" prefix=", "/>
              <date variable="issued">
                <date-part name="year" prefix="(" suffix=")"/>
              </date>
            </group>
          </else-if>
          <else>
            <group delimiter=", ">
              <text macro="title"/>
              <text macro="issued"/>
              <text variable="URL"/>
            </group>
          </else>
        </choose>
      </group>
    </layout>
  </bibliography>
</style>
