            <choose>
              <if variable="page">
                <number variable="volume" suffix=":"/>
                <text variable="page" prefix="(" suffix=")"/>
              </if>
            </choose>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="locators-journal">
