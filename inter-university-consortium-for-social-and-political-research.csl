<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" delimiter-precedes-et-al="never" demote-non-dropping-particle="never">
  <info>
    <title>Inter-university Consortium for Social and Political Research</title>
    <title-short>ICPSR</title-short>
    <id>http://www.zotero.org/styles/inter-university-consortium-for-social-and-political-research</id>
    <link href="http://www.zotero.org/styles/inter-university-consortium-for-social-and-political-research" rel="self"/>
    <link href="https://doi.org/10.3886/ICPSR08079.v2" rel="documentation"/>
    <author>
      <name>Inter-university Consortium for Social and Political Research</name>
      <uri>https://icpsr.umich.edu</uri>
      <email>icpsr-metadata@umich.edu</email>
    </author>
    <category citation-format="author-date"/>
    <summary>Citation style for data collections managed by the Inter-university Consortium for Social and Political Research.</summary>
    <updated>2024-11-18T11:16:32+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <citation>
    <layout>
      <group delimiter=". " suffix=".">
        <names variable="author" delimiter=",">
          <name and="text" name-as-sort-order="all"/>
          <substitute>
            <names variable="editor"/>
            <text variable="title"/>
          </substitute>
        </names>
        <date form="numeric" variable="issued" suffix=". "/>
      </group>
    </layout>
  </citation>
  <bibliography>
    <layout>
      <group delimiter=". " suffix=". ">
        <names variable="author" delimiter=",">
          <name and="text" name-as-sort-order="all"/>
          <substitute>
            <names variable="editor"/>
            <text variable="title"/>
          </substitute>
        </names>
        <text variable="title"/>
        <group delimiter=", ">
          <choose>
            <if variable="publisher">
              <group delimiter=" ">
                <text variable="publisher"/>
                <text value="[distributor]"/>
              </group>
            </if>
          </choose>
          <date form="numeric" variable="issued"/>
        </group>
      </group>
      <text variable="DOI" prefix="https://doi.org/"/>
    </layout>
  </bibliography>
</style>
