  <macro name="author">
    <names variable="author">
      <name sort-separator=" " initialize-with=". " name-as-sort-order="all" delimiter=", " delimiter-precedes-last="never" et-al-min="4" et-al-use-first="3" delimiter-precedes-et-al="never"> 
        <name-part name="given" prefix="(" suffix=")"/>
      </name>
      <label form="short" prefix=", " suffix=". " text-case="uppercase" strip-periods="true"/>
<et-al font-style="italic" font-variant="normal"/>
      <substitute>
       <text macro="editor-biblio" suffix=" dir.- "/>
      </substitute>
    </names>
  </macro>
 <macro name="author-short">
<choose>
<if variable="author">
<names variable="author">
       <name form="short" delimiter=", " delimiter-precedes-last="never" name-as-sort-order="all" sort-separator=" " et-al-min="4" et-al-use-first="1">
       <name-part name="family" text-case="uppercase"/>
      </name>
      <label form="short"  text-case="uppercase" strip-periods="true" />    
      <et-al font-style="italic" font-variant="normal"/>
  </names>
</if>
<else-if variable="editor">
<text macro="editor-short"/>
</else-if>
<else-if type="book">
<text variable="title-short" suffix=" ..."/>
</else-if>
<else-if type="article-journal">
            <text macro="journal-no-author"/>
</else-if>
</choose>
</macro>
<macro name="author-citation">
  <choose>
<if variable="author">
<names variable="author">
       <name form="short" delimiter=", " initialize-with=". " delimiter-precedes-last="never" name-as-sort-order="all" sort-separator=" " et-al-min="4" et-al-use-first="1">
       <name-part name="family"/>
      </name>
      <label form="short" prefix=", " suffix=". " text-case="uppercase" strip-periods="true" />    
      <et-al font-style="italic" font-variant="normal"/>
  </names>
</if>
<else-if variable="editor">
<text macro="editor-citation"/>
</else-if>
<else-if type="book">
<text variable="title-short" suffix=" ..."/>
</else-if>
<else-if type="article-journal">
            <text macro="journal-no-author"/>
</else-if>
</choose>
</macro>
<macro name="year-date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </if>
      <else>
        <text value="[s.d.]"/>
      </else>
    </choose>
  </macro>
<macro name="iconographie-citation">
<text variable="title-short" text-case="uppercase" suffix=""/> 
  </macro>
  <macro name="editor-citation">
    <names variable="editor">
      <name form="short" delimiter=", " initialize-with=". " delimiter-precedes-last="never" name-as-sort-order="all" sort-separator=" " et-al-min="4" et-al-use-first="1">
      </name>
    </names>
  </macro>
  <macro name="editor-short">
    <names variable="editor">
<name form="short" delimiter=", " initialize-with=". " delimiter-precedes-last="never" name-as-sort-order="all" sort-separator=" " et-al-min="4" et-al-use-first="1">
       <name-part name="family" text-case="uppercase"/>
      </name>   
      <et-al font-style="italic" font-variant="normal"/>
  </names>
  </macro>
 <macro name="editor-biblio">
    <names variable="editor" suffix=" ">
      <name sort-separator=" " initialize-with=". " name-as-sort-order="all" delimiter=", " delimiter-precedes-last="never" delimiter-precedes-et-al="never">
      </name>    
<et-al font-style="italic" font-variant="normal"/>
    </names>
 </macro>
 <macro name="title">
    <group delimiter=" ">
      <choose>
        <if type="chapter paper-conference article-journal article-magazine article-newspaper webpage" match="none">
          <text variable="title" font-style="italic"/>
          <choose>
            <if variable="URL DOI" match="any">
              <text value="en ligne" prefix=" [" suffix="]" text-case="capitalize-first"/>
            </if>
          </choose>
        </if>
        <else>
          <text variable="title" suffix=""/>
        </else>
      </choose>
    </group>
  </macro>
<macro name="publisher-place">   
<choose>
  <if variable="publisher-place">
<text variable="publisher-place" prefix=""/>
</if>
<else>
<text value="[s.l.]&#160;, "/>
</else>
</choose>
</macro>
<macro name="publisher">
<choose>
 <if variable="publisher">         
        <text variable="publisher"  suffix=" : "/>
      </if>
  <else-if type="manuscript">
 <text variable="archive" prefix="lieu de consultation : " suffix=", "/>
<text variable="call-number" suffix=", " />
</else-if>
<else-if type="report">
 <text variable="archive" prefix="lieu de consultation : " suffix=", "/>
</else-if>
      <else>
        <text value="[s.n.], "/>
      </else>
    </choose>
</macro>
<macro name="pages">
    <choose>
        <if type="report" match="any">
        <text variable="page"/>
      </if>
      <else>
        <text variable="number-of-pages"/>
      </else>
    </choose>
  </macro>
  <macro name="collection">
    <choose>
      <if variable="collection-number">
        <text variable="collection-title" prefix=" (" suffix=" ; "/>
        <text variable="collection-number" prefix=""  suffix="). "/>
      </if>
      <else>
        <text variable="collection-title" prefix=" (" suffix="). "/>
      </else>
    </choose>
  </macro>
<macro name="collection-conference">
    <choose>
      <if variable="volume">
        <text variable="collection-title" prefix=" (" suffix=" ; "/>
        <text variable="volume" prefix=""  suffix="). "/>
      </if>
      <else>
        <text variable="collection-title" prefix=" (" suffix="). "/>
      </else>
    </choose>
  </macro>
 <macro name="journal-title">
    <choose>
      <if type="chapter paper-conference article-journal article-magazine article-newspaper webpage" match="any">
        <group suffix=", ">
          <text variable="container-title" font-style="italic" form="short"/>
          <choose>
            <if variable="URL DOI" match="any">
              <text value="en ligne" prefix=" [" suffix="]" text-case="capitalize-first"/>
            </if>
          </choose>
        </group>
      </if>
      <else>
        <text variable="container-title" font-style="italic" suffix=". " form="short"/>
      </else>
    </choose>
  </macro>
<macro name="location">
    <choose>
      <if variable="edition">
        <text variable="edition" prefix=" (" suffix=""/>
        <text variable="archive" prefix=", "  suffix=""/>
        <text variable="call-number" prefix=" : "  suffix="). "/>
      </if>
      <else>
       <text variable="archive" prefix=" ("  suffix=""/>
        <text variable="call-number" prefix=" : "  suffix="). "/>
      </else>
    </choose>
  </macro>
<macro name="sort-key">
<choose>
<if type="map" match="any">
<text value="1"/>
</if>
<else-if type="graphic" match="any">
<text value="2"/>
</else-if>
<else-if type="entry-dictionary" match="any">
<text value="3"/>
</else-if>
<else-if type="manuscript" match="any">
<text value="4"/>
</else-if>
<else>
<text value="5"/>
</else>
</choose>
</macro>
   <citation disambiguate-add-year-suffix="true" disambiguate-add-names="true" disambiguate-add-givenname="true" givenname-disambiguation-rule="primary-name">
    <layout prefix=" " suffix="" delimiter=" ; ">
      <group delimiter="&#160;:&#160;p. ">
        <group delimiter=" ">
        <choose>
          <if type="thesis bill book report chapter paper-conference article-journal" match="any">
          <text macro="author-citation"/>
          <text macro="year-date"/>
          <text variable="year-suffix" prefix=" "/>
         </if>
          <else-if type="map graphic" match="any">
            <text macro="iconographie-citation"/>
          <text macro="year-date"/>
          </else-if>
          <else-if type="manuscript">
        <text variable="archive" prefix=""  suffix=""/>
        <text variable="call-number" prefix=""  suffix=""/>
          </else-if>
          <else>
          <text macro="author-citation"/>
          <text macro="year-date"/>
          <text variable="year-suffix" prefix=" "/>
          </else>
        </choose>
        </group>
        <text variable="locator"/>
      </group>
    </layout>
  </citation>
  <bibliography entry-spacing="1" line-spacing="1">
    <sort>
<key macro="sort-key"/>
      <key macro="author-short"/>
      <key variable="issued"/>
    </sort>
    <layout>
   <group prefix="" suffix="" delimiter=" ">
        <choose>
          <if type="thesis bill book report chapter paper-conference article-journal" match="any">
        <text macro="author-short"/>
        <text macro="year-date"/>
        <text variable="year-suffix" prefix=" "/>
         </if>
          <else-if type="map graphic" match="any">
            <text macro="iconographie-citation"/>
          <text macro="year-date"/>
          </else-if>
          <else-if type="manuscript">
        <text variable="archive" prefix=""  suffix=""/>
        <text variable="call-number" prefix=""  suffix=""/>
           </else-if>
          <else>
   <text macro="author-short"/>
        <text macro="year-date"/>
        <text variable="year-suffix" prefix=" "/>
          </else>
        </choose>
      </group>
      <group display="block" delimiter="" prefix="" suffix="">
        <choose>
          <if type="thesis bill book report chapter paper-conference article-journal" match="any">
        <text macro="author"   suffix=".- "/>
        <text macro="title" prefix="" suffix=""/>
         </if>
          <else-if type="map graphic" match="any">
        <text macro="title" prefix="" suffix=""/>
    <text variable="genre" prefix=", "  suffix=""/>
    <text variable="scale" prefix=", au "  suffix=", "/>
    <text variable="note" prefix=" "  suffix=""/>
          </else-if>
<else-if type="manuscript">
        <text variable="archive" prefix=""  suffix=""/>
          </else-if>
          <else>
        <text macro="author"   suffix=".- "/>
        <text macro="title" prefix="" suffix=""/>
          </else>
        </choose>
        <choose>
          <if type="thesis">
            <text variable="genre" prefix=", "  suffix=" : "/>
            <text variable="publisher-place"/>
            <text variable="number-of-pages"  prefix=", " suffix=" p."/>
          </if>
          <else-if type="bill book" match="any">
            <text macro="publisher-place" prefix=", "/>
            <text macro="publisher" prefix=", "/>
            <text macro="edition"/>
            <text macro="pages" suffix=" p."/>
<text variable="note" prefix=", " suffix=" ill. "/>
            <text macro="collection"/>
<text macro="location"/>
          </else-if>
          <else-if type="report" match="any">
            <text variable="genre" prefix=", "  suffix=""/>
            <text macro="publisher-place" prefix=", "/>
            <text macro="publisher" prefix=", "/>
            <text macro="edition"/>
            <text macro="pages" suffix=" p."/>
<text variable="note" prefix=", " suffix=" ill. "/>
          </else-if>
          <else-if type="manuscript" match="any">
            <group prefix="" suffix=".- ">
        <text variable="call-number" prefix=" : "  suffix=""/>
            <text variable="number-of-pages" prefix=", " suffix=""/>
       <text macro="author" prefix=" : " />
            </group>
            <group prefix="" suffix=".">
        <text macro="title" prefix="" suffix=""/>
        <text macro="year-date"  prefix=", "/>
            </group>
          </else-if>
          <else-if type="chapter paper-conference" match="any">
            <group prefix="">
              <text value="In" prefix=". " suffix=" : " font-style="italic"/>
       <text macro="editor-biblio" suffix=" (dir.), "/>
              <text variable="event" prefix="" suffix=" : " font-style="italic"/>
              <text macro="journal-title" prefix="" />
            </group>
            <text macro="publisher-place" />
            <text macro="publisher" prefix=", "/>
            <text variable="page" prefix=" p. " suffix=""/>
            <text macro="collection-conference"/>
          </else-if>
          <else-if type="webpage" match="any">
            <group prefix=" ">
              <text macro="editor" prefix=", "/>
              <text macro="journal-title"/>
              <text variable="event" suffix=". " font-style="italic"/>
            </group>
            <text macro="webpageinfo" prefix=" "/>
            <text variable="page" prefix=" p. " suffix="."/>
            <text macro="parution" prefix=" mis en ligne le "/>
            <text macro="accessed-date"/>
            <text macro="access"/>
          </else-if>
          <else-if type="article-journal" match="any">
            <group prefix="">
              <text macro="journal-title" prefix=", " />
            </group>
            <text variable="volume" prefix="" form="short"/>
      <text variable="issue" prefix=", " form="short"/>
            <text variable="page" prefix="&#160;:&#160;p.&#160;" suffix="."/>
          </else-if>
          <else-if type="map graphic" match="any">
            <group suffix=".">
        <text variable="archive" prefix=", "  suffix=""/>
        <text variable="call-number" prefix=" : "  suffix=""/>
            </group>
          </else-if>
          <else>
            <text macro="journal-title"/>
            <group suffix=".">
              <date variable="issued" suffix=".">
                <date-part name="day" suffix=" "/>
                <date-part name="month" suffix=" "/>
                <date-part name="year" suffix=""/>
              </date>
            </group>
            <group suffix=". ">
              <text macro="locators"/>
            </group>
          </else>
        </choose>
        <choose>
          <if type="bill book chapter graphic legal_case legislation motion_picture paper-conference song" match="any">
          </if>
        </choose>
      </group>
    </layout>
  </bibliography>
