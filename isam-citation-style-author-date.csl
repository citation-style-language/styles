                  <else>
                    <group delimiter="" suffix="">
                      <text variable="dimensions" suffix=". "/>
                      <text variable="archive"/>
                      <text variable="archive_location"/>
                    </group>
                  </else>
                </choose>
              </else-if>
              <else-if variable="collection-number collection-title" match="all">
                <group delimiter=", " suffix=". ">
                  <text variable="archive"/>
                  <text variable="collection-number"/>
                </group>
              </else-if>
              <else>
                <group delimiter="" suffix=". ">
                  <text variable="archive"/>
                  <text variable="archive_location"/>
                </group>
              </else>
            </choose>
            <choose>
              <if variable="abstract" match="any">
                <choose>
                  <if variable="publisher-place" match="any">
                    <text variable="publisher-place" suffix=". "/>
                  </if>
                </choose>
              </if>
            </choose>
            <group delimiter=". ">
              <group delimiter=": ">
                <text term="accessed" text-case="capitalize-first"/>
                <date variable="accessed" form="text"/>
              </group>
              <text variable="URL"/>
            </group>
          </group>
        </else-if>
        <else-if type="map" match="any">
          <group>
            <group delimiter=". " suffix=". ">
              <text macro="contributors"/>
            </group>
            <group delimiter=", " suffix=". ">
              <text variable="genre" text-case="capitalize-first"/>
              <text variable="scale"/>
            </group>
            <text variable="collection-title" text-case="title" suffix=". "/>
            <group delimiter=", " suffix=". ">
              <text variable="archive"/>
              <text variable="archive-place"/>
              <text variable="archive_location" text-case="capitalize-first"/>
            </group>
            <group delimiter=", " suffix=". ">
              <group delimiter=": ">
                <text variable="publisher-place"/>
                <text variable="publisher"/>
              </group>
              <choose>
                <if variable="available-date" match="any">
                  <choose>
                    <if variable="status">
                      <date variable="available-date" form="text"/>
                    </if>
                  </choose>
                </if>
              </choose>
            </group>
            <group delimiter=": ">
              <text term="accessed" text-case="capitalize-first"/>
              <date variable="accessed" form="text" suffix=". "/>
            </group>
            <text variable="URL"/>
          </group>
        </else-if>
        <else-if type="legal_case" match="any">
          <group>
            <group delimiter=". " suffix=". ">
              <text variable="authority"/>
              <text macro="issued-bib"/>
            </group>
            <group delimiter=", ">
              <text variable="number"/>
              <text variable="volume"/>
              <text variable="container-title"/>
              <text variable="page"/>
              <text variable="references"/>
            </group>
            <group>
              <choose>
                <if is-uncertain-date="issued">
                  <date variable="issued" form="text" prefix="[" suffix="?]"/>
                </if>
                <else>
                  <date variable="issued" form="text" prefix=", " suffix=". "/>
                </else>
              </choose>
              <choose>
                <if variable="accessed URL" match="any">
                  <group delimiter=" ">
                    <group delimiter=": " suffix=". ">
                      <text term="accessed" text-case="capitalize-first"/>
                      <date variable="accessed" form="text"/>
                    </group>
                    <text variable="URL"/>
                  </group>
                </if>
              </choose>
            </group>
          </group>
        </else-if>
        <else-if type="personal_communication" match="any">
          <group>
            <group delimiter=". " suffix=". ">
              <names variable="author">
                <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
              </names>
              <text macro="issued-bib"/>
            </group>
            <choose>
              <if variable="note">
                <text variable="title" text-case="title" quotes="true"/>
                <text value=". "/>
              </if>
              <else>
                <text variable="title" text-case="title" font-style="italic"/>
                <text value=". "/>
              </else>
            </choose>
            <group delimiter=", " suffix=". ">
              <text variable="annote"/>
              <choose>
                <if variable="division" match="any">
                  <text variable="division"/>
                </if>
                <else>
                  <choose>
                    <if is-uncertain-date="issued">
                      <date variable="issued" form="text" prefix="[" suffix="?]"/>
                    </if>
                    <else>
                      <date variable="issued" form="text"/>
                    </else>
                  </choose>
                </else>
              </choose>
            </group>
            <choose>
              <if variable="annote note" match="any">
                <group delimiter=". " prefix="Alıcı: " suffix=". ">
                  <names variable="recipient"/>
                </group>
              </if>
            </choose>
            <group delimiter=": ">
              <text term="accessed" text-case="capitalize-first"/>
              <date variable="accessed" form="text" suffix=". "/>
            </group>
            <text variable="URL"/>
          </group>
        </else-if>
        <else-if type="document" match="any">
          <choose>
            <if variable="author">
              <group delimiter=". " suffix=". ">
                <choose>
                  <if variable="archive" match="none">
                    <text macro="contributors"/>
                  </if>
                </choose>
                <choose>
                  <if variable="archive" match="none">
                    <text macro="issued-bib"/>
                    <text macro="title"/>
                    <group delimiter=", ">
                      <text variable="medium"/>
                      <text variable="genre"/>
                      <group>
                        <date variable="issued" form="text"/>
                      </group>
                      <group delimiter=": ">
                        <text variable="publisher-place"/>
                        <text variable="publisher"/>
                      </group>
                    </group>
                  </if>
                  <else>
                    <text macro="archive"/>
                  </else>
                </choose>
              </group>
            </if>
            <else>
              <choose>
                <if variable="archive" match="none">
                  <group delimiter=". " suffix=". ">
                    <group delimiter=". ">
                      <text macro="title"/>
                      <text macro="issued-bib"/>
                    </group>
                    <group delimiter=", ">
                      <text variable="medium"/>
                      <text variable="genre"/>
                      <group delimiter=": ">
                        <text variable="publisher-place"/>
                        <text variable="publisher"/>
                      </group>
                      <date variable="issued" form="text"/>
                      <text variable="number"/>
                    </group>
                  </group>
                </if>
                <else>
                  <text macro="archive" suffix=". "/>
                </else>
              </choose>
            </else>
          </choose>
          <text macro="access"/>
          <text variable="URL"/>
        </else-if>
        <else-if type="software" match="any">
          <group>
            <group delimiter=" - " suffix=", ">
              <names variable="author">
                <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
              </names>
            </group>
            <group delimiter=". " suffix=". ">
              <text variable="title" text-case="title" font-style="italic"/>
              <text macro="issued-bib"/>
            </group>
            <text variable="version" suffix=". "/>
            <group delimiter=", " suffix=". ">
              <text variable="medium" text-case="capitalize-first"/>
              <text variable="genre" text-case="capitalize-first"/>
            </group>
            <text value="Yazılım" suffix=", "/>
            <group delimiter=", " suffix=". ">
              <group delimiter=": ">
                <text variable="publisher-place"/>
                <text variable="publisher"/>
              </group>
              <choose>
                <if is-uncertain-date="issued">
                  <date variable="issued" form="text" prefix="[" suffix="?]"/>
                </if>
                <else>
                  <date variable="issued" form="text"/>
                </else>
              </choose>
            </group>
            <group delimiter=". ">
              <group delimiter=": ">
                <text term="accessed" text-case="capitalize-first"/>
                <date variable="accessed" form="text"/>
              </group>
              <choose>
                <if variable="DOI">
                  <text variable="DOI" prefix="https://doi.org/"/>
                </if>
                <else>
                  <text variable="URL"/>
                </else>
              </choose>
            </group>
          </group>
        </else-if>
        <else-if type="patent" match="any">
          <group>
            <group delimiter=". " suffix=". ">
              <names variable="author">
                <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
              </names>
              <text macro="issued-bib"/>
            </group>
            <text variable="title" text-case="title" font-style="italic" suffix=". "/>
            <text variable="annote" suffix=", "/>
            <group delimiter=". " suffix=", ">
              <text variable="authority"/>
            </group>
            <group delimiter=", " suffix=". ">
              <text variable="number"/>
              <text variable="call-number"/>
              <text variable="publisher-place"/>
              <choose>
                <if is-uncertain-date="submitted">
                  <date variable="submitted" form="text"/>
                </if>
                <else>
                  <date variable="submitted" form="text"/>
                </else>
              </choose>
              <choose>
                <if is-uncertain-date="issued">
                  <date variable="issued" form="text"/>
                </if>
                <else>
                  <date variable="issued" form="text"/>
                </else>
              </choose>
            </group>
            <group delimiter=": ">
              <text term="accessed" text-case="capitalize-first"/>
              <date variable="accessed" form="text" suffix=". "/>
            </group>
            <group delimiter=" ">
              <text variable="URL"/>
              <choose>
                <if variable="references">
                  <text variable="references" prefix="(" suffix=")"/>
                </if>
              </choose>
            </group>
          </group>
        </else-if>
        <else-if type="thesis" match="any">
          <group>
            <group delimiter=". ">
              <names variable="author">
                <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
              </names>
              <choose>
                <if is-uncertain-date="issued">
                  <date variable="issued" form="text" prefix="[" suffix="?]. "/>
                </if>
                <else>
                  <text macro="issued-bib"/>
                </else>
              </choose>
              <text variable="title" text-case="title" quotes="true" suffix=". "/>
            </group>
            <text variable="genre" suffix=" Tezi, "/>
            <group delimiter=": " suffix=". ">
              <text variable="publisher-place"/>
              <text variable="publisher"/>
            </group>
            <group delimiter=": ">
              <text term="accessed" text-case="capitalize-first"/>
              <date variable="accessed" form="text" suffix=". "/>
            </group>
            <text variable="URL"/>
          </group>
        </else-if>
        <else-if type="chapter" match="any">
          <group>
            <names variable="author" delimiter=", " suffix=". ">
              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
            </names>
            <text macro="issued-bib" suffix=". "/>
            <group>
              <choose>
                <if variable="interviewer" match="all">
                  <text variable="title" text-case="title" quotes="true" suffix=". "/>
                </if>
                <else-if variable="interviewer">
                  <text variable="title" text-case="title" quotes="false" font-style="italic" suffix=", "/>
                </else-if>
                <else-if variable="division" match="any">
                  <text variable="title" text-case="title" quotes="false" font-style="italic" suffix=". "/>
                </else-if>
                <else>
                  <text variable="title" text-case="title" quotes="true" suffix=". "/>
                </else>
              </choose>
              <group suffix=". ">
                <choose>
                  <if variable="interviewer" match="all">
                    <text macro="interviewer" prefix="" suffix=""/>
                  </if>
                </choose>
              </group>
              <group delimiter=", " suffix=". ">
                <names variable="editor">
                  <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                  <name delimiter=" - "/>
                </names>
                <choose>
                  <if variable="editor" match="any">
                    <names variable="collection-editor">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="collection-editor">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
                <choose>
                  <if variable="editor collection-editor" match="any">
                    <names variable="translator">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="translator">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
                <choose>
                  <if variable="editor collection-editor translator" match="any">
                    <names variable="reviewed-author">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="reviewed-author">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
                <choose>
                  <if variable="editor collection-editor translator reviewed-author" match="any">
                    <names variable="editorial-director">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="editorial-director">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
                <choose>
                  <if variable="editor collection-editor translator reviewed-author editorial-director" match="any">
                    <names variable="compiler">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="compiler">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
                <choose>
                  <if variable="editor collection-editor translator reviewed-author editorial-director compiler" match="any">
                    <names variable="director">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="director">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
              </group>
              <names variable="container-author" suffix=", "/>
            </group>
            <group delimiter=" " suffix=", ">
              <text variable="container-title" font-style="italic"/>
              <choose>
                <if variable="division" match="any">
                  <text term="in"/>
                </if>
              </choose>
            </group>
            <choose>
              <if variable="division" match="none">
                <group delimiter=", " suffix=". ">
                  <names variable="series-creator"/>
                  <text macro="locators-chapter"/>
                </group>
              </if>
              <else>
                <group delimiter=", ">
                  <names variable="series-creator"/>
                  <text macro="locators-chapter"/>
                </group>
              </else>
            </choose>
            <group suffix=". ">
              <group delimiter=": ">
                <text variable="publisher-place"/>
                <text variable="publisher"/>
              </group>
            </group>
            <group delimiter=": " suffix=". ">
              <text term="accessed" text-case="capitalize-first"/>
              <date variable="accessed" form="text"/>
            </group>
            <group delimiter=". ">
              <choose>
                <if variable="DOI">
                  <text variable="DOI" prefix="https://doi.org/"/>
                </if>
                <else>
                  <text variable="URL"/>
                </else>
              </choose>
            </group>
          </group>
        </else-if>
        <else-if type="standard" match="any">
          <group delimiter=". " suffix=". ">
            <text macro="contributors"/>
            <date variable="issued">
              <date-part name="year"/>
            </date>
          </group>
          <text macro="title"/>
          <text macro="container-contributors"/>
          <text macro="locators-chapter"/>
          <text macro="collection-title-journal"/>
          <group delimiter=", ">
            <text macro="publisher"/>
            <group delimiter=", " suffix=", ">
              <date variable="issued" delimiter=" ">
                <date-part name="day"/>
                <date-part name="month"/>
                <date-part name="year"/>
              </date>
            </group>
          </group>
          <text variable="number" suffix=". "/>
          <text macro="access"/>
        </else-if>
        <else-if type="paper-conference">
          <group delimiter=". " suffix=". ">
            <text macro="contributors"/>
            <text macro="issued-bib"/>
            <text macro="title"/>
          </group>
          <group delimiter=", " suffix=". ">
            <text macro="container-title"/>
            <text macro="container-contributors"/>
            <text macro="locators-chapter"/>
          </group>
          <text macro="collection-title-journal"/>
          <text macro="locators"/>
          <text macro="collection-title"/>
          <text macro="reprint"/>
          <group delimiter=", " suffix=". ">
            <text macro="publisher"/>
            <text macro="locators-article"/>
          </group>
          <text macro="access"/>
        </else-if>
        <else-if type="entry-encyclopedia">
          <group delimiter=". " suffix=". ">
            <text macro="contributors"/>
            <text macro="issued-bib"/>
            <text macro="title"/>
          </group>
          <text macro="secondary-contributors"/>
          <group delimiter=", " suffix=". ">
            <text macro="container-title"/>
            <text macro="edition"/>
            <text macro="container-contributors"/>
            <text macro="locators-chapter"/>
          </group>
          <text macro="collection-title-journal"/>
          <text macro="locators"/>
          <text macro="collection-title"/>
          <group delimiter=", " suffix=". ">
            <text macro="publisher"/>
            <text macro="locators-article"/>
          </group>
          <text macro="access"/>
        </else-if>
        <else>
          <choose>
            <if variable="author editor collection-editor translator reviewed-author compiler director curator" match="none">
              <choose>
                <if type="book" match="none">
                  <group delimiter=". ">
                    <text macro="title"/>
                    <text macro="issued-bib"/>
                  </group>
                </if>
                <else>
                  <group delimiter=". " suffix=". ">
                    <text macro="title"/>
                    <text macro="issued-bib"/>
                  </group>
                </else>
              </choose>
            </if>
            <else-if variable="note annote" match="all">
              <group delimiter=". ">
                <text macro="title"/>
                <text macro="issued-bib"/>
                <text macro="contributors"/>
              </group>
            </else-if>
            <else>
              <group delimiter=". " suffix=". ">
                <text macro="contributors"/>
                <text macro="issued-bib"/>
                <text macro="title"/>
                <choose>
                  <if type="entry-encyclopedia">
                    <group delimiter=", ">
                      <text macro="container-title"/>
                      <text macro="edition"/>
                    </group>
                  </if>
                </choose>
                <choose>
                  <if type="article-journal">
                    <text macro="secondary-contributors"/>
                  </if>
                </choose>
              </group>
            </else>
          </choose>
          <text macro="description"/>
          <choose>
            <if type="manuscript entry-encyclopedia article-journal" match="none">
              <text macro="secondary-contributors"/>
            </if>
          </choose>
          <choose>
            <if type="paper-conference">
              <text macro="container-title"/>
            </if>
          </choose>
          <text macro="container-contributors"/>
          <text macro="locators-chapter"/>
          <choose>
            <if type="article-journal" match="none">
              <text macro="collection-title-journal" prefix=", " suffix=", "/>
            </if>
          </choose>
          <text macro="locators"/>
          <choose>
            <if type="manuscript" match="none">
              <text macro="collection-title"/>
            </if>
          </choose>
          <text macro="issue"/>
          <text macro="locators-article" suffix="."/>
          <choose>
            <if type="manuscript paper-conference" match="any">
              <text macro="access" prefix=" "/>
            </if>
            <else-if type="article-journal entry-encyclopedia" match="any">
              <text macro="access" prefix=". "/>
            </else-if>
            <else>
              <text macro="access"/>
            </else>
          </choose>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>
