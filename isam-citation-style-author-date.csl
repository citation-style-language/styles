        <if type="report" match="any">
          <text macro="archive"/>
        </if>
        <else-if type="article-journal article-newspaper bill book chapter paper-conference software dataset document" match="none">
          <text macro="archive"/>
        </else-if>
      </choose>
      <choose>
        <if type="map" match="any"/>
        <else-if type="software" match="any"/>
        <else-if type="graphic"/>
        <else-if type="report" variable="medium" match="all"/>
        <else-if type="motion_picture" variable="annote" match="all"/>
        <else-if type="article-newspaper"/>
        <else-if type="legal_case"/>
        <else-if type="personal_communication"/>
        <else-if type="interview"/>
        <else-if type="patent"/>
        <else-if type="broadcast"/>
        <else>
          <group delimiter=": ">
            <text term="accessed" text-case="capitalize-first"/>
            <date variable="accessed" form="text"/>
          </group>
        </else>
      </choose>
    </group>
    <choose>
      <if type="legal_case report interview motion_picture broadcast map software patent article-newspaper personal_communication graphic document" match="none">
        <choose>
          <if variable="DOI" match="any">
            <text variable="DOI" prefix="https://doi.org/"/>
