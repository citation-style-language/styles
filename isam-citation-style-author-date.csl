                <text variable="genre" text-case="capitalize-first"/>
              </else>
            </choose>
            <text macro="event"/>
          </group>
          <text variable="event-place"/>
          <text macro="issued-cit"/>
        </group>
      </else-if>
      <else-if type="article-newspaper"/>
      <else-if type="book" variable="publisher-place publisher host keyword" match="all">
        <group suffix=".">
          <text macro="publisher"/>
        </group>
      </else-if>
      <else-if type="book" variable="division PMID" match="all">
        <group suffix=". ">
          <text macro="publisher"/>
        </group>
      </else-if>
      <else-if variable="publisher-place publisher" match="any">
        <group delimiter=", ">
          <group delimiter=". ">
            <text macro="originally-published"/>
            <group suffix=".">
              <choose>
                <if type="entry-encyclopedia entry-dictionary" match="any">
                  <choose>
                    <if variable="number-of-volumes" match="any">
                      <text value=". "/>
                    </if>
                  </choose>
                  <text macro="publisher"/>
                </if>
                <else>
                  <text macro="publisher"/>
                </else>
              </choose>
            </group>
          </group>
        </group>
      </else-if>
    </choose>
