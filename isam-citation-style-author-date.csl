<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="sort-only" name-delimiter=" - " et-al-min="5" et-al-use-first="1" page-range-format="expanded" default-locale="tr-TR">
  <info>
    <title>İSAM Atıf Sistemi (metiniçi)</title>
    <title-short>İSAM</title-short>
    <id>http://www.zotero.org/styles/isam-citation-style-author-date</id>
    <link href="http://www.zotero.org/styles/isam-citation-style-author-date" rel="self"/>
    <link href="http://www.zotero.org/styles/chicago-author-date" rel="template"/>
    <link href="https://www.isam.org.tr/uploads/67f7a5f5216f9.pdf" rel="documentation"/>
    <author>
      <name>Hüseyin Mertoğlu</name>
      <uri>https://orcid.org/0009-0003-6734-2048</uri>
      <email>huseyin.mertoglu@isam.org.tr</email>
    </author>
    <contributor>
      <name>Taha Yasin Tan</name>
      <email>tahayasin.tan@isam.org.tr</email>
    </contributor>
    <category citation-format="author-date"/>
    <category field="social_science"/>
    <category field="theology"/>
    <category field="history"/>
    <category field="humanities"/>
    <summary>İSAM Citation Style is a study in which the accumulation of İSAM’s knowledge is reshaped, interpreted and new standards are produced according to the special needs of İSAM, taking into account the standards of international systems developed to prove knowledge in academic texts. It can be used in all academic studies and publications in the fields of humanities, theology and social sciences. The official guide is available at https://www.isam.org.tr/uploads/67f7a5f5216f9.pdf</summary>
    <updated>2025-08-14T11:15:01+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="tr">
    <date form="text">
      <date-part name="day" form="numeric" suffix=" "/>
      <date-part name="month" suffix=" "/>
      <date-part name="year"/>
    </date>
    <terms>
      <term name="edition">basım</term>
      <term name="issue" form="short"/>
      <term name="number" form="verb-short">Kanun No</term>
      <term name="no date" form="short">t.y.</term>
      <term name="editor" form="verb-short">ed.</term>
      <term name="composer" form="verb-short">bst.</term>
      <term name="collection-editor" form="verb-short">thk.</term>
      <term name="editorial-director" form="verb-short">nşr.</term>
      <term name="director" form="verb-short">haz.</term>
      <term name="reviewed-author" form="verb-short">sd.</term>
      <term name="interview" form="verb">görüşme</term>
      <term name="interviewer" form="verb">görüşmeci</term>
      <term name="recipient" form="verb">alıcı</term>
      <term name="section" form="verb">kanun no</term>
      <term name="section" form="verb-short">k</term>
      <term name="volume" form="short">c</term>
      <term name="volume" form="verb">cilt</term>
      <term name="in" form="verb-short">içinde</term>
      <term name="sub-verbo" form="long">
        <single>numara</single>
        <multiple>numara</multiple>
      </term>
      <term name="sub-verbo" form="short">
        <single>no.</single>
        <multiple>no.</multiple>
      </term>
    </terms>
  </locale>
  <macro name="secondary-contributors-nehir">
    <group delimiter=", " suffix="">
      <names variable="editor">
        <label form="verb-short" suffix=" "/>
        <name delimiter=" - "/>
      </names>
      <choose>
        <if variable="editor" match="any">
          <names variable="collection-editor">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="collection-editor">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
          </names>
        </else>
      </choose>
      <choose>
        <if variable="editor collection-editor" match="any">
          <names variable="translator">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="translator">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
          </names>
        </else>
      </choose>
      <choose>
        <if variable="editor collection-editor translator" match="any">
          <names variable="reviewed-author">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="reviewed-author">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
          </names>
        </else>
      </choose>
      <choose>
        <if variable="editor collection-editor translator reviewed-author" match="any">
          <names variable="editorial-director">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="editorial-director">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
          </names>
        </else>
      </choose>
      <choose>
        <if variable="editor collection-editor translator reviewed-author editorial-director" match="any">
          <names variable="compiler">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="compiler">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
          </names>
        </else>
      </choose>
      <choose>
        <if variable="editor collection-editor translator reviewed-author editorial-director compiler" match="any">
          <names variable="director">
            <label form="verb-short" suffix=" "/>
            <name delimiter=" - "/>
          </names>
        </if>
        <else>
          <names variable="director">
            <label form="verb-short" suffix=" " text-case="capitalize-first"/>
            <name delimiter=" - "/>
          </names>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="secondary-contributors">
    <group>
      <choose>
        <if type="interview" variable="section page editor publisher-place publisher ISBN" match="all"/>
        <else-if type="book">
          <choose>
            <if match="none" variable="section">
              <group delimiter=", " suffix=". ">
                <names variable="editor">
                  <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                  <name delimiter=" - "/>
                </names>
                <choose>
                  <if variable="editor" match="any">
                    <names variable="collection-editor">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="collection-editor">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
                <choose>
                  <if variable="editor collection-editor" match="any">
                    <names variable="translator">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="translator">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
                <choose>
                  <if variable="editor collection-editor translator" match="any">
                    <names variable="reviewed-author">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="reviewed-author">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
                <choose>
                  <if variable="editor collection-editor translator reviewed-author" match="any">
                    <names variable="editorial-director">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="editorial-director">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
                <choose>
                  <if variable="editor collection-editor translator reviewed-author editorial-director" match="any">
                    <names variable="compiler">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="compiler">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
                <choose>
                  <if variable="editor collection-editor translator reviewed-author editorial-director compiler" match="any">
                    <names variable="director">
                      <label form="verb-short" suffix=" "/>
                      <name delimiter=" - "/>
                    </names>
                  </if>
                  <else>
                    <names variable="director">
                      <label form="verb-short" suffix=" " text-case="capitalize-first"/>
                      <name delimiter=" - "/>
                    </names>
                  </else>
                </choose>
              </group>
            </if>
          </choose>
        </else-if>
        <else-if type="manuscript">
          <names variable="editor translator editorial-director collection-editor curator reviewed-author original-author director" delimiter=". " prefix=". " suffix=". ">
            <label form="verb-short" text-case="capitalize-first" suffix=" "/>
            <name delimiter=" - " initialize-with=". "/>
          </names>
        </else-if>
        <else-if type="chapter entry-dictionary entry-encyclopedia paper-conference motion_picture broadcast" match="none">
          <names variable="editor translator editorial-director collection-editor curator reviewed-author original-author director" delimiter=". ">
            <label form="verb-short" text-case="capitalize-first" suffix=" "/>
            <name delimiter=" - " initialize-with=". "/>
          </names>
        </else-if>
        <else-if type="entry-encyclopedia"/>
        <else>
          <group delimiter=" ">
            <names variable="collection-editor editorial-director reviewed-author">
              <name form="short" delimiter=" - " initialize-with=". "/>
              <label form="short" prefix=" (" suffix=")"/>
              <substitute>
                <names variable="collection-editor"/>
              </substitute>
            </names>
            <text macro="recipient-note"/>
          </group>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="container-contributors">
    <choose>
      <if type="chapter" variable="section" match="all"/>
      <else-if type="chapter" variable="keyword" match="all"/>
      <else-if type="entry-encyclopedia" match="any">
        <group delimiter=", ">
          <choose>
            <if variable="container-author" match="any">
              <names variable="container-author">
                <name delimiter=" - "/>
              </names>
            </if>
          </choose>
          <choose>
            <if variable="container-author author" match="all">
              <group delimiter=". ">
                <names variable="editor translator" delimiter=", ">
                  <label form="verb" suffix=" "/>
                  <name and="text" delimiter=", "/>
                </names>
              </group>
            </if>
            <else>
              <group>
                <names variable="editor collection-editor editorial-director reviewed-author original-author director" delimiter=", ">
                  <label form="verb-short" text-case="lowercase" suffix=" "/>
                  <name delimiter=" - "/>
                </names>
              </group>
            </else>
          </choose>
        </group>
      </else-if>
      <else-if type="paper-conference" match="any">
        <group delimiter=", ">
          <choose>
            <if variable="author">
              <choose>
                <if variable="container-author" match="any">
                  <names variable="container-author">
                    <name delimiter=" - "/>
                  </names>
                </if>
              </choose>
              <choose>
                <if variable="container-author author" match="all">
                  <group delimiter=". ">
                    <names variable="editor translator" delimiter=", ">
                      <label form="verb" suffix=" "/>
                      <name and="text" delimiter=", "/>
                    </names>
                  </group>
                </if>
                <else>
                  <names variable="editor collection-editor editorial-director reviewed-author original-author director" delimiter=", ">
                    <label form="verb-short" text-case="lowercase" suffix=" "/>
                    <name delimiter=" - "/>
                  </names>
                </else>
              </choose>
            </if>
          </choose>
        </group>
      </else-if>
      <else-if type="chapter entry-dictionary" match="any">
        <group delimiter=", " suffix=", ">
          <choose>
            <if variable="author">
              <choose>
                <if variable="container-author" match="any">
                  <names variable="container-author">
                    <name delimiter=" - "/>
                  </names>
                </if>
              </choose>
              <choose>
                <if variable="container-author author" match="all">
                  <group delimiter=". ">
                    <names variable="editor translator" delimiter=", ">
                      <label form="verb" suffix=" "/>
                      <name and="text" delimiter=", "/>
                    </names>
                  </group>
                </if>
                <else>
                  <names variable="editor collection-editor editorial-director reviewed-author original-author director" delimiter=", ">
                    <label form="verb-short" text-case="lowercase" suffix=" "/>
                    <name delimiter=" - "/>
                  </names>
                </else>
              </choose>
            </if>
          </choose>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="recipient-note">
    <names variable="recipient" delimiter=", ">
      <label form="verb" text-case="lowercase" suffix=" "/>
      <name and="text" delimiter=", "/>
    </names>
  </macro>
  <macro name="editor">
    <choose>
      <if type="entry-encyclopedia" match="none">
        <names variable="editor">
          <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
          <label form="short" prefix=" (" suffix=")"/>
        </names>
      </if>
      <else>
        <names variable="author">
          <substitute>
            <text term="editor" form="short" suffix=". "/>
            <names variable="editor">
              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
            </names>
          </substitute>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="translator">
    <names variable="translator">
      <name name-as-sort-order="first" and="text" sort-separator=", " delimiter=", " delimiter-precedes-last="always"/>
      <label form="verb-short" prefix=", "/>
    </names>
  </macro>
  <macro name="recipient">
    <choose>
      <if type="personal_communication" variable="recipient" match="all"/>
      <else>
        <group delimiter=" ">
          <choose>
            <if type="personal_communication">
              <choose>
                <if variable="genre">
                  <text variable="genre" text-case="capitalize-first"/>
                </if>
                <else>
                  <text term="letter" text-case="capitalize-first"/>
                </else>
              </choose>
            </if>
          </choose>
          <text macro="recipient-note"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="contributors">
    <group delimiter=". ">
      <choose>
        <if type="report">
          <choose>
            <if variable="author contributor" match="any">
              <choose>
                <if variable="author" match="any">
                  <group delimiter=". ">
                    <names variable="author">
                      <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
                    </names>
                  </group>
                </if>
                <else>
                  <group delimiter=". ">
                    <names variable="contributor">
                      <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
                    </names>
                  </group>
                </else>
              </choose>
            </if>
            <else>
              <text variable="publisher"/>
            </else>
          </choose>
        </if>
        <else-if type="document">
          <group>
            <names variable="author">
              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="never"/>
            </names>
          </group>
        </else-if>
        <else-if variable="author contributor editor collection-editor translator reviewed-author compiler director curator performer script-writer" match="none">
          <choose>
            <if variable="annote note PMID" match="all">
              <group delimiter=". " suffix=". ">
                <text variable="title" font-style="italic"/>
                <text macro="date-sort"/>
              </group>
            </if>
            <else-if type="report" match="any">
              <text variable="publisher"/>
            </else-if>
            <else-if variable="issued">
              <group delimiter=". " suffix=". ">
                <text variable="title"/>
                <text macro="issued-bib" suffix="."/>
                <text variable="collection-title" font-style="italic"/>
              </group>
            </else-if>
            <else>
              <group delimiter=". ">
                <text variable="title" font-style="italic"/>
                <text macro="date-sort"/>
              </group>
            </else>
          </choose>
        </else-if>
        <else-if type="map">
          <group delimiter=". ">
            <names variable="author">
              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
            </names>
            <text macro="issued-cit"/>
            <text variable="title" font-style="italic"/>
          </group>
        </else-if>
        <else-if variable="annote note PMID" match="all">
          <choose>
            <if variable="author translator" match="any">
              <group delimiter=". " suffix=". ">
                <text variable="title" font-style="italic"/>
                <text macro="date-sort"/>
                <names variable="author">
                  <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
                </names>
              </group>
            </if>
            <else>
              <group delimiter=". " suffix=". ">
                <text variable="title" font-style="italic"/>
                <text macro="date-sort"/>
              </group>
            </else>
          </choose>
        </else-if>
        <else-if variable="division PMID" match="all">
          <group delimiter=". ">
            <text macro="title"/>
            <text macro="date-sort"/>
            <names variable="author">
              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="never"/>
            </names>
          </group>
        </else-if>
        <else-if type="graphic">
          <choose>
            <if variable="PMID part-number" match="all">
              <group suffix=". ">
                <group delimiter=". " suffix=". ">
                  <text macro="title"/>
                  <text macro="issued-bib"/>
                  <choose>
                    <if variable="PMID" match="any">
                      <choose>
                        <if variable="illustrator" match="any">
                          <group delimiter=" - ">
                            <names variable="illustrator">
                              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
                            </names>
                          </group>
                        </if>
                      </choose>
                    </if>
                  </choose>
                </group>
                <text variable="collection-title" font-style="italic" suffix=", "/>
                <text variable="medium" text-case="capitalize-first" suffix=". "/>
                <names variable="author contributor">
                  <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="never"/>
                </names>
              </group>
            </if>
            <else>
              <group delimiter=". " suffix=". ">
                <names variable="author contributor">
                  <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="never"/>
                </names>
                <text macro="issued-bib"/>
              </group>
            </else>
          </choose>
        </else-if>
        <else-if variable="note annote PMID" match="all">
          <group suffix=". ">
            <names variable="author">
              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="never"/>
            </names>
          </group>
        </else-if>
        <else-if variable="division section" match="any">
          <names variable="author">
            <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="never"/>
          </names>
        </else-if>
        <else-if type="book" variable="status medium" match="all">
          <group>
            <names variable="author" prefix="[" suffix="] ">
              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="never"/>
            </names>
            <names variable="contributor" prefix="" suffix="">
              <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
            </names>
          </group>
        </else-if>
        <else-if type="book" variable="note annote" match="all"/>
        <else-if type="motion_picture">
          <group suffix=". ">
            <names variable="author">
              <name name-as-sort-order="first" and="text" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
              <substitute>
                <names variable="director" suffix=" (yön.). ">
                  <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
                </names>
              </substitute>
            </names>
          </group>
        </else-if>
        <else-if type="webpage" variable="note annote" match="all"/>
        <else-if type="entry-encyclopedia">
          <names variable="author">
            <name name-as-sort-order="first" and="text" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
            <substitute>
              <names variable="editor">
                <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
                <label form="verb-short" prefix=" (" suffix=")"/>
              </names>
            </substitute>
          </names>
        </else-if>
        <else-if type="broadcast" match="any">
          <names variable="author">
            <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
            <substitute>
              <names variable="director" suffix=" (sun.). ">
                <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
              </names>
            </substitute>
          </names>
        </else-if>
        <else-if type="chapter map legal_case graphic software legislation" match="none">
          <names variable="author">
            <name name-as-sort-order="first" sort-separator=", " delimiter=" - " delimiter-precedes-last="always"/>
            <substitute>
              <choose>
                <if type="report">
                  <text variable="publisher" suffix=". "/>
                </if>
                <else-if type="entry-encyclopedia" match="none">
                  <text macro="editor"/>
                </else-if>
              </choose>
              <text macro="translator"/>
              <choose>
                <if type="article-magazine article-newspaper webpage post-weblog" match="any">
                  <text variable="container-title"/>
                </if>
              </choose>
            </substitute>
          </names>
        </else-if>
      </choose>
      <text macro="recipient"/>
    </group>
  </macro>
  <macro name="contributors-sort">
    <choose>
      <if type="motion_picture software legislation">
        <choose>
          <if variable="author" match="none">
            <text variable="title"/>
          </if>
        </choose>
      </if>
      <else-if variable="host keyword" match="all">
        <choose>
          <if variable="status medium" match="none">
            <text variable="keyword"/>
            <names variable="author contributor editor">
              <name name-as-sort-order="all" and="text" sort-separator=", " delimiter=", " delimiter-precedes-last="always"/>
              <substitute>
                <names variable="contributor"/>
                <names variable="editor"/>
                <names variable="translator"/>
                <text macro="title"/>
              </substitute>
            </names>
          </if>
        </choose>
      </else-if>
      <else-if type="book" variable="section interviewer author editor" match="all">
        <names variable="author contributor editor">
          <name name-as-sort-order="all" and="text" sort-separator=", " delimiter=", " delimiter-precedes-last="always"/>
          <substitute>
            <names variable="contributor"/>
            <names variable="editor"/>
            <names variable="translator"/>
            <text macro="title"/>
          </substitute>
        </names>
      </else-if>
      <else-if type="book" variable="note annote" match="all">
        <text variable="title"/>
      </else-if>
      <else-if type="book" variable="note section" match="all">
        <text variable="section"/>
      </else-if>
      <else-if type="webpage" variable="note annote" match="all">
        <text variable="title"/>
      </else-if>
      <else-if type="entry-dictionary">
        <choose>
          <if variable="author" match="none">
            <text variable="publisher"/>
          </if>
        </choose>
      </else-if>
      <else-if type="entry-encyclopedia">
        <choose>
          <if variable="author" match="none">
            <text variable="title"/>
          </if>
        </choose>
      </else-if>
      <else-if type="dataset report">
        <choose>
          <if variable="author" match="none">
            <text variable="publisher"/>
          </if>
        </choose>
      </else-if>
      <else-if type="legal_case">
        <choose>
          <if variable="author" match="none">
            <text variable="authority"/>
          </if>
        </choose>
      </else-if>
      <else-if type="graphic" variable="issued genre publisher-place PMID part-number abstract" match="all">
        <text variable="title"/>
      </else-if>
      <else-if type="graphic" variable="issued collection-title collection-number author PMID part-number" match="all">
        <text variable="title"/>
      </else-if>
      <else-if type="document" variable="jurisdiction annote" match="all">
        <text variable="title"/>
      </else-if>
      <else-if variable="division PMID" match="all">
        <text variable="title"/>
      </else-if>
    </choose>
    <names variable="author contributor editor">
      <name name-as-sort-order="all" and="text" sort-separator=", " delimiter=", " delimiter-precedes-last="always"/>
      <substitute>
        <names variable="contributor"/>
        <names variable="editor"/>
        <names variable="translator"/>
        <text macro="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="contributors-short">
    <choose>
      <if variable="host keyword" match="all">
        <text variable="keyword"/>
      </if>
      <else-if type="dataset" variable="division" match="all">
        <text variable="division"/>
      </else-if>
      <else-if type="graphic" variable="illustrator" match="all">
        <text macro="title-note"/>
      </else-if>
      <else>
        <names variable="author">
          <name form="short" delimiter=" - " initialize-with=". "/>
          <substitute>
            <names variable="contributor"/>
            <names variable="editor"/>
            <names variable="translator"/>
            <names variable="director"/>
            <choose>
              <if type="manuscript" match="none">
                <text macro="substitute-title"/>
              </if>
            </choose>
            <text macro="title"/>
          </substitute>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="interviewer">
    <names variable="interviewer" delimiter=" - ">
      <label form="verb" text-case="capitalize-first" suffix=": "/>
      <name delimiter=" - "/>
    </names>
  </macro>
  <macro name="title-note">
    <choose>
      <if variable="title" match="none">
        <text variable="genre"/>
      </if>
      <else-if type="dataset">
        <text variable="title" quotes="true" prefix="Aranan: " form="short"/>
      </else-if>
      <else-if type="document" variable="jurisdiction" match="all">
        <text variable="title" text-case="title" quotes="false" form="short"/>
      </else-if>
      <else-if type="document" variable="medium number publisher-place" match="all">
        <text variable="title" text-case="title" quotes="false" font-style="italic" form="short"/>
        <group prefix=" (" suffix=")">
          <text variable="publisher" suffix=", "/>
          <group delimiter=".">
            <date variable="issued" form="numeric">
              <date-part name="day" form="numeric-leading-zeros"/>
              <date-part name="month"/>
              <date-part name="year"/>
            </date>
          </group>
        </group>
      </else-if>
      <else-if type="document" variable="genre issued" match="all">
        <text variable="title" text-case="title" quotes="true"/>
        <group prefix=" (" suffix=")">
          <text variable="genre" suffix=", "/>
          <date variable="issued" form="text"/>
        </group>
      </else-if>
      <else-if type="book" variable="annote" match="all">
        <choose>
          <if variable="annote note PMCID" match="all">
            <text variable="title" text-case="title" form="short"/>
          </if>
          <else-if variable="annote note" match="all">
            <text variable="title" text-case="title" font-style="italic" form="short"/>
          </else-if>
          <else-if variable="annote jurisdiction" match="all">
            <text variable="title" text-case="title" form="short" font-style="italic"/>
          </else-if>
          <else-if variable="annote division" match="all">
            <group delimiter=", ">
              <text variable="title" text-case="title" form="short" font-style="italic"/>
              <text variable="keyword"/>
            </group>
          </else-if>
          <else-if variable="annote"/>
          <else>
            <text variable="title" text-case="title" form="short"/>
          </else>
        </choose>
      </else-if>
      <else-if type="book manuscript map" match="any">
        <text variable="title" text-case="title" font-style="italic" form="short"/>
        <group delimiter=" " prefix=", ">
          <text term="version"/>
          <text variable="version"/>
        </group>
      </else-if>
      <else-if type="thesis" match="any">
        <text variable="title" text-case="title" quotes="true" form="short"/>
      </else-if>
      <else-if type="article-journal paper-conference entry-dictionary" match="any">
        <text variable="title" quotes="true" form="short"/>
      </else-if>
      <else-if type="interview" match="any">
        <text variable="title" quotes="true" form="short"/>
      </else-if>
      <else-if variable="reviewed-author">
        <text variable="title" font-style="italic" prefix="review of "/>
      </else-if>
      <else-if type="report">
        <text variable="title" font-style="italic" form="short"/>
      </else-if>
      <else-if type="webpage">
        <choose>
          <if variable="note" match="any">
            <text variable="title" font-style="italic" form="short"/>
          </if>
          <else>
            <text variable="title" quotes="true" form="short"/>
          </else>
        </choose>
      </else-if>
      <else-if type="legislation" match="any">
        <text variable="title" text-case="title" form="short"/>
      </else-if>
      <else-if type="motion_picture" variable="annote" match="all">
        <text variable="title" text-case="title" form="short"/>
      </else-if>
      <else-if type="motion_picture broadcast song software patent" match="any">
        <text variable="title" text-case="title" font-style="italic" form="short"/>
      </else-if>
      <else-if type="personal_communication" match="any">
        <choose>
          <if variable="note" match="any">
            <text variable="title" text-case="title" quotes="true" form="short"/>
          </if>
          <else>
            <text variable="title" text-case="title" font-style="italic" form="short"/>
          </else>
        </choose>
      </else-if>
      <else-if type="graphic" match="any">
        <choose>
          <if variable="part-title" match="any">
            <group delimiter=", ">
              <text variable="title" text-case="title" form="short"/>
              <text variable="part-title" font-style="italic"/>
            </group>
          </if>
          <else-if variable="collection-title genre PMID" match="any">
            <text variable="title" text-case="title" form="short"/>
          </else-if>
          <else>
            <group delimiter=", ">
              <text variable="title" text-case="title" font-style="italic" form="short"/>
            </group>
          </else>
        </choose>
      </else-if>
      <else-if type="legal_case" match="any">
        <text variable="title" form="short" text-case="title"/>
      </else-if>
      <else-if type="chapter">
        <text variable="title" font-style="italic" quotes="false"/>
      </else-if>
      <else>
        <text variable="title" quotes="true"/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if variable="division PMID" match="all">
        <text variable="title" text-case="title" font-style="italic"/>
      </if>
      <else-if variable="note annote PMID" match="all">
        <choose>
          <if variable="author editor collection-editor translator reviewed-author compiler director curator" match="none">
            <text variable="title" text-case="title" font-style="italic" suffix=". "/>
          </if>
          <else>
            <text variable="title" text-case="title" font-style="italic"/>
          </else>
        </choose>
      </else-if>
      <else-if type="document" variable="annote jurisdiction" match="all">
        <text variable="title" text-case="title" form="short"/>
      </else-if>
      <else-if type="document" variable="source abstract volume" match="all">
        <text variable="title" text-case="title" form="short"/>
      </else-if>
      <else-if type="book" variable="annote" match="all">
        <choose>
          <if variable="reviewed-genre">
            <text variable="title" text-case="title" font-style="italic"/>
          </if>
          <else-if variable="annote note PMCID" match="all">
            <text variable="title" text-case="title" form="short"/>
          </else-if>
          <else-if variable="annote note" match="all">
            <text variable="title" text-case="title" font-style="italic" form="short"/>
          </else-if>
          <else-if variable="annote jurisdiction" match="all">
            <text variable="title" text-case="title" font-style="italic"/>
          </else-if>
          <else-if variable="annote division" match="all">
            <group delimiter=", ">
              <text variable="title" text-case="title" font-style="italic" suffix=". "/>
              <text variable="keyword"/>
            </group>
          </else-if>
          <else-if variable="annote host" match="all">
            <group delimiter=", ">
              <text variable="title" text-case="title" font-style="italic" suffix=". "/>
            </group>
          </else-if>
          <else>
            <text variable="title" text-case="title" form="short" font-style="italic" suffix=". "/>
          </else>
        </choose>
      </else-if>
      <else-if type="book">
        <choose>
          <if variable="editor translator collection-editor reviewed-author editorial-director compiler director" match="any">
            <text variable="title" text-case="title" font-style="italic"/>
          </if>
          <else-if variable="host keyword">
            <choose>
              <if variable="publisher publisher-place issued" match="any">
                <text variable="title" text-case="title" font-style="italic"/>
              </if>
              <else>
                <text variable="title" text-case="title" font-style="italic"/>
              </else>
            </choose>
          </else-if>
          <else>
            <text variable="title" text-case="title" font-style="italic"/>
          </else>
        </choose>
        <choose>
          <if variable="section" match="any">
            <text macro="interviewer" prefix=". "/>
            <text macro="secondary-contributors-nehir" prefix=", "/>
            <group delimiter=":" prefix=", ">
              <text variable="volume"/>
              <text variable="page"/>
            </group>
          </if>
        </choose>
      </else-if>
      <else-if type="article-newspaper">
        <text variable="title" quotes="true"/>
      </else-if>
      <else-if variable="PMID" match="any">
        <choose>
          <if type="entry-dictionary">
            <text variable="title" quotes="false" font-style="italic"/>
          </if>
          <else>
            <text variable="title"/>
          </else>
        </choose>
      </else-if>
      <else-if variable="author editor collection-editor translator reviewed-author compiler director curator" match="none">
        <choose>
          <if type="entry-dictionary" variable="number-of-volumes edition event-place" match="none">
            <text variable="title" text-case="title" font-style="italic"/>
          </if>
          <else-if type="entry-dictionary" variable="number-of-volumes edition event-place" match="all">
            <text variable="title" text-case="title" font-style="italic" suffix=". "/>
          </else-if>
          <else>
            <text variable="title" text-case="title" font-style="italic"/>
          </else>
        </choose>
      </else-if>
      <else-if type="chapter" variable="section" match="all"/>
      <else-if variable="title" match="none">
        <choose>
          <if type="personal_communication" match="none">
            <text variable="genre" text-case="capitalize-first"/>
          </if>
        </choose>
      </else-if>
      <else-if type="dataset">
        <text variable="title" quotes="true" prefix="Aranan: "/>
      </else-if>
      <else-if type="graphic" match="any"/>
      <else-if type="song" match="any"/>
      <else-if type="map" match="any"/>
      <else-if type="software" match="any"/>
      <else-if type="legislation" match="any"/>
      <else-if type="motion_picture" match="any"/>
      <else-if type="manuscript" match="any">
        <text variable="title" text-case="title" font-style="italic"/>
      </else-if>
      <else-if type="standard">
        <text variable="title" text-case="title" font-style="italic" suffix=". "/>
      </else-if>
      <else-if type="bill" match="any">
        <text variable="title"/>
      </else-if>
      <else-if type="chapter" variable="interviewer genre" match="all">
        <group delimiter=". ">
          <text variable="title" quotes="true"/>
          <text macro="description"/>
        </group>
      </else-if>
      <else-if type="chapter" variable="interviewer" match="all">
        <group delimiter=" ">
          <text variable="title" quotes="true"/>
          <text macro="description"/>
        </group>
      </else-if>
      <else-if type="chapter" variable="keyword" match="all">
        <group delimiter=" ">
          <text variable="title" quotes="false" font-style="italic" suffix="."/>
        </group>
      </else-if>
      <else-if type="paper-conference">
        <text variable="title" quotes="true"/>
      </else-if>
      <else>
        <choose>
          <if type="thesis" match="any"/>
          <else-if type="patent" match="any"/>
          <else-if type="webpage">
            <choose>
              <if variable="note" match="any">
                <text variable="title" font-style="italic"/>
              </if>
              <else-if variable="contributor translator" match="any">
                <text variable="title" quotes="true" suffix=". "/>
              </else-if>
              <else>
                <text variable="title" quotes="true"/>
              </else>
            </choose>
          </else-if>
          <else-if type="article-journal">
            <text variable="title" quotes="true" suffix="."/>
          </else-if>
          <else-if type="entry-dictionary" match="none">
            <text variable="title" quotes="true" suffix="."/>
          </else-if>
          <else>
            <text variable="title" text-case="title" font-style="italic"/>
          </else>
        </choose>
      </else>
    </choose>
  </macro>
  <macro name="description">
    <group delimiter=", ">
      <choose>
        <if variable="title" match="none"/>
        <else-if type="thesis speech" match="any"/>
        <else-if type="report" match="any"/>
        <else-if type="motion_picture" match="any"/>
        <else-if type="personal_communication" match="any"/>
        <else-if type="document" match="any"/>
        <else-if type="graphic" match="any"/>
        <else-if type="map" match="any"/>
        <else-if type="software" match="any"/>
        <else-if variable="annote genre keyword medium note source status" match="all"/>
        <else-if type="webpage" match="any"/>
        <else-if type="manuscript" match="any"/>
        <else-if type="chapter" variable="interviewer genre" match="all">
          <text macro="interviewer" prefix="" suffix=""/>
        </else-if>
        <else-if type="chapter" variable="interviewer" match="all">
          <text macro="interviewer" prefix="(" suffix=")"/>
        </else-if>
        <else>
          <choose>
            <if variable="author editor translator collection-editor" match="any">
              <text variable="genre" text-case="capitalize-first" prefix=". " suffix=". "/>
            </if>
            <else>
              <text variable="genre" text-case="capitalize-first" suffix=". "/>
            </else>
          </choose>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="container-title">
    <choose>
      <if type="article-journal">
        <choose>
          <if variable="collection-title" match="any">
            <text variable="container-title" text-case="title" font-style="italic" suffix=", "/>
          </if>
          <else-if variable="volume issue issued" match="any">
            <text variable="container-title" text-case="title" font-style="italic" suffix=" "/>
          </else-if>
          <else>
            <text variable="container-title" text-case="title" font-style="italic"/>
          </else>
        </choose>
      </if>
      <else>
        <group delimiter=" ">
          <choose>
            <if type="entry-encyclopedia paper-conference" match="any">
              <choose>
                <if variable="keyword">
                  <text variable="keyword" font-style="italic"/>
                </if>
                <else>
                  <text variable="container-title" font-style="italic"/>
                </else>
              </choose>
            </if>
            <else-if type="chapter" variable="section" match="all"/>
            <else-if type="chapter" variable="keyword annote" match="all">
              <text variable="container-title" font-style="italic"/>
              <text term="in"/>
            </else-if>
            <else-if type="chapter" variable="keyword" match="all">
              <text variable="container-title" font-style="italic"/>
              <text term="in"/>
            </else-if>
            <else-if type="webpage" match="any">
              <choose>
                <if variable="note" match="any">
                  <text variable="container-title" text-case="title" font-style="italic"/>
                </if>
                <else>
                  <text variable="container-title" text-case="title" quotes="true" suffix=". "/>
                </else>
              </choose>
            </else-if>
            <else-if type="post-weblog">
              <text variable="container-title" text-case="title" font-style="italic" suffix=" (blog)"/>
            </else-if>
            <else-if type="legislation" match="any"/>
            <else-if type="broadcast" match="any"/>
            <else-if type="bill" match="any"/>
            <else-if type="entry-dictionary" match="any"/>
            <else>
              <text variable="container-title" text-case="title" font-style="italic"/>
            </else>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="collection-title">
    <choose>
      <if type="book" match="any"/>
      <else-if type="graphic map software motion_picture report entry-encyclopedia" match="none">
        <choose>
          <if match="none" type="article-journal">
            <choose>
              <if match="none" is-numeric="collection-number">
                <choose>
                  <if type="paper-conference" match="none">
                    <group delimiter=" " suffix=". ">
                      <text variable="collection-title" text-case="title"/>
                      <text variable="collection-number"/>
                    </group>
                  </if>
                </choose>
              </if>
              <else>
                <group delimiter=" ">
                  <text variable="collection-title" text-case="title"/>
                  <text variable="collection-number"/>
                </group>
              </else>
            </choose>
          </if>
        </choose>
      </else-if>
    </choose>
  </macro>
  <macro name="collection-title-journal">
    <choose>
      <if type="article-journal">
        <group delimiter=" ">
          <text variable="collection-title"/>
          <text variable="collection-number"/>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if type="book paper-conference report" match="any">
        <choose>
          <if is-numeric="edition">
            <group delimiter=" ">
              <number variable="edition" suffix="."/>
              <text term="edition"/>
            </group>
          </if>
          <else>
            <choose>
              <if variable="number-of-volumes">
                <text variable="edition"/>
              </if>
              <else>
                <text variable="edition" text-case="capitalize-first" suffix="."/>
              </else>
            </choose>
          </else>
        </choose>
      </if>
      <else-if type="entry-dictionary" match="any">
        <choose>
          <if is-numeric="edition">
            <group delimiter=" " prefix=", ">
              <number variable="edition" suffix="."/>
              <text term="edition"/>
            </group>
          </if>
          <else>
            <choose>
              <if variable="number-of-volumes">
                <text variable="edition"/>
              </if>
              <else>
                <text variable="edition" text-case="capitalize-first" suffix="."/>
              </else>
            </choose>
          </else>
        </choose>
      </else-if>
      <else-if type="entry-encyclopedia">
        <choose>
          <if variable="edition" match="any">
            <group delimiter=". ">
              <text variable="edition"/>
              <choose>
                <if is-numeric="edition" match="any">
                  <text value="edisyon"/>
                </if>
              </choose>
            </group>
          </if>
        </choose>
      </else-if>
      <else-if type="chapter">
        <choose>
          <if variable="division" match="none">
            <choose>
              <if is-numeric="edition">
                <group delimiter=" ">
                  <number variable="edition" suffix="."/>
                  <text term="edition"/>
                </group>
              </if>
              <else>
                <choose>
                  <if variable="number-of-volumes">
                    <text variable="edition" suffix=". "/>
                  </if>
                  <else>
                    <text variable="edition" text-case="capitalize-first" suffix="."/>
                  </else>
                </choose>
              </else>
            </choose>
          </if>
          <else>
            <choose>
              <if is-numeric="edition">
                <group delimiter=" ">
                  <number variable="edition" suffix="."/>
                  <text term="edition"/>
                </group>
              </if>
              <else>
                <choose>
                  <if variable="number-of-volumes">
                    <text variable="edition" suffix=". "/>
                  </if>
                  <else>
                    <text variable="edition" text-case="capitalize-first" suffix="."/>
                  </else>
                </choose>
              </else>
            </choose>
          </else>
        </choose>
      </else-if>
    </choose>
  </macro>
  <macro name="legal-cites">
    <choose>
      <if type="bill" match="any">
        <group delimiter=", ">
          <choose>
            <if variable="number">
              <text variable="number" prefix="Pub. L. No. "/>
              <group delimiter=" ">
                <text term="section" form="symbol"/>
                <text variable="section"/>
              </group>
              <group delimiter=" ">
                <text variable="volume"/>
                <text variable="container-title"/>
                <text variable="page-first"/>
              </group>
            </if>
            <else>
              <group delimiter=" ">
                <text variable="volume"/>
                <text variable="container-title"/>
                <text term="section" form="symbol"/>
                <text variable="section"/>
              </group>
            </else>
          </choose>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="locators">
    <choose>
      <if type="article-journal">
        <text variable="container-title" font-style="italic"/>
        <text variable="collection-title" prefix=", " suffix=", "/>
        <text variable="volume" prefix=" " suffix=","/>
        <text variable="issue" prefix=" "/>
      </if>
      <else-if type="bill" match="any">
        <text macro="legal-cites"/>
      </else-if>
      <else-if type="book" variable="note annote PMID" match="all">
        <group delimiter=", " suffix=". ">
          <group delimiter=" ">
            <number variable="number-of-volumes" form="numeric"/>
            <text term="volume"/>
          </group>
          <text macro="edition"/>
        </group>
      </else-if>
      <else-if type="book report" match="any">
        <group delimiter=", ">
          <choose>
            <if type="book"/>
            <else>
              <group delimiter=" ">
                <number variable="volume" form="numeric" suffix="."/>
                <text term="volume"/>
              </group>
            </else>
          </choose>
          <group delimiter=", " suffix=". ">
            <group delimiter=" ">
              <number variable="number-of-volumes" form="numeric"/>
              <text term="volume"/>
            </group>
            <text macro="edition"/>
          </group>
        </group>
      </else-if>
      <else-if variable="division jurisdiction medium genre status chapter-number part-number ISBN URL DOI" type="webpage" match="all">
        <choose>
          <if type="entry-dictionary" match="any">
            <group delimiter=", ">
              <group delimiter=" ">
                <number variable="number-of-volumes" form="numeric"/>
                <text term="volume"/>
              </group>
              <text macro="edition"/>
            </group>
          </if>
        </choose>
      </else-if>
      <else-if type="entry-encyclopedia paper-conference" match="any"/>
    </choose>
