  <macro name="issue">
    <choose>
      <if type="paper-conference" match="any">
        <group suffix=".">
          <choose>
            <if variable="publisher-place publisher" match="any">
              <group delimiter=", ">
                <group delimiter=". ">
                  <text macro="originally-published"/>
                  <group delimiter=", ">
                    <text macro="reprint"/>
                    <text macro="publisher"/>
                  </group>
                </group>
              </group>
            </if>
          </choose>
        </group>
      </if>
      <else-if type="legislation" match="any"/>
