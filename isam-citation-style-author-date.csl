  <macro name="locators-chapter">
    <choose>
      <if type="paper-conference entry-encyclopedia" match="any">
        <choose>
          <if variable="author container-author" match="all"/>
          <else>
            <choose>
              <if variable="page">
                <group delimiter=":">
                  <text variable="volume"/>
                  <text variable="page"/>
                </group>
              </if>
              <else>
                <group delimiter=". ">
                  <text term="volume" form="short"/>
                  <number variable="volume" form="numeric"/>
                </group>
              </else>
            </choose>
            <group delimiter=" " prefix=". ">
              <number variable="number-of-volumes" form="numeric"/>
              <text term="volume"/>
            </group>
          </else>
        </choose>
      </if>
      <else-if type="entry-dictionary" match="any">
        <choose>
          <if variable="author container-author" match="all"/>
          <else>
            <choose>
              <if variable="page">
                <group delimiter=":" suffix=". ">
                  <text variable="volume"/>
                  <text variable="page"/>
                </group>
              </if>
              <else>
                <group delimiter=". ">
                  <text term="volume" form="short"/>
                  <number variable="volume" form="numeric" suffix="."/>
                </group>
              </else>
            </choose>
            <group delimiter=" ">
              <number variable="number-of-volumes" form="numeric"/>
              <text term="volume"/>
            </group>
          </else>
        </choose>
      </else-if>
    </choose>
    <choose>
      <if type="chapter" variable="division" match="all">
        <group delimiter=". " suffix=". ">
          <group delimiter=":">
            <choose>
              <if variable="page">
                <text variable="volume" suffix=":"/>
                <text variable="page"/>
              </if>
              <else>
                <group delimiter=". ">
                  <text term="volume" form="short"/>
                  <number variable="volume" form="numeric" suffix="."/>
                </group>
              </else>
            </choose>
          </group>
          <group delimiter=", ">
            <group>
              <number variable="number-of-volumes" form="numeric" suffix=" "/>
              <text term="volume"/>
            </group>
            <text macro="edition"/>
          </group>
        </group>
      </if>
      <else-if type="chapter" variable="interviewer issue volume genre" match="all">
        <group>
          <text variable="volume" suffix=", "/>
          <text variable="issue"/>
          <choose>
            <if is-uncertain-date="issued">
              <date variable="issued" form="text" prefix="[" suffix="?]"/>
            </if>
            <else>
              <date variable="issued" form="text" prefix=" (" suffix=")"/>
            </else>
          </choose>
          <text variable="page" prefix=": "/>
        </group>
      </else-if>
      <else-if type="chapter" match="any">
        <choose>
          <if variable="compiler" match="any">
            <group delimiter=", ">
              <text macro="compiler"/>
              <group>
                <choose>
                  <if variable="page">
                    <text variable="volume" suffix=":"/>
                    <text variable="page"/>
                  </if>
                  <else>
                    <group delimiter=". ">
                      <text term="volume" form="short"/>
                      <number variable="volume" form="numeric" suffix="."/>
                    </group>
                  </else>
                </choose>
              </group>
              <text macro="edition"/>
            </group>
          </if>
          <else>
            <choose>
              <if variable="page">
                <text variable="volume" suffix=":"/>
                <text variable="page"/>
              </if>
              <else>
                <group delimiter=". ">
                  <text term="volume" form="short"/>
                  <number variable="volume" form="numeric" suffix="."/>
                </group>
              </else>
            </choose>
            <choose>
              <if variable="number-of-volumes">
                <group delimiter=", " prefix=". ">
                  <group>
                    <number variable="number-of-volumes" form="numeric" suffix=" "/>
                    <text term="volume"/>
                  </group>
                </group>
              </if>
            </choose>
            <text macro="edition" prefix=". "/>
            <text macro="compiler"/>
