            <text macro="isbn" suffix=". "/>
            <text macro="url" suffix=". "/>
          </group>
        </else>
      </choose>
      <group display="right-inline">
        <text macro="archive"/>
        <text macro="archive_location"/>
      </group>
      <group display="right-inline">
        <text macro="abstract"/>
      </group>
      <group display="right-inline">
