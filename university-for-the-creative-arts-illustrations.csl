<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" and="text" et-al-min="3" et-al-use-first="1" demote-non-dropping-particle="sort-only" default-locale="en-US">
  <info>
    <title>University for the Creative Arts - Illustrations</title>
    <title-short>uca-illustrations</title-short>
    <id>http://www.zotero.org/styles/university-for-the-creative-arts-illustrations</id>
    <link href="http://www.zotero.org/styles/university-for-the-creative-arts-illustrations" rel="self"/>
    <link href="http://www.uca.ac.uk/library/academic-support/harvard-referencing/" rel="documentation"/>
    <author>
      <name>Ian Badger</name>
    </author>
    <category citation-format="numeric"/>
    <category field="humanities"/>
    <updated>2017-03-21T09:08:29+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <citation>
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout prefix="(" suffix=")">
      <group delimiter=" ">
        <text term="figure" form="symbol" text-case="capitalize-first"/>
        <text variable="citation-number" suffix="."/>
        <text variable="title" font-style="italic"/>
        <date date-parts="year" form="text" variable="issued"/>
      </group>
    </layout>
  </citation>
  <bibliography>
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout>
      <group delimiter=" ">
        <text term="figure"/>
        <text variable="citation-number" suffix="."/>
        <group delimiter=" ">
          <names variable="author" suffix=".">
            <name and="text" delimiter-precedes-last="never" et-al-min="3" et-al-use-first="1" initialize-with="." name-as-sort-order="all"/>
            <label form="short"/>
            <et-al font-style="italic"/>
            <substitute>
              <text variable="title" font-style="italic" suffix="."/>
            </substitute>
          </names>
          <date variable="issued">
            <date-part name="year" prefix="(" suffix=")"/>
          </date>
          <text variable="title" font-style="italic" suffix="."/>
          <choose>
            <if match="any" variable="title-short">
              <text variable="title-short" font-style="italic" suffix="."/>
            </if>
          </choose>
          <choose>
            <if type="graphic" match="any">
              <text variable="medium" prefix="[" suffix="]"/>
              <text variable="note" prefix="In: "/>
              <choose>
                <if match="any" variable="URL">
                  <text variable="URL" prefix="At: "/>
                  <text term="accessed" text-case="capitalize-first" prefix="(" suffix=" on"/>
                  <group delimiter=" ">
                    <date delimiter="" variable="accessed" suffix=")">
                      <date-part name="day" suffix=" "/>
                      <date-part name="month" suffix=" "/>
                      <date-part name="year"/>
                    </date>
                  </group>
                </if>
              </choose>
            </if>
          </choose>
          <choose>
            <if type="map" match="any">
              <text variable="genre" prefix="[" suffix="]"/>
              <text variable="scale" suffix="."/>
              <text variable="publisher-place" suffix=": "/>
              <text variable="publisher" suffix="."/>
              <text variable="collection-title" prefix="(" suffix=")."/>
              <choose>
                <if match="all" variable="URL">
                  <text variable="URL" prefix="At: "/>
                  <text term="accessed" text-case="capitalize-first" prefix="(" suffix=" on"/>
                  <group>
                    <date delimiter=" " variable="accessed" suffix=")">
                      <date-part name="day"/>
                      <date-part name="month"/>
                      <date-part name="year"/>
                    </date>
                  </group>
                </if>
              </choose>
            </if>
          </choose>
        </group>
      </group>
    </layout>
  </bibliography>
</style>
