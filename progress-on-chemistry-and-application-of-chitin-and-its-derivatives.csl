<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="never" default-locale="en-US">
  <info>
    <title>Progress on Chemistry and Application of Chitin and its Derivatives</title>
    <title-short>PCACD</title-short>
    <id>http://www.zotero.org/styles/progress-on-chemistry-and-application-of-chitin-and-its-derivatives</id>
    <link href="http://www.zotero.org/styles/progress-on-chemistry-and-application-of-chitin-and-its-derivatives" rel="self"/>
    <link href="http://www.zotero.org/styles/proceedings-of-the-royal-society-b" rel="template"/>
    <link href="https://ptchit.lodz.pl/en/2022/03/11/informacje-dla-autorow/" rel="documentation"/>
    <author>
      <name>Patrick O'Brien</name>
      <email>citationstyler@gmail.com</email>
      <uri>https://citationstyler.com/</uri>
    </author>
    <category citation-format="numeric"/>
    <category field="chemistry"/>
    <issn>1896-5644</issn>
    <updated>2025-12-11T13:37:37+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en">
    <terms>
      <term name="available at">available online</term>
      <term name="edition" form="short">ed.</term>
    </terms>
  </locale>
  <macro name="author">
    <names variable="author">
      <name sort-separator=" " initialize-with="" name-as-sort-order="all" delimiter=", " delimiter-precedes-last="never" et-al-min="10" et-al-use-first="1"/>
      <et-al font-style="italic"/>
      <label form="short" prefix=" (" suffix=")"/>
      <substitute>
        <names variable="editor translator"/>
        <text variable="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names variable="editor">
      <name delimiter-precedes-last="never" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label form="short" prefix=" (" suffix=")"/>
    </names>
  </macro>
  <macro name="publisher">
    <group delimiter=", ">
      <text variable="publisher"/>
      <text variable="publisher-place"/>
    </group>
  </macro>
  <macro name="title">
    <choose>
      <if type="report patent standard" match="any">
        <names variable="author">
          <name sort-separator=" " initialize-with="" name-as-sort-order="all" delimiter=", " delimiter-precedes-last="never" et-al-min="10" et-al-use-first="1"/>
          <et-al font-style="italic"/>
          <label form="short" prefix=" (" suffix=")"/>
          <substitute>
            <names variable="editor translator"/>
          </substitute>
        </names>
      </if>
      <else>
        <group delimiter=", ">
          <text variable="title"/>
          <choose>
            <if type="book" match="any">
              <text macro="edition"/>
              <text macro="volume"/>
            </if>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number variable="edition" form="ordinal"/>
          <text term="edition" form="short"/>
        </group>
      </if>
      <else>
        <text variable="edition"/>
      </else>
    </choose>
  </macro>
  <macro name="issued">
    <choose>
      <if variable="issued">
        <choose>
          <if type="paper-conference" match="any">
            <date form="numeric" variable="issued"/>
          </if>
          <else>
            <date variable="issued">
              <date-part name="year"/>
            </date>
          </else>
        </choose>
      </if>
      <else>
        <text term="in press" text-case="capitalize-first"/>
      </else>
    </choose>
  </macro>
  <macro name="access">
    <choose>
      <if variable="DOI">
        <group delimiter=" ">
          <text value="DOI:" font-weight="bold"/>
          <text variable="DOI"/>
        </group>
      </if>
      <else-if type="webpage post post-weblog" match="any">
        <group delimiter=" " suffix=".">
          <group delimiter=": ">
            <text term="available at" text-case="capitalize-first"/>
            <text variable="URL"/>
          </group>
          <group delimiter=": " prefix="(" suffix=")">
            <text term="accessed"/>
            <date variable="accessed">
              <date-part name="day" form="numeric-leading-zeros" suffix="."/>
              <date-part name="month" form="numeric-leading-zeros" suffix="."/>
              <date-part name="year"/>
            </date>
          </group>
        </group>
      </else-if>
      <else-if type="report" match="any">
        <group delimiter=": ">
          <group delimiter=" ">
            <text term="report" text-case="capitalize-first"/>
            <text term="available at" suffix=":"/>
            <text variable="URL"/>
          </group>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="volume">
    <group delimiter=" ">
      <label text-case="capitalize-first" variable="volume" form="short"/>
      <text variable="volume"/>
    </group>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter=", " prefix="[" suffix="]">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography second-field-align="flush">
    <layout>
      <text variable="citation-number" prefix="[" suffix="]"/>
      <group delimiter=". " suffix=".">
        <group delimiter=" ">
          <group delimiter="; ">
            <choose>
              <if type="report" match="any">
                <text variable="title"/>
              </if>
              <else-if type="patent" match="any">
                <group delimiter=". ">
                  <text variable="title"/>
                  <choose>
                    <if variable="number" match="any">
                      <group delimiter=" ">
                        <text term="patent" text-case="capitalize-first" font-weight="bold"/>
                        <text variable="number"/>
                      </group>
                    </if>
                    <else>
                      <text term="patent" text-case="capitalize-first" font-weight="bold"/>
                    </else>
                  </choose>
                </group>
              </else-if>
              <else-if type="standard" match="any">
                <group delimiter=". ">
                  <text variable="title"/>
                  <group delimiter=" ">
                    <text variable="genre" text-case="capitalize-first"/>
                    <text variable="number"/>
                  </group>
                </group>
              </else-if>
              <else>
                <text macro="author"/>
              </else>
            </choose>
            <text macro="issued" prefix="(" suffix=")"/>
          </group>
          <text macro="title"/>
        </group>
        <choose>
          <if type="article-journal">
            <group delimiter=" ">
              <text variable="container-title" strip-periods="true"/>
              <group delimiter=", ">
                <group>
                  <text variable="volume"/>
                  <text variable="issue" prefix="(" suffix=")"/>
                </group>
                <text variable="page"/>
              </group>
            </group>
          </if>
          <else-if type="chapter" match="any">
            <group delimiter=", ">
              <group delimiter=": ">
                <text term="in" text-case="capitalize-first"/>
                <group delimiter=", ">
                  <text macro="editor"/>
                  <text variable="container-title"/>
                </group>
              </group>
              <text macro="volume"/>
              <text macro="edition"/>
              <text macro="publisher"/>
              <group delimiter=" ">
                <label variable="page" form="short"/>
                <text variable="page"/>
              </group>
            </group>
          </else-if>
          <else-if type="thesis">
            <group delimiter=", ">
              <text variable="genre"/>
              <text macro="publisher"/>
            </group>
          </else-if>
          <else-if type="webpage post post-weblog" match="any">
            <text variable="container-title"/>
          </else-if>
          <else-if type="patent" match="any">
            <group delimiter=", ">
              <text variable="publisher-place"/>
              <text variable="authority"/>
            </group>
          </else-if>
          <else-if type="paper-conference" match="any">
            <group delimiter=", ">
              <group delimiter=" ">
                <text term="in" text-case="capitalize-first"/>
                <text variable="container-title"/>
              </group>
              <text variable="publisher-place"/>
            </group>
          </else-if>
          <else-if type="report" match="any">
            <text variable="publisher"/>
          </else-if>
          <else>
            <text macro="publisher"/>
          </else>
        </choose>
      </group>
      <text macro="access" prefix=" "/>
    </layout>
  </bibliography>
</style>
