<?xml version="1.0" encoding="utf-8"?>
<style class="in-text" version="1.0" xmlns="http://purl.org/net/xbiblio/csl" demote-non-dropping-particle="sort-only" page-range-format="minimal" default-locale="fr-FR">
  <info>
    <title>Vancouver DRSM (French)</title>
    <id>http://www.zotero.org/styles/vancouver-drsm</id>
    <link rel="self" href="http://www.zotero.org/styles/vancouver-drsm"/>
    <link href="http://www.zotero.org/styles/vancouver-fr-ca" rel="template"/>
    <author>
      <name>Pierre Morlanne-Fendan</name>
      <email>pierre.morlanne-fendan@ersm-sudest.cnamts.fr</email>
      <uri>http://www.ameli.fr</uri>
    </author>
    <summary>Adaptation pour Zotero de l'adaptation de la norme de Vancouver en vigeur dans les DRSM</summary>
    <updated>2016-08-17T13:36:22+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale>
    <date form="text">
      <date-part name="day" form="numeric-leading-zeros" suffix="/"/>
      <date-part name="month" form="numeric-leading-zeros" strip-periods="false" suffix="/"/>
      <date-part name="year"/>
    </date>
    <terms>
      <term name="retrieved">disponible sur :</term>
      <term name="in">in</term>
      <term name="cited">consulté le</term>
      <term name="internet">en ligne</term>
      <term name="editor">
        <single>rédacteur</single>
        <multiple>rédacteurs</multiple>
      </term>
      <term name="original-author">
        <single>inventeur</single>
        <multiple>inventeurs</multiple>
      </term>
      <term name="accessed">consulté le</term>
      <term name="page" form="short">p</term>
	  <term name="page-range-delimiter">-</term>
    </terms>
  </locale>
  <macro name="auteur">
    <names variable="author">
      <name delimiter-precedes-last="always" initialize-with="" name-as-sort-order="all" sort-separator=" "/>
      <label prefix=", "/>
      <substitute>
        <names variable="editor collection-editor"/>
      </substitute>
    </names>
    <choose>
      <if type="patent" match="all">
        <text term="original-author" prefix=", "/>
      </if>
    </choose>
  </macro>
  <macro name="auteur-contenant">
    <choose>
      <if type="paper-conference" match="any">
        <names variable="container-author">
          <name delimiter-precedes-last="always"/>
          <substitute>
            <names variable="editor collection-editor"/>
          </substitute>
        </names>
      </if>
      <else>
        <names variable="container-author">
          <name delimiter-precedes-last="always" name-as-sort-order="all" sort-separator=" "/>
          <label prefix=", "/>
          <substitute>
            <names variable="editor collection-editor"/>
          </substitute>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="redacteur">
    <names variable="editor" suffix=".">
      <name sort-separator=" " initialize-with="" name-as-sort-order="all" delimiter=", " delimiter-precedes-last="always"/>
      <label form="long" prefix=", "/>
    </names>
  </macro>
  <macro name="editeur">
    <group delimiter=" : " suffix=";">
      <text variable="publisher-place"/>
      <text variable="publisher" suffix=" "/>
    </group>
  </macro>
  <macro name="acces-url">
    <choose>
      <if variable="URL">
        <group delimiter=" " prefix="(" suffix=").">
          <group delimiter=" ">
            <text term="retrieved" text-case="capitalize-first"/>
          </group>
          <text variable="URL" prefix="&lt; " suffix=" &gt;"/>
          <text macro="date-acces"/>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="titre-contenant">
    <choose>
      <if type="article-journal article-magazine chapter paper-conference article-newspaper review review-book entry-dictionary entry-encyclopedia" match="any">
        <group delimiter=" ">
          <choose>
            <if type="article-journal review review-book" match="any">
              <text variable="container-title" form="short" strip-periods="true"/>
            </if>
            <else-if type="entry-dictionary entry-encyclopedia" match="any">
              <text variable="container-title"/>
            </else-if>
            <else>
              <text macro="auteur-contenant" suffix="."/>
              <text variable="container-title" strip-periods="true" suffix="."/>
            </else>
          </choose>
          <text macro="edition"/>
          <choose>
            <if variable="URL">
              <text term="internet" prefix="[" suffix="]" text-case="capitalize-first"/>
            </if>
          </choose>
        </group>
      </if>
      <else-if type="bill legislation" match="any">
        <group delimiter=", ">
          <group delimiter=". ">
            <text variable="container-title"/>
            <group delimiter=" ">
              <text term="section" form="short" text-case="capitalize-first"/>
              <text variable="section"/>
            </group>
          </group>
          <text variable="number"/>
        </group>
      </else-if>
      <else>
        <text variable="container-title" suffix="."/>
      </else>
    </choose>
  </macro>
  <macro name="titre">
    <text variable="title"/>
    <choose>
      <if type="thesis">
        <text variable="genre" prefix=". Thèse d'exercice : "/>
      </if>
    </choose>
    <choose>
      <if type="article-journal article-magazine chapter paper-conference article-newspaper review review-book entry-dictionary entry-encyclopedia" match="none">
        <choose>
          <if type="book" match="any">
            <choose>
              <if match="all" variable="volume">
                <text variable="volume" prefix=". Vol. " suffix="."/>
              </if>
            </choose>
          </if>
        </choose>
        <choose>
          <if variable="URL">
            <text term="internet" prefix=" [" suffix="]" text-case="capitalize-first"/>
          </if>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number variable="edition" form="ordinal"/>
          <text term="edition" form="short"/>
        </group>
      </if>
      <else>
        <text variable="edition" suffix="."/>
      </else>
    </choose>
  </macro>
  <macro name="date">
    <choose>
      <if type="article-journal article-magazine article-newspaper webpage review review-book" match="any">
        <choose>
          <if match="any" variable="issue volume">
            <group delimiter=" " suffix=";">
              <date date-parts="year" form="text" variable="issued"/>
            </group>
          </if>
          <else-if match="none" variable="volume issue">
            <group suffix=":">
              <date date-parts="year" form="text" variable="issued"/>
            </group>
          </else-if>
        </choose>
      </if>
      <else-if type="bill legislation" match="any">
        <group delimiter=", ">
          <date variable="issued" form="text"/>
        </group>
      </else-if>
      <else-if type="report">
        <date variable="issued" delimiter=" ">
          <date-part name="month" form="short" strip-periods="true"/>
          <date-part name="year"/>
        </date>
      </else-if>
      <else-if type="patent">
        <group suffix=".">
          <group delimiter=", ">
            <text variable="number"/>
            <date variable="issued" form="text"/>
          </group>
          <text macro="date-acces" prefix=" "/>
        </group>
      </else-if>
      <else-if type="book chapter" match="any">
        <choose>
          <if match="any" variable="page number-of-pages">
            <date date-parts="year" form="text" variable="issued" prefix=" " suffix=" ,"/>
          </if>
          <else>
            <date date-parts="year" form="text" variable="issued"/>
          </else>
        </choose>
      </else-if>
      <else-if type="paper-conference" match="any">
        <choose>
          <if match="any" variable="page number-of-pages">
            <date date-parts="year" form="text" variable="issued" suffix=" :"/>
          </if>
          <else>
            <date date-parts="year" form="text" variable="issued"/>
          </else>
        </choose>
      </else-if>
      <else>
        <group suffix=".">
          <date variable="issued">
            <date-part name="year"/>
          </date>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="pages">
    <choose>
      <if type="article-journal article-magazine article-newspaper review review-book" match="any">
        <text variable="page" form="short" prefix=":"/>
      </if>
      <else-if type="book" match="any">
        <text variable="number-of-pages" prefix=" "/>
        <choose>
          <if is-numeric="number-of-pages">
            <label variable="number-of-pages" form="short" prefix=" " plural="never"/>
          </if>
        </choose>
      </else-if>
      <else-if type="entry-dictionary entry-encyclopedia" match="any">
        <text variable="page" suffix=" p."/>
      </else-if>
      <else-if type="legislation" match="any">
        <choose>
          <if match="any" variable="page">
            <text variable="page" suffix="."/>
          </if>
          <else>
            <text value="" suffix="."/>
          </else>
        </choose>
      </else-if>
      <else>
        <group prefix=" " delimiter=" ">
          <label plural="never" variable="page" form="short"/>
          <text variable="page"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="localisation-revue">
    <choose>
      <if type="article-journal article-magazine review review-book" match="any">
        <text variable="volume"/>
        <text variable="issue" prefix="(" suffix=")"/>
      </if>
    </choose>
  </macro>
  <macro name="details-rapport">
    <choose>
      <if type="report">
        <text variable="number" prefix="Rapport no "/>
      </if>
    </choose>
  </macro>
  <macro name="date-acces">
    <group prefix="[" suffix="]">
      <text term="cited" text-case="capitalize-first" suffix=" "/>
      <date form="text" variable="accessed"/>
    </group>
  </macro>
  <citation>
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter="," prefix="(" suffix=")">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography et-al-min="6" et-al-use-first="6" second-field-align="flush">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout>
      <text variable="citation-number" suffix=". "/>
      <choose>
        <if type="article article-journal article-magazine article-newspaper" match="any">
          <group suffix=". ">
            <text macro="auteur"/>
          </group>
          <group suffix=". ">
            <text macro="titre"/>
          </group>
          <group suffix=" ">
            <text macro="titre-contenant"/>
          </group>
          <group>
            <text macro="date"/>
          </group>
          <group suffix=":">
            <text macro="localisation-revue"/>
          </group>
          <group suffix=".">
            <text macro="pages"/>
          </group>
        </if>
        <else-if type="book" match="any">
          <group suffix=". ">
            <text macro="auteur"/>
          </group>
          <group suffix=". ">
            <text macro="titre"/>
          </group>
          <group suffix=" ">
            <text macro="edition"/>
          </group>
          <group>
            <text macro="editeur"/>
          </group>
          <group prefix=" ">
            <text macro="date"/>
          </group>
          <group>
            <text macro="pages"/>
          </group>
        </else-if>
        <else-if type="chapter" match="any">
          <group suffix=". ">
            <text macro="auteur"/>
          </group>
          <group suffix=". ">
            <text macro="titre"/>
          </group>
          <group suffix=" ">
            <text term="in" text-case="capitalize-first" suffix=" : "/>
            <text macro="titre-contenant"/>
            <text variable="abstract"/>
          </group>
          <group>
            <text macro="editeur"/>
          </group>
          <group>
            <text macro="date"/>
          </group>
          <group suffix=".">
            <text macro="pages"/>
          </group>
        </else-if>
        <else-if type="thesis" match="any">
          <group suffix=". ">
            <text macro="auteur"/>
          </group>
          <group suffix=" : ">
            <text macro="titre"/>
          </group>
          <group delimiter=" : " suffix=".">
            <text variable="publisher-place"/>
            <date form="text" date-parts="year-month-day" variable="issued"/>
          </group>
        </else-if>
        <else-if type="webpage" match="any">
          <group suffix=". ">
            <text macro="auteur"/>
          </group>
          <group suffix=". ">
            <text macro="titre"/>
          </group>
        </else-if>
        <else-if type="entry-encyclopedia entry-dictionary" match="any">
          <group>
            <text macro="auteur" suffix=". "/>
          </group>
          <group suffix=". ">
            <text macro="titre"/>
          </group>
          <group suffix=",">
            <text macro="titre-contenant"/>
          </group>
          <group prefix=" ">
            <text variable="collection-title"/>
            <date variable="issued" prefix=" ">
              <date-part name="year"/>
            </date>
            <text variable="collection-number" prefix=";"/>
            <text macro="pages" prefix=":" suffix="."/>
          </group>
        </else-if>
        <else-if type="paper-conference" match="any">
          <group suffix=". ">
            <text macro="auteur"/>
          </group>
          <group suffix=". ">
            <text macro="titre"/>
          </group>
          <group suffix=". ">
            <choose>
              <if match="any" variable="container-author event">
                <text term="in" text-case="capitalize-first" suffix=" : "/>
                <text macro="auteur-contenant" suffix=". "/>
                <text variable="event"/>
              </if>
            </choose>
          </group>
          <group>
            <text macro="editeur"/>
          </group>
          <group>
            <text macro="date"/>
          </group>
          <text macro="pages" suffix="."/>
        </else-if>
        <else-if type="legal_case" match="any">
          <group>
            <text variable="title"/>
          </group>
        </else-if>
        <else-if type="legislation" match="any">
          <group>
            <text macro="auteur"/>
          </group>
          <group suffix=". ">
            <text macro="titre"/>
          </group>
          <group suffix="; ">
            <text macro="titre-contenant"/>
          </group>
          <date variable="issued" suffix=":">
            <date-part name="day" suffix=" "/>
            <date-part name="month"/>
          </date>
          <text macro="pages"/>
        </else-if>
      </choose>
      <text macro="acces-url" prefix=" "/>
    </layout>
  </bibliography>
</style>
