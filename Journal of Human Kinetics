<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="sort-only" page-range-format="minimal">
  <info>
    <title>Journal of Human Kinetics- Fernando Claver Rabaz - FERNANDO CLAVER RABAZ</title>
    <id>http://csl.mendeley.com/styles/22622781/JournalOfHumanKineticsFernandoClaver</id>
    <link href="http://www.zotero.org/styles/vancouver-author-date" rel="self"/>
    <link href="http://www.zotero.org/styles/vancouver" rel="template"/>
    <link href="http://www.zotero.org/styles/cell" rel="template"/>
    <link href="http://www.nlm.nih.gov/bsd/uniform_requirements.html" rel="documentation"/>
    <author>
      <name>Fernando Claver</name>
      <email>claverrabazfernando@gmail.com</email>
    </author>
    <contributor>
      <name>FERNANDO CLAVER RABAZ</name>
      <uri>http://www.mendeley.com/profiles/fernando-claver-rabaz1/</uri>
    </contributor>
    <category citation-format="author-date"/>
