<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="sort-only" default-locale="en-GB">
  <info>
    <title>Sun Yat-sen University Natural Science</title>
    <title-short>SYSU Nat</title-short>
    <id>http://csl.mendeley.com/styles/27363251/syaoming01</id>
    <author>
      <name>Syaoming</name>
      <email>syaomingl@gmail.com</email>
    </author>
    <category citation-format="numeric"/>
    <category field="science"/>
    <category field="generic-base"/>
    <eissn></eissn>
    <updated>2014-06-01T06:28:24+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en">
    <terms>
      <term name="editor" form="short">
        <single>ed.</single>
        <multiple>eds</multiple>
      </term>
    </terms>
  </locale>
  <macro name="title">
    <text variable="title"/>
    <choose>
      <if type="bill book graphic legal_case legislation motion_picture report song" match="any">
        <text value="［M］" font-style="normal" suffix="."/>
        <text macro="edition"/>
      </if>
      <else-if type="article article-journal review review-book" match="any">
        <text value="［J］" suffix="."/>
        <choose>
          <if match="any" variable="container-title-short">
            <text variable="container-title-short"/>
          </if>
          <else-if match="any" variable="container-title">
            <text variable="container-title"/>
          </else-if>
        </choose>
      </else-if>
    </choose>
  </macro>
  <macro name="author">
    <names variable="author" font-variant="normal" delimiter="，">
      <name font-variant="small-caps" delimiter="，" delimiter-precedes-et-al="always" delimiter-precedes-last="always" et-al-min="3" et-al-use-first="3" initialize-with=" " name-as-sort-order="all" sort-separator=" "/>
      <label form="short" prefix=", "/>
      <et-al font-style="normal" font-variant="normal"/>
    </names>
  </macro>
  <macro name="edition">
    <choose>
      <if match="any" variable="edition">
        <text variable="edition"/>
      </if>
    </choose>
    <choose>
      <if match="any" variable="publisher">
        <text variable="publisher-place" suffix="："/>
        <text variable="publisher"/>
      </if>
    </choose>
  </macro>
  <macro name="issuerance">
    <choose>
      <if match="any" variable="volume">
        <text variable="volume" prefix="，"/>
        <text variable="issue" prefix="(" suffix=")"/>
      </if>
    </choose>
    <choose>
      <if match="any" variable="page">
        <text variable="page" prefix="："/>
      </if>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout vertical-align="sup" delimiter="," prefix="[" suffix="]">
      <text variable="citation-number"/>
    </layout>
  </citation>
  <bibliography et-al-min="6" et-al-use-first="1" second-field-align="flush" entry-spacing="0">
    <layout>
      <text variable="citation-number" prefix="［" suffix="］"/>
      <group delimiter=" ">
        <text macro="author" text-case="capitalize-all" suffix="."/>
        <text macro="title"/>
        <date date-parts="year" form="text" variable="issued" prefix="，"/>
        <text macro="issuerance"/>
        <text value="."/>
      </group>
    </layout>
  </bibliography>
</style>
