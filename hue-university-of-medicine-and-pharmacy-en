<?xml version="1.0" encoding="utf-8"?>
<style class="in-text" version="1.0" et-al-use-first="3" demote-non-dropping-particle="sort-only" default-locale="en-US" xmlns="http://purl.org/net/xbiblio/csl">
  <info>
    <title>Hue University of Medicine and Pharmacy-En</title>
    <title-short>HUMP</title-short>
    <id>http://www.zotero.org/styles/hue-university-of-medicine-and-pharmacy-en</id>
    <link href="http://www.zotero.org/styles/hue-university-of-medicine-and-pharmacy" rel="self"/>
    <link href="http://www.zotero.org/styles/acm-sigchi-proceedings" rel="template"/>
    <link href="https://github.com/quochung2310/zotero-things/blob/main/HUMP-citation-style.md" rel="documentation"/>
    <link href="http://dtdh-ydh.edu.vn/2022/07/tb-ve-viec-huong-dan-viet-va-nop-luan-van-khoa-luan-tot-nghiep/" rel="documentation"/>
    <author>
      <name>Pham Thanh Tung</name>
      <email>tung.pham@fsh.org.vn</email>
      <uri>http://fsh.org.vn</uri>
    </author>
    <author>
      <name>Hung Quoc Vo</name>
      <email>vqhung@hueuni.edu.vn</email>
      <uri>https://orcid.org/0000-0003-4368-9672</uri>
    </author>
    <contributor>
      <name>Bùi Phương Linh</name>
      <email>linh.bui@fsh.org.vn</email>
      <uri>http://fsh.org.vn</uri>
    </contributor>
    <contributor>
      <name>Nguyễn Phước Bích Ngọc</name>
      <email>npbngoc@huemed-univ.edu.vn</email>
    </contributor>
    <category citation-format="numeric"/>
    <summary>Style Trường Đại học Y-Dược, Đại học Huế</summary>
    <updated>2023-11-20T02:58:07+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <group suffix=" ">
      <names variable="author">
        <name and="text" delimiter-precedes-et-al="always" initialize-with="." name-as-sort-order="all" sort-separator=" "/>
        <label form="short" strip-periods="true" prefix=", "/>
        <substitute>
          <names variable="editor"/>
        </substitute>
      </names>
    </group>
    <choose>
      <if type="book thesis legislation report legal_case" match="any">
        <text macro="year" prefix="(" suffix="), "/>
      </if>
      <else>
        <text macro="year" prefix="(" suffix="), "/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if type="book thesis legislation report" match="any">
        <text variable="title" quotes="false" font-style="italic" suffix=", "/>
      </if>
      <else>
        <text variable="title" quotes="false" prefix="&quot;" suffix="&quot;, "/>
      </else>
    </choose>
  </macro>
  <macro name="year">
    <date date-parts="year" form="numeric" variable="issued">
      <date-part name="year"/>
    </date>
  </macro>
  <macro name="conference">
    <group delimiter=", ">
      <text variable="container-title" font-style="italic"/>
      <text variable="publisher-place"/>
      <date form="numeric" variable="event-date"/>
      <text variable="publisher"/>
      <text variable="page"/>
    </group>
  </macro>
  <macro name="book-publisher">
    <group delimiter=", ">
      <text variable="genre"/>
      <text variable="publisher"/>
      <text variable="publisher-place"/>
    </group>
  </macro>
  <citation near-note-distance="2">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout font-variant="normal" delimiter="], [" prefix=" [" suffix="]">
      <group delimiter=", ">
        <text variable="citation-number" form="short" strip-periods="false" quotes="false" font-variant="normal"/>
        <text variable="locator"/>
      </group>
    </layout>
  </citation>
  <bibliography and="text" delimiter-precedes-et-al="always" initialize-with="." sort-separator="." second-field-align="flush" entry-spacing="0" hanging-indent="true">
    <sort>
      <key variable="language" sort="descending"/>
      <key variable="author"/>
    </sort>
    <layout suffix=".">
      <text variable="citation-number" suffix="."/>
      <text macro="author" strip-periods="false" suffix=" "/>
      <text macro="title"/>
      <choose>
        <if type="bill book graphic legal_case legislation motion_picture report song thesis" match="any">
          <text macro="book-publisher" suffix="."/>
        </if>
        <else-if type="paper-conference">
          <group suffix="." delimiter=", ">
            <text macro="conference"/>
            <text variable="page"/>
          </group>
        </else-if>
        <else-if type="chapter" match="any">
          <text variable="container-title" font-style="italic" suffix=". "/>
          <group suffix="." delimiter=", ">
            <text variable="edition"/>
            <text macro="book-publisher"/>
            <text variable="page"/>
          </group>
        </else-if>
        <else-if type="article-journal">
          <group delimiter=", " suffix=".">
            <text variable="container-title" form="short" strip-periods="true" font-style="italic"/>
            <group>
              <text variable="volume" font-weight="normal"/>
              <text variable="issue" font-weight="normal" prefix="(" suffix=")"/>
            </group>
            <text variable="page" form="short" strip-periods="false" font-weight="normal"/>
          </group>
        </else-if>
        <else>
          <group delimiter=", ">
            <text variable="container-title"/>
            <text variable="volume"/>
            <text variable="page"/>
            <text variable="URL" prefix="&lt;" suffix="&gt;"/>
            <date form="numeric" variable="accessed" prefix="accessed: "/>
          </group>
        </else>
      </choose>
    </layout>
  </bibliography>
  <locale xml:lang="en-US"/>
</style>
