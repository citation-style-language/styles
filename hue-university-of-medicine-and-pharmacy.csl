<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="sort-only">
  <info>
    <title>Hue University of Medicine and Pharmacy</title>
    <title-short>HUMP</title-short>
    <id>http://www.zotero.org/styles/hue-university-of-medicine-and-pharmacy</id>
    <link href="http://www.zotero.org/styles/hue-university-of-medicine-and-pharmacy" rel="self"/>
    <link href="http://www.zotero.org/styles/acm-sigchi-proceedings" rel="template"/>
    <link href="https://github.com/citation-style-language/styles/pull/6781" rel="documentation"/>
    <author>
      <name>Pham Thanh Tung</name>
      <email>tung.pham@fsh.org.vn</email>
      <uri>http://fsh.org.vn</uri>
    </author>
    <contributor>
      <name>Hung Quoc Vo</name>
      <email>vqhung@hueuni.edu.vn</email>
      <uri>https://orcid.org/0000-0003-4368-9672</uri>
    </contributor>
    <contributor>
      <name>Bùi Phương Linh</name>
      <email>linh.bui@fsh.org.vn</email>
      <uri>http://fsh.org.vn</uri>
    </contributor>
    <contributor>
      <name>Nguyễn Phước Bích Ngọc</name>
      <email>npbngoc@huemed-univ.edu.vn</email>
    </contributor>
    <contributor>
      <name>Patrick O'Brien</name>
    </contributor>
    <category citation-format="numeric"/>
    <summary>Style Trường Đại học Y-Dược, Đại học Huế</summary>
    <updated>2025-09-19T11:52:54+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en">
    <terms>
      <term name="open-quote">“</term>
      <term name="close-quote">”</term>
      <term name="open-inner-quote">‘</term>
      <term name="close-inner-quote">’</term>
    </terms>
  </locale>
  <macro name="author">
    <names variable="author">
      <name and="text" delimiter-precedes-et-al="always" initialize-with="." name-as-sort-order="all" sort-separator=" "/>
      <label form="short" strip-periods="true" prefix=", "/>
      <substitute>
        <names variable="editor translator"/>
        <text variable="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="title">
    <choose>
      <if type="book thesis legislation report" match="any">
        <text variable="title" quotes="false" font-style="italic"/>
      </if>
      <else>
        <text variable="title" quotes="true"/>
      </else>
    </choose>
  </macro>
  <macro name="year">
    <date date-parts="year" form="numeric" variable="issued">
      <date-part name="year"/>
    </date>
  </macro>
  <macro name="conference">
    <group delimiter=", ">
      <text variable="container-title" font-style="italic"/>
      <text variable="publisher-place"/>
      <date form="numeric" variable="event-date"/>
      <text variable="publisher"/>
      <text variable="page"/>
    </group>
  </macro>
  <macro name="book-publisher">
    <group delimiter=", ">
      <text variable="genre"/>
      <text variable="publisher"/>
      <text variable="publisher-place"/>
    </group>
  </macro>
  <macro name="pages">
    <group delimiter=" ">
      <label variable="page" form="short"/>
      <text variable="page"/>
    </group>
  </macro>
  <citation>
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter="], [" prefix=" [" suffix="]">
      <group delimiter=", ">
        <text variable="citation-number" form="short" strip-periods="false" quotes="false"/>
        <text variable="locator"/>
      </group>
    </layout>
  </citation>
  <bibliography and="text" delimiter-precedes-et-al="always" initialize-with="." sort-separator="." second-field-align="flush" entry-spacing="0" hanging-indent="true">
    <sort>
      <key variable="language" sort="descending"/>
      <key variable="author"/>
    </sort>
    <layout suffix=".">
      <text variable="citation-number" suffix="."/>
      <group delimiter=", ">
        <group delimiter=" ">
          <text macro="author" strip-periods="false" suffix=" "/>
          <text macro="year" prefix="(" suffix=")"/>
        </group>
        <text macro="title"/>
        <choose>
          <if type="bill book graphic legal_case legislation motion_picture report song thesis" match="any">
            <text macro="book-publisher" suffix="."/>
          </if>
          <else-if type="paper-conference">
            <group suffix="." delimiter=", ">
              <text macro="conference"/>
              <text macro="pages"/>
            </group>
          </else-if>
          <else-if type="chapter" match="any">
            <text variable="container-title" font-style="italic" suffix=". "/>
            <group suffix="." delimiter=", ">
              <text variable="edition"/>
              <text macro="book-publisher"/>
              <text macro="pages"/>
            </group>
          </else-if>
          <else-if type="article-journal">
            <group delimiter=". " suffix=".">
              <text variable="container-title" form="short" strip-periods="true" font-style="italic"/>
              <group delimiter=", ">
                <group>
                  <text variable="volume"/>
                  <text variable="issue" prefix="(" suffix=")"/>
                </group>
                <text macro="pages" strip-periods="false"/>
              </group>
            </group>
          </else-if>
          <else>
            <group delimiter=", ">
              <text variable="container-title"/>
              <text variable="volume"/>
              <text macro="pages"/>
              <text variable="URL" prefix="&lt;" suffix="&gt;"/>
              <group delimiter=": ">
                <text term="accessed"/>
                <date form="numeric" variable="accessed"/>
              </group>
            </group>
          </else>
        </choose>
      </group>
    </layout>
  </bibliography>
</style>
